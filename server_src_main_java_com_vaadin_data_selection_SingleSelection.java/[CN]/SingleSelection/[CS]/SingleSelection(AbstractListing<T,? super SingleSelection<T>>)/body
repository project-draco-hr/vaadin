{
  registerRpc(new SelectionServerRpc(){
    @Override public void select(    String key){
      if (!Objects.equals(selectedItem,getData(key))) {
        doSelect(getData(key),true);
      }
    }
    @Override public void deselect(    String key){
      if (Objects.equals(selectedItem,getData(key))) {
        doSelect(null,true);
      }
    }
  }
);
  extend(parent);
}
