{
  openTestURL();
  final int rowsToRemove=5;
  final int offset=5;
  removeRows(offset,rowsToRemove);
  final List<WebElement> rows=getBodyRows();
  int i=0;
  for (; i < offset; i++) {
    final String expectedStart="Row " + i;
    final String text=rows.get(i).getText();
    assertTrue("Expected " + expectedStart + "... but was "+ text,text.startsWith(expectedStart));
  }
  for (; i < 10; i++) {
    final String expectedStart="Row " + (i + rowsToRemove);
    final String text=rows.get(i).getText();
    assertTrue("(post remove) Expected " + expectedStart + "... but was "+ text,text.startsWith(expectedStart));
  }
}
