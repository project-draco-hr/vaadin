{
  Window.Notification n=new Window.Notification("POJO state",Window.Notification.TYPE_TRAY_NOTIFICATION);
  n.setPosition(Window.Notification.POSITION_CENTERED);
  n.setDescription("First name: " + person.getFirstName() + "<br/>Last name: "+ person.getLastName()+ "<br/>Country: "+ person.getCountryCode()+ "<br/>Birthdate: "+ person.getBirthdate()+ "<br/>Shoe size: "+ +person.getShoesize()+ "<br/>Password: "+ person.getPassword()+ "<br/>UUID: "+ person.getUuid());
  getWindow().showNotification(n);
}
