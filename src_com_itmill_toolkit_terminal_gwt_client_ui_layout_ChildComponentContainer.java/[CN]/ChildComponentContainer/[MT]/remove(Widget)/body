{
  if (child != caption && child != widget) {
    return false;
  }
  orphan(child);
  if (child == caption) {
    containerDIV.removeChild(caption.getElement());
    caption=null;
  }
 else {
    if (widget.isAttached()) {
      containerDIV.removeChild(widget.getElement());
    }
    widget=null;
  }
  return true;
}
