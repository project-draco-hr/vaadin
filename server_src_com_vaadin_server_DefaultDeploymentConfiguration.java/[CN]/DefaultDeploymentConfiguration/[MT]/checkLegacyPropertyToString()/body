{
  String param=getApplicationOrSystemProperty(Constants.SERVLET_PARAMETER_LEGACY_PROPERTY_TOSTRING,DEFAULT_LEGACY_PROPERTY_TO_STRING.name().toLowerCase());
  try {
    legacyPropertyToStringMode=LegacyProperyToStringMode.valueOf(param.toUpperCase());
  }
 catch (  IllegalArgumentException e) {
    getLogger().log(Level.WARNING,Constants.WARNING_UNKNOWN_LEGACY_PROPERTY_TOSTRING_VALUE,param);
    legacyPropertyToStringMode=DEFAULT_LEGACY_PROPERTY_TO_STRING;
  }
}
