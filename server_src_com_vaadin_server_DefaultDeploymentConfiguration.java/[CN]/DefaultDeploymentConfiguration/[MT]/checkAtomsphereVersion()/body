{
  try {
    String rawVersion=Version.getRawVersion();
    if (!Constants.REQUIRED_ATMOSPHERE_VERSION.equals(rawVersion)) {
      getLogger().log(Level.WARNING,Constants.INVALID_ATMOSPHERE_VERSION_WARNING,new Object[]{Constants.REQUIRED_ATMOSPHERE_VERSION,rawVersion});
    }
    return true;
  }
 catch (  NoClassDefFoundError e) {
    getLogger().log(Level.SEVERE,Constants.ATMOSPHERE_MISSING_ERROR,e);
    return false;
  }
}
