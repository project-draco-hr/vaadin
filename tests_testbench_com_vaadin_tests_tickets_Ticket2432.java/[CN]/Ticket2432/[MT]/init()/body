{
  LegacyWindow w=new LegacyWindow();
  setMainWindow(w);
  w.getContent().setSizeFull();
  ((SpacingHandler)w.getContent()).setSpacing(true);
  Layout layout=new GridLayout(3,3);
  populateLayout(layout);
  w.addComponent(layout);
  layout=new HorizontalLayout();
  populateLayout(layout);
  w.addComponent(layout);
}
