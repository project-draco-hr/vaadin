{
  Window w=new Window(getClass().getSimpleName());
  setMainWindow(w);
  Panel p=new Panel();
  p.setCaption("ExpandLayout");
  p.setWidth(500);
  p.setHeight(500);
  p.getLayout().setSizeFull();
  layout=new ExpandLayout();
  p.getLayout().addComponent(layout);
  w.getLayout().addComponent(p);
  createUI(layout);
}
