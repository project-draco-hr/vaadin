{
  if (isNotificationExpression(path)) {
    List<Element> elements=new ArrayList<Element>();
    for (    VNotification n : findNotificationsByPath(path)) {
      elements.add(n.getElement());
    }
    return elements;
  }
  List<Element> elems=eliminateDuplicates(getElementsByPathStartingAtConnector(path,client.getUIConnector()));
  return elems;
}
