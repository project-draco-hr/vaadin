{
  String[] starts={"//","/"};
  String[] frags={"com.vaadin.ui.Notification.class","com.vaadin.ui.Notification","VNotification.class","VNotification","Notification.class","Notification"};
  String[] ends={"/","["};
  for (  String s : starts) {
    for (    String f : frags) {
      if (path.equals(s + f)) {
        return true;
      }
      for (      String e : ends) {
        if (path.startsWith(s + f + e)) {
          return true;
        }
      }
    }
  }
  return false;
}
