{
  openTestURL();
  GridElement grid=getGridElement();
  grid.scrollToRow(300);
  grid.getHeaderCell(0,7).click();
  assertTrue("Header cell is not active.",grid.getHeaderCell(0,7).isActive());
  new Actions(getDriver()).sendKeys(Keys.ARROW_DOWN).perform();
  assertTrue("Body cell 282, 7 is not active",grid.getCell(282,7).isActive());
}
