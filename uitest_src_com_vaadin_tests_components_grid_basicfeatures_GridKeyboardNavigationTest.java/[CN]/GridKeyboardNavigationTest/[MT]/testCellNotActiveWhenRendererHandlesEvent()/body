{
  openTestURL();
  GridElement grid=getGridElement();
  assertTrue("Body cell 0, 0 is not active on init.",grid.getCell(0,0).isActive());
  grid.getHeaderCell(0,3).click();
  assertFalse("Body cell 0, 0 is active after click on header.",grid.getCell(0,0).isActive());
  assertTrue("Header cell 0, 3 is not active after click on header.",grid.getHeaderCell(0,3).isActive());
}
