{
  if (item.getCommand() != null) {
    System.out.println("itemClick, running command");
    setSelected(null);
    if (visibleChildMenu != null) {
      visibleChildMenu.hideChildren();
    }
    hideParents();
    DeferredCommand.addCommand(item.getCommand());
  }
 else {
    if (item.getSubMenu() != null && item.getSubMenu() != visibleChildMenu) {
      setSelected(item);
      showChildMenu(item);
    }
  }
}
