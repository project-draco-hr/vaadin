{
  if (item != null) {
    System.out.println("setSelected " + item.getText());
  }
 else {
    System.out.println("setSelected was null");
  }
  if (item != selected && selected != null) {
    selected.setSelected(false);
  }
  if (item != null) {
    item.setSelected(true);
  }
  selected=item;
}
