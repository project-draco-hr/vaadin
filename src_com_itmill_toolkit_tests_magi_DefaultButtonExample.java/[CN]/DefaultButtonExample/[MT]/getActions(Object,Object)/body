{
  Action[] actions=new Action[1];
  if (sender == ok) {
    actions[0]=new ShortcutAction("Default key",ShortcutAction.KeyCode.ENTER,null);
  }
 else   if (sender == cancel) {
    actions[0]=new ShortcutAction("Alt+C",ShortcutAction.KeyCode.C,new int[]{ShortcutAction.ModifierKey.ALT});
  }
 else   return null;
  return actions;
}
