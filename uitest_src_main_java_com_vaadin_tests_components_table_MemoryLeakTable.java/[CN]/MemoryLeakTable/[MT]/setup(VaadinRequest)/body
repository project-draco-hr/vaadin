{
  btnAdd.addClickListener(new ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      addRows();
    }
  }
);
  btnRemove.addClickListener(new ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      removeRows();
    }
  }
);
  addComponent(btnAdd);
  addComponent(btnRemove);
}
