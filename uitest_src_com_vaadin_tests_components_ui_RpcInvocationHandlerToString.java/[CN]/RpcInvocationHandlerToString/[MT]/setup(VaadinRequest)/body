{
  addComponent(log);
  Button b=new Button("Exec toString() for an invocation proxy",new Button.ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      log.log("An invoation proxy: " + dummyProxy.toString());
    }
  }
);
  addComponent(b);
  b=new Button("Exec hashCode() for an invocation proxy",new Button.ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      log.log("Invocation proxy.hashCode(): " + dummyProxy.hashCode());
    }
  }
);
  addComponent(b);
  b=new Button("Exec equals(false) for an invocation proxy",new Button.ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      log.log("Invocation proxy.equals(false): " + dummyProxy.equals(false));
    }
  }
);
  addComponent(b);
}
