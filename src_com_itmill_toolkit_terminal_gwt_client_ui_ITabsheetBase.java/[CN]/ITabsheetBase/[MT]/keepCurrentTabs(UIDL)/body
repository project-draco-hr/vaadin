{
  final UIDL tabs=uidl.getChildUIDL(0);
  boolean retval=tabKeys.size() == tabs.getNumberOfChildren();
  for (int i=0; retval && i < tabKeys.size(); i++) {
    retval=tabKeys.get(i).equals(tabs.getChildUIDL(i).getStringAttribute("key")) && captions.get(i).equals(tabs.getChildUIDL(i).getStringAttribute("caption"));
  }
  return retval;
}
