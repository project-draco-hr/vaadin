{
  VCaption caption=getWidgetForPaintable().captions.get(child);
  if (caption != null) {
    MeasureManager.MeasuredSize measuredSize=child.getMeasuredSize();
    int captionHeight=measuredSize.getDependencyHeight(caption.getElement());
    caption.getElement().getStyle().setMarginTop(-captionHeight,Unit.PX);
    return captionHeight;
  }
 else {
    return 0;
  }
}
