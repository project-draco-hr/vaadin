{
  for (  Node child : node.getChildren()) {
    if (child instanceof MixinDefNode) {
      mixinDefs.put(((MixinDefNode)child).getName(),(MixinDefNode)child);
    }
  }
  replaceMixins(node);
  for (  Node child : new ArrayList<Node>(node.getChildren())) {
    if (child instanceof MixinDefNode) {
      node.removeChild(child);
    }
  }
}
