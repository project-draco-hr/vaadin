{
  if (min != null) {
    final Date d=new Date(min.getTime());
    d.setHours(0);
    d.setMinutes(0);
    d.setSeconds(1);
    minDate=d;
  }
 else {
    minDate=null;
  }
  if (max != null) {
    final Date d=new Date(max.getTime());
    d.setHours(24);
    d.setMinutes(59);
    d.setSeconds(59);
    maxDate=d;
  }
 else {
    maxDate=null;
  }
}
