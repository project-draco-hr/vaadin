{
  if (sender instanceof VEventButton) {
    processClickEvent(sender,false);
    timer=new Timer(){
      @Override public void run(){
        timer=new Timer(){
          @Override public void run(){
            processClickEvent(sender,false);
          }
        }
;
        timer.scheduleRepeating(150);
      }
    }
;
    timer.schedule(500);
  }
}
