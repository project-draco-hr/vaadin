{
  Object sender=event.getSource();
  Cell cell=cal.days.getCellForEvent(event);
  if (sender != cal.days || cell == null || cell.getRowIndex() < 1 || cell.getRowIndex() > 6 || !cal.datefield.isEnabled() || cal.datefield.isReadonly() || cell.getCellIndex() < 1) {
    return;
  }
  final String text=cal.days.getText(cell.getRowIndex(),cell.getCellIndex());
  if (text.equals(" ")) {
    return;
  }
  selectDate(text);
}
