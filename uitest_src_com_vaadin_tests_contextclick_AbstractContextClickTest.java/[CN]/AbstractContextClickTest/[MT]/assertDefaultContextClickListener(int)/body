{
  AbstractComponentElement component=$(AbstractComponentElement.class).id("testComponent");
  contextClick(component);
  Point l=component.getLocation();
  int drift=0;
  if (BrowserUtil.isIE(getDesiredCapabilities(),10) || BrowserUtil.isIE(getDesiredCapabilities(),11)) {
    drift=1;
  }
  assertEquals(index + ". ContextClickEvent: (" + (l.getX() + 10)+ ", "+ (l.getY() + 10 + drift)+ ")",getLogRow(0));
}
