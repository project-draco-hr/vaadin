{
  latency=1000;
  openTestURL();
  addColumn(Renderers.CPLX_RENDERER);
  getGrid().scrollToRow(50);
  String backgroundColor=getGrid().getCell(1,1).getCssValue("backgroundColor");
  assertEquals("rgba(255, 0, 0, 1)",backgroundColor);
  sleep(3000);
  backgroundColor=getGrid().getCell(1,1).getCssValue("backgroundColor");
  assertEquals("rgba(255, 255, 255, 1)",backgroundColor);
}
