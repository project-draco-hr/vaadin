{
  selectMenuPath("Component","State","Width","1000px");
  assertColumnHeaderOrder(0,1,2,3,4,5,6);
  toggleHideColumn(2);
  assertColumnHeaderOrder(0,1,3,4,5,6);
  toggleHideColumn(2);
  assertColumnHeaderOrder(0,1,2,3,4,5,6);
  toggleHideColumn(2);
  assertColumnHeaderOrder(0,1,3,4,5,6);
  toggleHideColumn(2);
  assertColumnHeaderOrder(0,1,2,3,4,5,6);
}
