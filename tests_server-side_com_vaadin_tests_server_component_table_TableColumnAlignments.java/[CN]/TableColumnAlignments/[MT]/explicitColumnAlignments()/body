{
  int properties=5;
  Table t=TableGenerator.createTableWithDefaultContainer(properties,10);
  String[] explicitAlignments=new String[]{Table.ALIGN_CENTER,Table.ALIGN_LEFT,Table.ALIGN_RIGHT,Table.ALIGN_RIGHT,Table.ALIGN_LEFT};
  t.setColumnAlignments(explicitAlignments);
  assertArrayEquals("Explicit visible columns, 5 properties",explicitAlignments,t.getColumnAlignments());
}
