{
  Table t=TableGenerator.createTableWithDefaultContainer(3,7);
  String[] defaultAlignments=new String[]{Table.ALIGN_LEFT,Table.ALIGN_LEFT,Table.ALIGN_LEFT};
  try {
    t.setColumnAlignment("Property 1200",Table.ALIGN_LEFT);
  }
 catch (  IllegalArgumentException e) {
  }
  assertArrayEquals("Invalid change affected alignments",defaultAlignments,t.getColumnAlignments());
}
