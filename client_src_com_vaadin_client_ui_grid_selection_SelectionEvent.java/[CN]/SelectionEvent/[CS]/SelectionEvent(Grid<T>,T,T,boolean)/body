{
  this.grid=grid;
  this.batched=batched;
  if (added != null) {
    this.added=Collections.singletonList(added);
  }
 else {
    this.added=Collections.emptyList();
  }
  if (removed != null) {
    this.removed=Collections.singletonList(removed);
  }
 else {
    this.removed=Collections.emptyList();
  }
}
