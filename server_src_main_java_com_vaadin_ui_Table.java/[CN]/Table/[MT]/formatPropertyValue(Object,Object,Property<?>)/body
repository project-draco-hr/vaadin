{
  if (property == null) {
    return "";
  }
  LegacyConverter<String,Object> converter=null;
  if (hasConverter(colId)) {
    converter=getConverter(colId);
  }
 else {
    converter=(LegacyConverter)LegacyConverterUtil.getConverter(String.class,property.getType(),getSession());
  }
  Object value=property.getValue();
  if (converter != null) {
    return converter.convertToPresentation(value,String.class,getLocale());
  }
  return (null != value) ? value.toString() : "";
}
