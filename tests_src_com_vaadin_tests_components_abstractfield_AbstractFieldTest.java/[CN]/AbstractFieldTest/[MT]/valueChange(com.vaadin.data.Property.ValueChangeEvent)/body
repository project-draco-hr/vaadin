{
  Object o=event.getProperty().getValue();
  if (o instanceof Collection) {
    try {
      ArrayList<Comparable> c=new ArrayList<Comparable>((Collection)o);
      Collections.sort(c);
      o=c;
    }
 catch (    Exception e) {
      log("Exception while sorting value: " + e.getMessage());
    }
  }
  String value="null";
  if (o != null) {
    value="'" + o.toString() + "'";
    if (o instanceof Date) {
      value+="(" + ((Date)o).getTime() + ")";
    }
  }
  log(event.getClass().getSimpleName() + ", new value: " + value);
}
