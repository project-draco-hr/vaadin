{
  if (widget instanceof HasWidgets) {
    for (    Widget child : (HasWidgets)widget) {
      addIfPaintable(child,paintables,paintableMap);
    }
  }
 else   if (widget instanceof HasOneWidget) {
    Widget child=((HasOneWidget)widget).getWidget();
    addIfPaintable(child,paintables,paintableMap);
  }
}
