{
  debug("VFS: onClick()");
  if (textInputEnabled && event.getNativeEvent().getEventTarget().cast() == tb.getElement()) {
    return;
  }
  if (enabled && !readonly) {
    if (!suggestionPopup.isJustClosed()) {
      filterOptions(-1,"");
      dataReceivedHandler.popupOpenerClicked();
      lastFilter="";
    }
    DOM.eventPreventDefault(DOM.eventGetCurrentEvent());
    focus();
    tb.selectAll();
  }
}
