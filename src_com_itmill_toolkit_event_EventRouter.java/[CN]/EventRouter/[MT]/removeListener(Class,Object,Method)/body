{
  if (listenerList != null) {
    Iterator i=listenerList.iterator();
    while (i.hasNext()) {
      try {
        ListenerMethod lm=(ListenerMethod)i.next();
        if (lm.matches(eventType,target,method)) {
          i.remove();
        }
      }
 catch (      java.lang.ClassCastException e) {
      }
    }
  }
}
