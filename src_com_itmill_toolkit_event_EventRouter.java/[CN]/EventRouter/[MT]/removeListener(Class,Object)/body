{
  if (listenerList != null) {
    final Iterator i=listenerList.iterator();
    while (i.hasNext()) {
      try {
        final ListenerMethod lm=(ListenerMethod)i.next();
        if (lm.matches(eventType,target)) {
          i.remove();
        }
      }
 catch (      final java.lang.ClassCastException e) {
      }
    }
  }
}
