{
  setElement(DOM.createSpan());
  setHTML(html);
  setCommand(cmd);
  setSelected(false);
  setStyleName(CLASSNAME + "-menuitem");
  sinkEvents(VTooltip.TOOLTIP_EVENTS);
  NodeList<com.google.gwt.dom.client.Element> imgElements=getElement().getElementsByTagName("img");
  for (int i=0; i < imgElements.getLength(); i++) {
    DOM.sinkEvents((Element)imgElements.getItem(i),Event.ONLOAD);
  }
}
