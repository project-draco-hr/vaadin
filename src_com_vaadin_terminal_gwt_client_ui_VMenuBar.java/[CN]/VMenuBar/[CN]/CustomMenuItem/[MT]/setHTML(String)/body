{
  this.html=html;
  DOM.setInnerHTML(getElement(),html);
  if (BrowserInfo.get().isIE6() && client != null) {
    final NodeList<com.google.gwt.dom.client.Element> imgs=getElement().getElementsByTagName("IMG");
    if (imgs.getLength() > 0) {
      client.addPngFix((Element)imgs.getItem(0).cast());
    }
  }
}
