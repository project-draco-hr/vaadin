{
  setSelected(item);
  boolean menuWasVisible=visibleChildMenu != null;
  if (menuWasVisible && visibleChildMenu != item.getSubMenu()) {
    popup.hide();
  }
  if (item.getSubMenu() != null && (parentMenu != null || menuWasVisible) && visibleChildMenu != item.getSubMenu()) {
    showChildMenu(item);
  }
}
