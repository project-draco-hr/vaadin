{
  HashMap<Object,Object> map=new HashMap<Object,Object>();
  Iterator<String> it=jsonMap.keySet().iterator();
  while (it.hasNext()) {
    String key=it.next();
    JSONArray encodedKey=(JSONArray)JSONParser.parseStrict(key);
    JSONArray encodedValue=(JSONArray)jsonMap.get(key);
    Object decodedKey=decodeValue(encodedKey,null,idMapper,connection);
    Object decodedValue=decodeValue(encodedValue,null,idMapper,connection);
    map.put(decodedKey,decodedValue);
  }
  return map;
}
