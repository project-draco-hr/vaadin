{
  if (Container.ItemSetChangeEvent.class.isAssignableFrom(eventType)) {
    if (itemSetEventListeners == null)     return Collections.EMPTY_LIST;
 else     return Collections.unmodifiableCollection(itemSetEventListeners);
  }
 else   if (Container.PropertySetChangeEvent.class.isAssignableFrom(eventType)) {
    if (propertySetEventListeners == null)     return Collections.EMPTY_LIST;
 else     return Collections.unmodifiableCollection(propertySetEventListeners);
  }
  return super.getListeners(eventType);
}
