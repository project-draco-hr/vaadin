{
  Method sanityCheckHeader;
  try {
    sanityCheckHeader=LegacyGrid.Header.class.getDeclaredMethod("sanityCheck");
    sanityCheckHeader.setAccessible(true);
    Method sanityCheckFooter=LegacyGrid.Footer.class.getDeclaredMethod("sanityCheck");
    sanityCheckFooter.setAccessible(true);
    sanityCheckHeader.invoke(getHeader());
    sanityCheckFooter.invoke(getFooter());
  }
 catch (  Exception e) {
    throw e.getCause();
  }
}
