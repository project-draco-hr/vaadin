{
  hasFocus=false;
  navKeyDown=false;
  if (BrowserInfo.get().isIE()) {
    Element focusedElement=WidgetUtil.getFocusedElement();
    if (Util.getConnectorForElement(client,getParent(),focusedElement) == this && focusedElement != null && focusedElement != scrollBodyPanel.getFocusElement()) {
      focus();
      return;
    }
  }
  if (isFocusable()) {
    setRowFocus(null);
  }
}
