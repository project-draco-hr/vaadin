{
  int prepx=(int)Math.round(getRowHeight() * firstRendered);
  if (prepx < 0) {
    prepx=0;
  }
  preSpacer.getStyle().setPropertyPx("height",prepx);
  int postpx=(int)(getRowHeight() * (totalRows - 1 - lastRendered));
  if (postpx < 0) {
    postpx=0;
  }
  postSpacer.getStyle().setPropertyPx("height",postpx);
}
