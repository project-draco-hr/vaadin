{
  int prepx=getRowHeight() * firstRendered;
  if (prepx < 0) {
    prepx=0;
  }
  DOM.setStyleAttribute(preSpacer,"height",prepx + "px");
  int postpx=getRowHeight() * (totalRows - 1 - lastRendered);
  if (postpx < 0) {
    postpx=0;
  }
  DOM.setStyleAttribute(postSpacer,"height",postpx + "px");
}
