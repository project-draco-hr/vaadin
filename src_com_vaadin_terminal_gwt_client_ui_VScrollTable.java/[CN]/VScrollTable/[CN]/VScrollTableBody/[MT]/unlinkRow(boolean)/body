{
  if (lastRendered - firstRendered < 0) {
    return false;
  }
  int index;
  if (fromBeginning) {
    index=0;
    firstRendered++;
  }
 else {
    index=renderedRows.size() - 1;
    lastRendered--;
  }
  if (index >= 0) {
    unlinkRowAtIndex(index);
    fixSpacers();
    return true;
  }
 else {
    return false;
  }
}
