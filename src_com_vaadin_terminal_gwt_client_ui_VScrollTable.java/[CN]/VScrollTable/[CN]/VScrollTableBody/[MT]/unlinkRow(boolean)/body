{
  if (lastRendered - firstRendered < 0) {
    return false;
  }
  int actualIx;
  if (fromBeginning) {
    actualIx=0;
    firstRendered++;
  }
 else {
    actualIx=renderedRows.size() - 1;
    lastRendered--;
  }
  if (actualIx >= 0) {
    unlinkRowAtActualIndex(actualIx);
    fixSpacers();
    return true;
  }
  return false;
}
