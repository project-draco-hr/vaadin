{
  selected=!selected;
  if (selected) {
    if (selectMode == Table.SELECT_MODE_SINGLE) {
      deselectAll();
    }
    selectedRowKeys.add(String.valueOf(rowKey));
    addStyleName("v-selected");
  }
 else {
    selectedRowKeys.remove(String.valueOf(rowKey));
    removeStyleName("v-selected");
  }
}
