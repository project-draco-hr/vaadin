{
  TableDDDetails oldDetails=dropDetails;
  updateDropDetails(drag);
  if (!oldDetails.equals(dropDetails)) {
    deEmphasis();
    VAcceptCallback cb=new VAcceptCallback(){
      public void accepted(      VDragEvent event){
        dragAccepted(event);
      }
    }
;
    validate(cb,drag);
  }
}
