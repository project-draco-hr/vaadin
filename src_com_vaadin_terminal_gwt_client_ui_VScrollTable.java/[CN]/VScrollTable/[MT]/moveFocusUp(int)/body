{
  if (isSelectable()) {
    if (focusedRow == null && scrollBody.iterator().hasNext()) {
      return setRowFocus((VScrollTableRow)scrollBody.iterator().next());
    }
 else {
      VScrollTableRow prev=getPreviousRow(focusedRow,offset);
      if (prev != null) {
        return setRowFocus(prev);
      }
    }
  }
  return false;
}
