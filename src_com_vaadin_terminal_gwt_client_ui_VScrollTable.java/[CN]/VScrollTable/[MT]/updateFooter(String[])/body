{
  if (strings == null) {
    return;
  }
  int colIndex=0;
  if (showRowHeaders) {
    tFoot.enableColumn("0",colIndex);
    colIndex++;
  }
 else {
    tFoot.removeCell("0");
  }
  int i;
  for (i=0; i < strings.length; i++) {
    final String cid=strings[i];
    tFoot.enableColumn(cid,colIndex);
    colIndex++;
  }
  tFoot.setVisible(showColFooters);
}
