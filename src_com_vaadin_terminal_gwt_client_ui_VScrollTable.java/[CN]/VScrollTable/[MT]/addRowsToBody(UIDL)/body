{
  if (partialRowAdditions == null) {
    return;
  }
  if (partialRowAdditions.hasAttribute("hide")) {
    scrollBody.unlinkRows(partialRowAdditions.getIntAttribute("firstprowix"),partialRowAdditions.getIntAttribute("numprows"));
  }
 else {
    scrollBody.insertRows(partialRowAdditions,partialRowAdditions.getIntAttribute("firstprowix"),partialRowAdditions.getIntAttribute("numprows"));
  }
  updateCache();
}
