{
  if (selectMode > VScrollTable.SELECT_MODE_NONE) {
    if (focusedRow == null) {
      setRowFocus((VScrollTableRow)scrollBody.iterator().next());
      return true;
    }
 else {
      VScrollTableRow prev=getPreviousRow(focusedRow);
      if (prev != null) {
        setRowFocus(prev);
        return true;
      }
    }
  }
  return false;
}
