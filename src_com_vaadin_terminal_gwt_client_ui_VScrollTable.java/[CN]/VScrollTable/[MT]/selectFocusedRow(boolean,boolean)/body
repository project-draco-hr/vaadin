{
  if (focusedRow != null) {
    if (selectMode > SELECT_MODE_NONE && !ctrlSelect && !shiftSelect) {
      deselectAll();
      focusedRow.toggleSelection(!ctrlSelect);
    }
 else     if (selectMode > SELECT_MODE_NONE && ctrlSelect && !shiftSelect) {
    }
 else     if (selectMode == SELECT_MODE_MULTI && !ctrlSelect && shiftSelect) {
      focusedRow.toggleShiftSelection(shiftSelect);
    }
  }
}
