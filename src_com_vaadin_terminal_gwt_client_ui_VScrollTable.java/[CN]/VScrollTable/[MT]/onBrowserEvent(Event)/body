{
  if (event.getTypeInt() == Event.ONKEYUP) {
    if (event.getKeyCode() == KeyCodes.KEY_SHIFT) {
      sendSelectedRows();
    }
 else     if ((event.getKeyCode() == getNavigationUpKey() || event.getKeyCode() == getNavigationDownKey() || event.getKeyCode() == getNavigationPageUpKey() || event.getKeyCode() == getNavigationPageDownKey()) && !event.getShiftKey()) {
      sendSelectedRows();
    }
    scrollingVelocityTimer.cancel();
    scrollingVelocityTimer=null;
    scrollingVelocity=10;
  }
}
