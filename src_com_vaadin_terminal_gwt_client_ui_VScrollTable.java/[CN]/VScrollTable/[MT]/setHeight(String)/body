{
  this.height=height;
  super.setHeight(height);
  setContainerHeight();
  if (initializedAndAttached) {
    updatePageLength();
  }
  if (!rendering) {
    Util.runWebkitOverflowAutoFix(scrollBodyPanel.getElement());
  }
  if (!isFocusable()) {
    scrollBodyPanel.getElement().setTabIndex(-1);
  }
 else {
    scrollBodyPanel.getElement().setTabIndex(0);
  }
}
