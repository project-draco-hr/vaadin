{
  hasFocus=false;
  navKeyDown=false;
  if (BrowserInfo.get().isIE()) {
    Element focusedElement=Util.getIEFocusedElement();
    if (Util.getPaintableForElement(client,getParent(),focusedElement) == this) {
      focus();
      return;
    }
  }
  if (isFocusable()) {
    setRowFocus(null);
  }
}
