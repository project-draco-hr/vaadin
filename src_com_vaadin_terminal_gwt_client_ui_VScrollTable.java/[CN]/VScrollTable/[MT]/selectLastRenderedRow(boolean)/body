{
  VScrollTableRow row=null;
  Iterator<Widget> it=scrollBody.iterator();
  while (it.hasNext()) {
    row=(VScrollTableRow)it.next();
  }
  if (row != null) {
    setRowFocus(row);
    if (!focusOnly) {
      deselectAll();
      selectFocusedRow(false,false);
      sendSelectedRows();
    }
  }
}
