{
  if (selectMode > VScrollTable.SELECT_MODE_NONE) {
    if (focusedRow == null) {
      setRowFocus((VScrollTableRow)scrollBody.iterator().next());
      return true;
    }
 else {
      VScrollTableRow next=getNextRow(focusedRow);
      if (next != null) {
        setRowFocus(next);
        return true;
      }
    }
  }
  return false;
}
