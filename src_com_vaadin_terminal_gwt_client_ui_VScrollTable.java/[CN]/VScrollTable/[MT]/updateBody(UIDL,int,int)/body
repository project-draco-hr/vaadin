{
  if (uidl == null || reqRows < 1) {
    if (firstRow <= 0) {
      while (scrollBody.getLastRendered() > scrollBody.firstRendered) {
        scrollBody.unlinkRow(false);
      }
      scrollBody.unlinkRow(false);
    }
    return;
  }
  scrollBody.renderRows(uidl,firstRow,reqRows);
  discardRowsOutsideCacheWindow();
}
