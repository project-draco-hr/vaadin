{
  if (hasFocus) {
    if (handleNavigation(event.getNativeEvent().getKeyCode(),event.isControlKeyDown() || event.isMetaKeyDown(),event.isShiftKeyDown())) {
      event.preventDefault();
    }
    if (scrollingVelocityTimer == null) {
      scrollingVelocityTimer=new Timer(){
        @Override public void run(){
          scrollingVelocity++;
        }
      }
;
      scrollingVelocityTimer.scheduleRepeating(100);
    }
  }
 else   if (!enabled) {
    event.preventDefault();
  }
}
