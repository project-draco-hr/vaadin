{
  assertNotNull("Grandchildren must not be null",root.childDesign.childLabel);
  assertNotNull("Grandchildren must not be null",root.childDesign.childCustomComponent);
  assertEquals("child label caption must be read","test content",root.childDesign.childLabel.getValue());
  assertEquals("child custom component caption must be read","custom content",root.childDesign.childCustomComponent.getCaption());
}
