{
  NestedMethodProperty streetProperty=new NestedMethodProperty(vaadin,"manager.address.street");
  ByteArrayOutputStream baos=new ByteArrayOutputStream();
  new ObjectOutputStream(baos).writeObject(streetProperty);
  NestedMethodProperty property2=(NestedMethodProperty)new ObjectInputStream(new ByteArrayInputStream(baos.toByteArray())).readObject();
  Assert.assertEquals("Ruukinkatu 2-4",property2.getValue());
}
