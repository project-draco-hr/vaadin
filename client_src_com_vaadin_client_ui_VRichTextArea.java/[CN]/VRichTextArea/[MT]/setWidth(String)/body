{
  if (width.endsWith("px")) {
    float w=Float.parseFloat(width.substring(0,width.length() - 2));
    w-=getExtraHorizontalPixels();
    if (w < 0) {
      w=0;
    }
    super.setWidth(w + "px");
  }
 else   if (width.equals("")) {
    super.setWidth(toolbarNaturalWidth + "px");
  }
 else {
    super.setWidth(width);
  }
}
