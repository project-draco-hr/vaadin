{
  StatementHelper sh;
  if (orderBys == null || orderBys.isEmpty()) {
    List<OrderBy> ob=new ArrayList<OrderBy>();
    ob.add(new OrderBy(primaryKeyColumns.get(0),true));
    sh=sqlGenerator.generateSelectQuery(tableName,filters,ob,offset,pagelength,null);
  }
 else {
    sh=sqlGenerator.generateSelectQuery(tableName,filters,orderBys,offset,pagelength,null);
  }
  return executeQuery(sh);
}
