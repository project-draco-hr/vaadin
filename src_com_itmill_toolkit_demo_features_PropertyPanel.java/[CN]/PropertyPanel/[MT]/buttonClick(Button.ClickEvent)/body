{
  try {
    FeatureUtil.debug(getApplication().getUser().toString(),"buttonClick " + event.getButton().getTag() + ", "+ event.getButton().getCaption()+ ", "+ event.getButton().getValue());
  }
 catch (  Exception e) {
  }
  if (event.getButton() == setButton) {
    for (Iterator i=forms.iterator(); i.hasNext(); )     ((Form)i.next()).commit();
  }
  if (event.getButton() == discardButton) {
    for (Iterator i=forms.iterator(); i.hasNext(); )     ((Form)i.next()).discard();
  }
}
