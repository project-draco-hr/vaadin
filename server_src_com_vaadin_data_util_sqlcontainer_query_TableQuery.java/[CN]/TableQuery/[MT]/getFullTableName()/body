{
  if (fullTableName == null) {
    StringBuilder sb=new StringBuilder();
    if (catalogName != null) {
      sb.append(catalogName).append(".");
    }
    if (schemaName != null) {
      sb.append(schemaName).append(".");
    }
    sb.append(tableName);
    fullTableName=sb.toString();
  }
  return fullTableName;
}
