{
  if ("button".equals(tag))   return new TkButton();
  if ("window".equals(tag))   return new TkWindow();
  if ("orderedlayout".equals(tag))   return new TkOrderedLayout();
  if ("label".equals(tag))   return new TkLabel();
  if ("gridlayout".equals(tag))   return new TkGridLayout();
  if ("tree".equals(tag))   return new TkTree();
  if ("select".equals(tag))   return new TkSelect();
  return new TkUnknownComponent();
}
