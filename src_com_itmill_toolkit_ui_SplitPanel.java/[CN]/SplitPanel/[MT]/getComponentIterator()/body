{
  return new Iterator(){
    int i=0;
    public boolean hasNext(){
      if (i < 2)       return true;
      return false;
    }
    public Object next(){
      i++;
      if (i == 1)       return firstComponent;
 else       if (i == 2)       return secondComponent;
      return null;
    }
    public void remove(){
      if (i == 1)       setFirstComponent(null);
 else       if (i == 2)       setSecondComponent(null);
    }
  }
;
}
