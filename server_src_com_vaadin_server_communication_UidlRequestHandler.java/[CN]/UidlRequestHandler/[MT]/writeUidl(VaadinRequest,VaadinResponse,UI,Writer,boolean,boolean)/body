{
  openJsonMessage(writer,response);
  Object writeSecurityTokenFlag=request.getAttribute(AbstractCommunicationManager.WRITE_SECURITY_TOKEN_FLAG);
  if (writeSecurityTokenFlag != null) {
    writer.write(ui.getSession().getCommunicationManager().getSecurityKeyUIDL(request));
  }
  new UidlWriter().write(ui,writer,repaintAll,analyzeLayouts);
  closeJsonMessage(writer);
}
