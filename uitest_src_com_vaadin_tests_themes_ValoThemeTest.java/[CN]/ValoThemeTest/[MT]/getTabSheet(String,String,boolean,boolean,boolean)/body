{
  TabSheet ts=new TabSheet();
  ts.addStyleName(style);
  ts.setCaption(caption);
  for (int i=0; i < (scrolling ? 10 : 3); i++) {
    String tabcaption=nextString(true);
    if (i == 0 && icon) {
      tabcaption=null;
    }
    Tab t=ts.addTab(new Label("??"),tabcaption);
    t.setClosable(closable);
    if (icon) {
      t.setIcon(icon(i == 2));
    }
  }
  return ts;
}
