{
  menuItems.clear();
  final VerticalLayout root=new VerticalLayout();
  root.setWidth("33.333%");
  root.setMargin(true);
  root.addStyleName("components");
  root.addStyleName("color-context" + num);
  labels(root);
  buttonsAndLinks(root);
  textfields(root);
  textareas(root);
  comboboxes(root);
  menubars(root);
  checkboxes(root);
  optiongroups(root);
  datefields(root);
  panels(root);
  trees(root);
  tables(root);
  treetables(root);
  sliders(root);
  splitpanels(root);
  tabsheets(root);
  colorpickers(root);
  accordions(root);
  nativeselects(root);
  return root;
}
