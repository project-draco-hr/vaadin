  combo.setContainerDataSource(generatedContainer);
  combo.setItemCaptionPropertyId(CAPTION_PROPERTY);
  combo.setItemIconPropertyId(ICON_PROPERTY);
  row.addComponent(combo);
  combo=new ComboBox("Explicit size");
