  combo.addItem("Option One");
  combo.addItem("Option Two");
  combo.addItem("Option Three");
  combo.setEnabled(false);
  row.addComponent(combo);
  combo=new ComboBox("Custom color");
  combo.setInputPrompt("You can type here");
