{
  Window w=new Window("#1934 : Horizontal ExpandLayout completely broken");
  setMainWindow(w);
  w.addComponent(new Label("Horizontal 500x200 ExpandLayout with two components:"));
  ExpandLayout testedLayout=new ExpandLayout(ExpandLayout.ORIENTATION_HORIZONTAL);
  testedLayout.setWidth(500);
  testedLayout.setHeight(200);
  Button b1=new Button("b1");
  testedLayout.addComponent(b1);
  testedLayout.expand(b1);
  testedLayout.addComponent(new Button("b2"));
  w.addComponent(testedLayout);
}
