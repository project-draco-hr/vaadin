{
  LinkedHashMap<String,Integer> options=new LinkedHashMap<>();
  options.put("-",0);
  for (int i=1; i <= 10; i++) {
    options.put(String.valueOf(i),i);
  }
  options.put("50",50);
  options.put("100",100);
  options.put("1000",1000);
  super.createSelectAction("Rows",CATEGORY_DATA_SOURCE,options,"-",rowsAction);
}
