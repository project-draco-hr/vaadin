{
  if (getHeader().getElement() != element && getHeader().getElement().isOrHasChild(element)) {
    return getHeader();
  }
 else   if (getBody().getElement() != element && getBody().getElement().isOrHasChild(element)) {
    return getBody();
  }
 else   if (getFooter().getElement() != element && getFooter().getElement().isOrHasChild(element)) {
    return getFooter();
  }
  return null;
}
