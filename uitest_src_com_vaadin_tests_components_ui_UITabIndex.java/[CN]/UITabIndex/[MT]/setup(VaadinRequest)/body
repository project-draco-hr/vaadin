{
  Button b;
  b=new Button("Set tabIndex to -1");
  b.addClickListener(new ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      setTabIndex(-1);
    }
  }
);
  addComponent(b);
  b=new Button("Set tabIndex to 0");
  b.addClickListener(new ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      setTabIndex(0);
    }
  }
);
  addComponent(b);
  b=new Button("Set tabIndex to 1");
  b.addClickListener(new ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      setTabIndex(1);
    }
  }
);
  addComponent(b);
}
