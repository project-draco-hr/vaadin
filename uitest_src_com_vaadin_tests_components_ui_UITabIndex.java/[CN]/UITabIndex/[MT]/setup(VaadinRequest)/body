{
  addButton("Set tabIndex to -1",new ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      setTabIndex(-1);
    }
  }
);
  addButton("Set tabIndex to 0",new ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      setTabIndex(0);
    }
  }
);
  addButton("Set tabIndex to 1",new ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      setTabIndex(1);
    }
  }
);
}
