{
  openTestURL();
  tf=$(TextFieldElement.class).first();
  tfSlot=tf.findElement(By.xpath(".."));
  cb=$(CheckBoxElement.class).first();
  cbSlot=cb.findElement(By.xpath(".."));
  tf.setValue("a");
  assertSizes();
  cb.click();
  assertSizes();
  tf.setValue("b");
  assertSizes();
  cb.click();
  assertSizes();
}
