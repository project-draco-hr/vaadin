{
  if (columnGenerators.containsKey(colunmId)) {
    columnGenerators.remove(colunmId);
    if (!items.getContainerPropertyIds().contains(colunmId)) {
      visibleColumns.remove(colunmId);
    }
    resetPageBuffer();
    refreshRenderedCells();
    return true;
  }
 else {
    return false;
  }
}
