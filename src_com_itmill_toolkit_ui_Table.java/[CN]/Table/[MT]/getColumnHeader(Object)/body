{
  if (getColumnHeaderMode() == COLUMN_HEADER_MODE_HIDDEN) {
    return null;
  }
  String header=(String)columnHeaders.get(propertyId);
  if ((header == null && getColumnHeaderMode() == COLUMN_HEADER_MODE_EXPLICIT_DEFAULTS_ID) || getColumnHeaderMode() == COLUMN_HEADER_MODE_ID) {
    header=propertyId.toString();
  }
  return header;
}
