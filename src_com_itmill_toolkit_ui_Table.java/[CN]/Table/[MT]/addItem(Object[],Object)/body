{
  Object[] cols=getVisibleColumns();
  if (cells.length != cols.length) {
    return null;
  }
  Item item;
  if (itemId == null) {
    itemId=items.addItem();
    if (itemId == null) {
      return null;
    }
    item=items.getItem(itemId);
  }
 else {
    item=items.addItem(itemId);
  }
  if (item == null) {
    return null;
  }
  for (int i=0; i < cols.length; i++) {
    item.getItemProperty(cols[i]).setValue(cells[i]);
  }
  return itemId;
}
