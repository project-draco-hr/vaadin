{
  OrderedLayout l=new OrderedLayout();
  TextField tf=new TextField("Caption");
  Panel test=new Panel("TextField Component Demo");
  test.addComponent(tf);
  l.addComponent(test);
  PropertyPanel p=new PropertyPanel(tf);
  l.addComponent(p);
  Form f=p.createBeanPropertySet(new String[]{"columns","rows","wordwrap","writeThrough","readThrough","nullRepresentation","nullSettingAllowed","secret"});
  p.addProperties("Text field properties",f);
  return l;
}
