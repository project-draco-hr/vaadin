{
  if (mimeType != this.mimeType || (mimeType != null && !mimeType.equals(this.mimeType))) {
    this.mimeType=mimeType;
    if ("application/x-shockwave-flash".equals(mimeType)) {
      if (getParameter("wmode") == null) {
        setParameter("wmode","transparent");
      }
    }
    requestRepaint();
  }
}
