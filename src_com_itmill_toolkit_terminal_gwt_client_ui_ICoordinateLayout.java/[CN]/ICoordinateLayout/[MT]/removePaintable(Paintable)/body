{
  CustomCaption header=componentToHeader.get(p);
  CustomCaption marker=componentToMarker.get(p);
  if (header != null) {
    componentToHeader.remove(p);
    remove(header);
  }
  if (marker != null) {
    componentToHeader.remove(p);
    remove(marker);
  }
  client.unregisterPaintable(p);
  return remove((Widget)p);
}
