{
  verticalDropLocation=VerticalDropLocation.get(getElement(),drag.getCurrentGwtEvent().getClientY(),0.2);
  drag.getDropDetails().put("verticalLocation",verticalDropLocation.toString());
  horizontalDropLocation=HorizontalDropLocation.get(getElement(),drag.getCurrentGwtEvent().getClientX(),0.2);
  drag.getDropDetails().put("horizontalLocation",horizontalDropLocation.toString());
}
