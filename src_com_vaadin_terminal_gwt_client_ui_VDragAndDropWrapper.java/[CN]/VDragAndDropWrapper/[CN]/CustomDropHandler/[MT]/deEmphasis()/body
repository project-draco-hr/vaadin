{
  if (verticalDropLocation != null) {
    VDragAndDropWrapper.setStyleName(getElement(),OVER_STYLE,false);
    VDragAndDropWrapper.setStyleName(getElement(),OVER_STYLE + "-" + verticalDropLocation.toString().toLowerCase(),false);
    VDragAndDropWrapper.setStyleName(getElement(),OVER_STYLE + "-" + horizontalDropLocation.toString().toLowerCase(),false);
  }
}
