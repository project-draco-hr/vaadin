{
  Object ret=null;
  try {
    if (clz == String.class) {
      ret=o.getString(k);
    }
 else     if (clz == JSONObject.class) {
      ret=o.getJSONObject(k);
    }
 else     if (clz == Integer.class) {
      ret=o.getInt(k);
    }
 else     if (clz == Date.class) {
      ret=new Date(o.getLong(k));
    }
 else     if (clz == Boolean.class) {
      ret=o.getBoolean(k);
    }
  }
 catch (  JSONException e) {
  }
  return (T)ret;
}
