{
  super.setWidgetStyleNameWithPrefix(prefix,styleName,add);
  if (!styleName.startsWith("-")) {
    getWidget().popup.setStyleName(VPopupCalendar.POPUP_PRIMARY_STYLE_NAME + "-" + styleName,add);
  }
 else {
    getWidget().popup.setStyleName(VPopupCalendar.POPUP_PRIMARY_STYLE_NAME + styleName,add);
  }
}
