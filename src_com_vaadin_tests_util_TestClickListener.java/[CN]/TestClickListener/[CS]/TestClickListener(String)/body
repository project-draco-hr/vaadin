{
  Integer count=null;
  try {
    count=(Integer)buttonListeners.get(name);
    count=new Integer(count.intValue() + 1);
    buttonListeners.put(name,count);
  }
 catch (  Exception e) {
    count=new Integer(1);
    buttonListeners.put(name,count);
  }
  this.name=name;
  this.count=count.intValue();
  System.out.println("Created listener " + name + ", id="+ count);
}
