{
  selectMenuPath("Component","Editor row","Edit item 100");
  List<WebElement> widgets=getEditorRow().findElements(By.className("v-textfield"));
  widgets.get(0).click();
  widgets.get(0).sendKeys(" changed");
  WebElement saveButton=getEditorRow().findElement(By.className("v-editor-row-save"));
  saveButton.click();
  assertEquals("(100, 0) changed",getGridElement().getCell(100,0).getText());
}
