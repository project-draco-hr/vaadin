{
  if (resourceElement != null || objectElement != null) {
    String src=getResourceUrl("src");
    if (src != null && !src.isEmpty()) {
      if (!src.equals(resourceUrl)) {
        setResourceUrl(src);
      }
    }
 else     if (resourceUrl != null && !resourceUrl.isEmpty()) {
      setResourceUrl("");
    }
  }
}
