{
  if (isIE() && getBrowserMajorVersion() < 8 && getBrowserEngineVersion() < 4) {
    return true;
  }
  if (isSafari() && getBrowserEngineVersion() < 533) {
    return true;
  }
  if (isFirefox() && getBrowserMajorVersion() < 4) {
    return true;
  }
  if (isOpera() && getBrowserMajorVersion() < 11) {
    return true;
  }
  return false;
}
