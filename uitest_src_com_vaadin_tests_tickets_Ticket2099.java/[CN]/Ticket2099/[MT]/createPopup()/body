{
  Window w=new Window("Popup");
  TabSheet ts=new TabSheet();
  ol1=new VerticalLayout();
  ol2=new VerticalLayout();
  ol3=new VerticalLayout();
  l1=new Label("111");
  l2=new Label("222");
  l3=new Label("333");
  ol1.addComponent(l1);
  ol2.addComponent(l2);
  ol3.addComponent(l3);
  ts.addTab(ol1,"1",null);
  ts.addTab(ol2,"2",null);
  ts.addTab(ol3,"3",null);
  w.addComponent(ts);
  return w;
}
