{
  openTestURL(DisabledSortingTableSqlContainer.class);
  assertThatFirstCellHasText("1");
  sortFirstColumnAscending();
  assertThatFirstCellHasText("2");
  disableSorting();
  sortByEmptyArray();
  assertThatFirstCellHasText("1");
  openTestURL(DisabledSortingTableSqlContainer.class,"closeApplication");
}
