{
  final Editor<T> editor=event.getEditor();
  final boolean isBody=event.getCell().isBody();
  if (event.getGrid().isEditorActive()) {
    return (!editor.isBuffered() && isBody && handleMoveEvent(event)) || handleCloseEvent(event) || editor.isBuffered();
  }
 else {
    return event.getGrid().isEnabled() && isBody && handleOpenEvent(event);
  }
}
