{
  if (filters.isEmpty()) {
    filteredItemIds=(ListSet<IDTYPE>)allItemIds.clone();
  }
  for (  Item item : itemIdToItem.values()) {
    addValueChangeListener(item,propertyId);
  }
  Filter f=new Filter(propertyId,filterString,ignoreCase,onlyMatchPrefix);
  filter(f);
  filters.add(f);
  fireItemSetChange();
}
