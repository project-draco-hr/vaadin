{
  return tokenGroup1.clientToken.equals(ApplicationConstants.CSRF_TOKEN_DEFAULT_VALUE) && isUndefined(tokenGroup1.tokenReceivedFromServer) && isUndefined(tokenGroup1.tokenSentToServer)&& tokenGroup2.clientToken.equals(ApplicationConstants.CSRF_TOKEN_DEFAULT_VALUE)&& isUndefined(tokenGroup2.tokenReceivedFromServer)&& isNull(tokenGroup2.tokenSentToServer);
}
