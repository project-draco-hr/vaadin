{
  String ver=null;
  for (int i=0; i < args.length; i++) {
    if (ARG_VERSION.equals(args[i])) {
      if (args.length >= i) {
        ver=args[i + 1];
      }
      break;
    }
  }
  combineTheme(new String[]{BASE},false,ver);
  combineTheme(new String[]{BASE,RUNO},false,ver);
  combineTheme(new String[]{BASE,REINDEER},true,ver);
}
