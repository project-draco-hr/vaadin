{
  selectMenuPath("Component","State","Selection mode","multi");
  selectMenuPath(EDIT_ROW_5);
  WebElement editor=getEditor();
  List<WebElement> selectorDivs=editor.findElements(By.cssSelector("div"));
  assertTrue("selector column cell should've been empty",selectorDivs.get(0).getAttribute("innerHTML").isEmpty());
  assertFalse("normal column cell shoul've had contents",selectorDivs.get(1).getAttribute("innerHTML").isEmpty());
}
