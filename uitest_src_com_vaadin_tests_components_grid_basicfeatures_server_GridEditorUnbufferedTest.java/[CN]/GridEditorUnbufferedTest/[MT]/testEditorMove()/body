{
  selectMenuPath(EDIT_ITEM_5);
  assertEditorOpen();
  String firstFieldValue=getEditorWidgets().get(0).getAttribute("value");
  assertTrue("Editor is not at correct row index (5)","(5, 0)".equals(firstFieldValue));
  getGridElement().getCell(10,0).click();
  firstFieldValue=getEditorWidgets().get(0).getAttribute("value");
  assertTrue("Editor is not at correct row index (10)","(10, 0)".equals(firstFieldValue));
}
