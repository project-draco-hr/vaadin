{
  ComboBox cb=new ComboBox();
  cb.setId("select" + debugIdCounter++);
  if (caption != null) {
    cb.setCaption(caption);
  }
  if (primaryStyleName != null) {
    cb.addStyleName(primaryStyleName);
  }
  if (width != null) {
    cb.setWidth(width);
  }
  createDummyData(cb);
  addComponent(cb);
}
