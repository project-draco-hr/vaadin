{
  pending=false;
  if (!isBusy()) {
    Class<? extends ServerConnector> nextType=getNextType();
    if (nextType == null) {
      deferredWidgetLoader=null;
    }
 else {
      communicationFree=0;
      widgetSet.loadImplementation(nextType);
    }
  }
 else {
    schedule(FREE_CHECK_TIMEOUT);
  }
}
