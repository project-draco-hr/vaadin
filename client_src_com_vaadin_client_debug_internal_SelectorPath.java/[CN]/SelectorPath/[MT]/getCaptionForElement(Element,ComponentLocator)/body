{
  String caption=null;
  ComponentConnector connector=Util.findPaintable(locator.getClient(),element);
  if (null != connector) {
    Property property=AbstractConnector.getStateType(connector).getProperty("caption");
    try {
      Object value=property.getValue(connector.getState());
      if (null != value) {
        caption=String.valueOf(value);
      }
    }
 catch (    NoDataException e) {
    }
  }
  return caption;
}
