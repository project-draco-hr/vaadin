{
  if (null != getParent()) {
    Element parentElement=getParent().findElement();
    if (null == parentElement) {
      return null;
    }
    Element element=getLocator().getElementByPathStartingAt(getPath(),parentElement);
    return element;
  }
 else {
    Element element=getLocator().getElementByPath(getPath());
    return element;
  }
}
