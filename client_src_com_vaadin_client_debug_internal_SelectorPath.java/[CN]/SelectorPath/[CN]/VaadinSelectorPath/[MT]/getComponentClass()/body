{
  ComponentConnector connector=Util.findPaintable(getLocator().getClient(),findElement());
  Class<? extends ServerConnector> connectorClass=connector.getClass();
  FastStringSet identifiers=TypeDataStore.get().findIdentifiersFor(connectorClass);
  JsArrayString ids=identifiers.dump();
  if (ids.length() == 1) {
    return ids.get(0);
  }
 else {
    return null;
  }
}
