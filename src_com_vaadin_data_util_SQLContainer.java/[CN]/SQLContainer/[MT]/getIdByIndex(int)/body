{
  if (index < 0 || index > size() - 1) {
    return null;
  }
  if (index < size) {
    if (itemIndexes.keySet().contains(index)) {
      return itemIndexes.get(index);
    }
    updateOffsetAndCache(index);
    return itemIndexes.get(index);
  }
 else {
    int offset=index - size;
    return addedItems.get(offset).getId();
  }
}
