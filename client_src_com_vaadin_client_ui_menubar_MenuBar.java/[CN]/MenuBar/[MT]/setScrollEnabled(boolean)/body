{
  if (enabled) {
    if (vertical) {
      outer.getStyle().setOverflowY(Overflow.AUTO);
    }
 else {
      outer.getStyle().setOverflowX(Overflow.AUTO);
    }
  }
 else {
    if (vertical) {
      outer.getStyle().clearOverflowY();
    }
 else {
      outer.getStyle().clearOverflowX();
    }
  }
}
