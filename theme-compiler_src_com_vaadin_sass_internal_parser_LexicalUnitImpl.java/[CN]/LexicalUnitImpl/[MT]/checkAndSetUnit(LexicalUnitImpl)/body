{
  if (getLexicalUnitType() != SAC_INTEGER && getLexicalUnitType() != SAC_REAL && another.getLexicalUnitType() != SAC_INTEGER && another.getLexicalUnitType() != SAC_REAL && getLexicalUnitType() != another.getLexicalUnitType()) {
    throw new IncompatibleUnitsException(toString());
  }
  if (another.getLexicalUnitType() != SAC_INTEGER && another.getLexicalUnitType() != SAC_REAL) {
    setLexicalUnitType(another.getLexicalUnitType());
  }
  setNextLexicalUnit(another.getNextLexicalUnit());
}
