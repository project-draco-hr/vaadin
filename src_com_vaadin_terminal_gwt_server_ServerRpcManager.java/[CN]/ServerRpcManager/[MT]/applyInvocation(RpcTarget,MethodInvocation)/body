{
  try {
    Class<?> rpcInterfaceClass=Class.forName(invocation.getInterfaceName());
    RpcManager manager=target.getRpcManager(rpcInterfaceClass);
    if (manager != null) {
      manager.applyInvocation(invocation);
    }
 else {
      getLogger().log(Level.WARNING,"RPC call received for RpcTarget " + target.getClass().getName() + " ("+ invocation.getConnectorId()+ ") but the target has not registered any RPC interfaces");
    }
  }
 catch (  ClassNotFoundException e) {
    throw new RuntimeException("Class for RPC interface " + invocation.getInterfaceName() + " of the target "+ target+ " could not be found.");
  }
}
