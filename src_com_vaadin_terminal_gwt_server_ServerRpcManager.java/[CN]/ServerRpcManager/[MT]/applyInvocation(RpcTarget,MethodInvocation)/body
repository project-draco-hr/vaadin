{
  try {
    Class<?> rpcInterfaceClass=Class.forName(invocation.getInterfaceName());
    RpcManager manager=target.getRpcManager(rpcInterfaceClass);
    if (manager != null) {
      manager.applyInvocation(invocation);
    }
 else {
      throw new RuntimeException("RPC call to a target without an RPC manager.");
    }
  }
 catch (  ClassNotFoundException e) {
    throw new RuntimeException("No RPC manager registered for RPC interface " + invocation.getInterfaceName() + " of the target "+ target+ ".");
  }
}
