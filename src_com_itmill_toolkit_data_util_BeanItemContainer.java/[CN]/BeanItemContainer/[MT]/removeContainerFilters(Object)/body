{
  if (!filters.isEmpty()) {
    for (Iterator<Filter> iterator=filters.iterator(); iterator.hasNext(); ) {
      Filter f=iterator.next();
      if (f.propertyId.equals(propertyId)) {
        iterator.remove();
      }
    }
    for (    BeanItem item : beanToItem.values()) {
      removeValueChangeListener(item,propertyId);
    }
    filterAll();
  }
}
