{
  if (filters != null) {
    for (Iterator<Filter> iterator=filters.iterator(); iterator.hasNext(); ) {
      Filter f=iterator.next();
      if (f.propertyId.equals(propertyId)) {
        iterator.remove();
      }
    }
    list=(ArrayList<BT>)list.clone();
    for (    Filter f : filters) {
      filter(f);
    }
    fireItemSetChange();
  }
}
