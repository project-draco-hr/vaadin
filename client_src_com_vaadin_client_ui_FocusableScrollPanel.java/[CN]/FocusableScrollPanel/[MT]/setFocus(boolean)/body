{
  if (useFakeFocusElement()) {
    if (focus) {
      FocusImpl.getFocusImplForPanel().focus((com.google.gwt.user.client.Element)focusElement.cast());
    }
 else {
      FocusImpl.getFocusImplForPanel().blur((com.google.gwt.user.client.Element)focusElement.cast());
    }
  }
 else {
    super.setFocus(focus);
  }
}
