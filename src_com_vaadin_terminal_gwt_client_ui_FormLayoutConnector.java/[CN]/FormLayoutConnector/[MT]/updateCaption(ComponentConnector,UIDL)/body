{
  final Caption c=getWidget().table.widgetToCaption.get(component.getWidget());
  if (c != null) {
    c.updateCaption(uidl,component.getState(),component.isEnabled());
  }
  final ErrorFlag e=getWidget().table.widgetToError.get(component.getWidget());
  if (e != null) {
    e.updateFromUIDL(uidl,component);
  }
}
