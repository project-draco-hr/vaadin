{
  openTestURL();
  toggleColumnReordering();
  dragDefaultColumnHeader(0,2,10);
  assertNoColumnReorderEvent();
  toggleColumnReorderListener();
  dragDefaultColumnHeader(0,2,110);
  assertColumnReorderEvent(true);
  toggleColumnReorderListener();
  dragDefaultColumnHeader(0,3,10);
  assertNoColumnReorderEvent();
}
