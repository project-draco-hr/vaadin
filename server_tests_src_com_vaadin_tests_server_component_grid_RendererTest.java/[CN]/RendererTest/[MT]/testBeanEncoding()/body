{
  baz.setRenderer(renderer(),converter());
  bah.setRenderer(renderer(),converter());
  assertEquals("renderer(TestBean(42))",render(baz,new TestBean()));
  assertEquals("renderer(ExtendedBean(42, 3.14))",render(baz,new ExtendedBean()));
  assertEquals("renderer(ExtendedBean(42, 3.14))",render(bah,new ExtendedBean()));
}
