{
  Button b=new Button("Open subwindow",new Button.ClickListener(){
    public void buttonClick(    ClickEvent event){
      Window w=new Window("Subwindow");
      w.center();
      w.setHeight("200px");
      w.addComponent(getBigComponent());
      getMainWindow().addWindow(w);
    }
  }
);
  return b;
}
