{
  return new ServletDeploymentConfiguration(this,applicationProperties){
    @Override public String getStaticFileLocation(    WrappedRequest request){
      URIS uris=getApplicationRunnerURIs(WrappedHttpServletRequest.cast(request));
      String staticFilesPath=uris.staticFilesPath;
      if (staticFilesPath.equals("/")) {
        staticFilesPath="";
      }
      return staticFilesPath;
    }
  }
;
}
