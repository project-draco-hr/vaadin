{
  return new VaadinServletService(this,deploymentConfiguration){
    @Override public String getStaticFileLocation(    VaadinRequest request){
      URIS uris=getApplicationRunnerURIs(VaadinServletRequest.cast(request));
      String staticFilesPath=uris.staticFilesPath;
      if (staticFilesPath.equals("/")) {
        staticFilesPath="";
      }
      return staticFilesPath;
    }
  }
;
}
