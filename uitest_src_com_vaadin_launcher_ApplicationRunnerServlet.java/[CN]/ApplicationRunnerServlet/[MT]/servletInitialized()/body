{
  super.servletInitialized();
  getVaadinService().addVaadinSessionInitializationListener(new VaadinSessionInitializationListener(){
    @Override public void vaadinSessionInitialized(    VaadinSessionInitializeEvent event) throws ServiceException {
      onVaadinSessionStarted(event.getRequest(),event.getVaadinSession());
    }
  }
);
}
