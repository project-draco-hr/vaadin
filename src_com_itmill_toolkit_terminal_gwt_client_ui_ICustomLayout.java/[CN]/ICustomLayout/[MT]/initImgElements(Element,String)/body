{
  NodeList<com.google.gwt.dom.client.Element> nodeList=e.getElementsByTagName("IMG");
  for (int i=0; i < nodeList.getLength(); i++) {
    com.google.gwt.dom.client.ImageElement img=(ImageElement)nodeList.getItem(i);
    String src=img.getSrc();
    if (!(src.startsWith("/") || src.contains("://"))) {
      img.setSrc(srcPrefix + src);
    }
    DOM.sinkEvents((Element)img.cast(),Event.ONLOAD);
  }
}
