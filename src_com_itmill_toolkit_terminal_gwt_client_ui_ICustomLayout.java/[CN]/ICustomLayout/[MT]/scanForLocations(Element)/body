{
  String location=getLocation(elem);
  if (location != null) {
    locationToElement.put(location,elem);
    DOM.setInnerHTML(elem,"");
  }
 else {
    int len=DOM.getChildCount(elem);
    for (int i=0; i < len; i++) {
      System.out.print(i);
      scanForLocations(DOM.getChild(elem,i));
    }
  }
}
