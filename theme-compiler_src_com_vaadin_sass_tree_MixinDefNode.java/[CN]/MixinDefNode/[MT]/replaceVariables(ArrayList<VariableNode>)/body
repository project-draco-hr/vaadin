{
  for (  final VariableNode var : variables) {
    for (    final VariableNode arg : new ArrayList<VariableNode>(arglist)) {
      if (arg.getName().equals(var.getName())) {
        arglist.add(arglist.indexOf(arg),(VariableNode)DeepCopy.copy(var));
        arglist.remove(arg);
      }
    }
  }
}
