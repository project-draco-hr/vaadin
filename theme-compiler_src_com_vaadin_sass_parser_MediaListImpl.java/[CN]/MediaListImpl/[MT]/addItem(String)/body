{
  if (medium.equals("all")) {
    array[0]="all";
    current=1;
    return;
  }
  for (int i=0; i < current; i++) {
    if (medium.equals(array[i])) {
      return;
    }
  }
  if (current == array.length) {
    String[] old=array;
    array=new String[current + current];
    System.arraycopy(old,0,array,0,current);
  }
  array[current++]=medium;
}
