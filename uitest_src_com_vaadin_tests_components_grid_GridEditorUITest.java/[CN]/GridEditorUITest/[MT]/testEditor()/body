{
  setDebug(true);
  openTestURL();
  assertFalse("Sanity check",isElementPresent(PasswordFieldElement.class));
  openEditor(5);
  new Actions(getDriver()).sendKeys(Keys.ESCAPE).perform();
  openEditor(10);
  assertTrue("Edtor should be opened with a password field",isElementPresent(PasswordFieldElement.class));
  assertFalse("Notification was present",isElementPresent(NotificationElement.class));
}
