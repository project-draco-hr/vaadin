{
  String design="<vaadin-text-field " + "maxlength=5 columns=3 " + "placeholder=input value-change-mode=eager "+ "value-change-timeout=100 />";
  AbstractTextField tf=new TextField();
  tf.setMaxLength(5);
  tf.setColumns(3);
  tf.setPlaceholder("input");
  tf.setValueChangeMode(ValueChangeMode.EAGER);
  tf.setValueChangeTimeout(100);
  testRead(design,tf);
  testWrite(design,tf);
}
