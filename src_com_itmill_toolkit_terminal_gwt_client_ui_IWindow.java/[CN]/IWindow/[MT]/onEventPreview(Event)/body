{
  if (dragging) {
    onHeaderEvent(event);
    return false;
  }
 else   if (resizing) {
    onResizeEvent(event);
    return false;
  }
 else   if (modal) {
    final Element target=DOM.eventGetTarget(event);
    if (!DOM.isOrHasChild(getElement(),target)) {
      return false;
    }
  }
  return true;
}
