{
  if (dragging) {
    onDragEvent(event);
    return false;
  }
 else   if (resizing) {
    onResizeEvent(event);
    return false;
  }
 else   if (modal) {
    final Element target=event.getTarget().cast();
    if (!DOM.isOrHasChild(getElement(),target)) {
      return false;
    }
  }
  return true;
}
