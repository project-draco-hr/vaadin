{
  addOrRemoveTypedListener();
  addOrRemoveDefaultListener();
  addOrRemoveDefaultListener();
  assertTypedContextClickListener(1);
  addOrRemoveDefaultListener();
  addOrRemoveTypedListener();
  assertDefaultContextClickListener(3);
  addOrRemoveDefaultListener();
  assertNoContextClickHandler();
  addOrRemoveTypedListener();
  assertTypedContextClickListener(4);
}
