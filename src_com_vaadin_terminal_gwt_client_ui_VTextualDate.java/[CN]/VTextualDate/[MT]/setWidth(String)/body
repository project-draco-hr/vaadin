{
  if (!"".equals(newWidth) && (isUndefinedWidth() || !newWidth.equals(width))) {
    needLayout=true;
    width=newWidth;
    super.setWidth(width);
    iLayout();
    if (newWidth.indexOf("%") < 0) {
      needLayout=false;
    }
  }
 else {
    if ("".equals(newWidth) && !isUndefinedWidth()) {
      super.setWidth("");
      iLayout(true);
      width=null;
    }
  }
}
