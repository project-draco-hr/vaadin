{
  testedLayout.setMargin(((Boolean)marginTop.getValue()).booleanValue(),((Boolean)marginRight.getValue()).booleanValue(),((Boolean)marginBottom.getValue()).booleanValue(),((Boolean)marginLeft.getValue()).booleanValue());
  if (testedLayout instanceof OrderedLayout) {
    ((OrderedLayout)testedLayout).setSpacing(((Boolean)spacing.getValue()).booleanValue());
  }
 else   if (testedLayout instanceof GridLayout) {
    ((GridLayout)testedLayout).setSpacing(((Boolean)spacing.getValue()).booleanValue());
  }
}
