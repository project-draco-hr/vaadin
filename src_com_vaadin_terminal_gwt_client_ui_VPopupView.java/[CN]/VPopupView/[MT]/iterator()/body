{
  return new Iterator<Widget>(){
    int pos=0;
    public boolean hasNext(){
      return (pos == 0);
    }
    public Widget next(){
      if (pos != 0) {
        throw new NoSuchElementException();
      }
      pos++;
      return popup;
    }
    public void remove(){
      throw new UnsupportedOperationException();
    }
  }
;
}
