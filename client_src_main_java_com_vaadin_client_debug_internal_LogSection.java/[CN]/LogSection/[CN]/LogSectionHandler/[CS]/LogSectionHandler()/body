{
  setLevel(Level.ALL);
  setFormatter(new HtmlLogFormatter(true){
    @Override protected String getHtmlPrefix(    LogRecord event){
      return "";
    }
    @Override protected String getHtmlSuffix(    LogRecord event){
      return "";
    }
    @Override protected String getRecordInfo(    LogRecord event,    String newline){
      return "";
    }
  }
);
}
