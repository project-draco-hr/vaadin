{
  int focusedColumn=cellFocusRange.getStart();
  if (containerWithFocus != escalator.getBody()) {
    return;
  }
 else   if (!removed.contains(rowWithFocus)) {
    if (removed.getStart() > rowWithFocus) {
      return;
    }
    setCellFocus(rowWithFocus - removed.length(),focusedColumn,containerWithFocus);
  }
 else {
    if (containerWithFocus.getRowCount() > removed.getEnd()) {
      setCellFocus(removed.getStart(),focusedColumn,containerWithFocus);
    }
 else     if (removed.getStart() > 0) {
      setCellFocus(removed.getStart() - 1,focusedColumn,containerWithFocus);
    }
 else {
      if (escalator.getHeader().getRowCount() > 0) {
        setCellFocus(lastFocusedHeaderRow,focusedColumn,escalator.getHeader());
      }
 else       if (escalator.getFooter().getRowCount() > 0) {
        setCellFocus(lastFocusedFooterRow,focusedColumn,escalator.getFooter());
      }
    }
  }
}
