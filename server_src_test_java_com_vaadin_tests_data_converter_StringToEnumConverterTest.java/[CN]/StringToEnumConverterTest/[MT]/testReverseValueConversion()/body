{
  Assert.assertEquals("Value1",reverseConverter.convertToModel(FooEnum.VALUE1,String.class,null));
  Assert.assertEquals("Some value",reverseConverter.convertToModel(FooEnum.SOME_VALUE,String.class,null));
  Assert.assertEquals("Foo bar baz",reverseConverter.convertToModel(FooEnum.FOO_BAR_BAZ,String.class,null));
  Assert.assertEquals("Bar",reverseConverter.convertToModel(FooEnum.Bar,String.class,null));
  Assert.assertEquals("Nonstandardcase",reverseConverter.convertToModel(FooEnum.nonStandardCase,String.class,null));
  Assert.assertEquals("_hugh",reverseConverter.convertToModel(FooEnum._HUGH,String.class,null));
}
