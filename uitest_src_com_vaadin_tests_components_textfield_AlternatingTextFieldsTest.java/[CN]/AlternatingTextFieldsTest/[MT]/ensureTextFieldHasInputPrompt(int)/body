{
  if (index == 0) {
    assertEquals("Incorrect or missing prompt,",AlternatingTextFields.FIRST_TEXTFIELD_INPUT_PROMPT,textfields.get(index).getValue());
  }
 else {
    assertEquals("Incorrect or missing prompt,",AlternatingTextFields.SECOND_TEXTFIELD_INPUT_PROMPT,textfields.get(index).getValue());
  }
}
