{
  openTestURL();
  $(CheckBoxElement.class).first().click();
  assertState($(LegacyDateFieldElement.class).id("DATEFIELD_ENABLED"),true,true);
  assertState($(LegacyDateFieldElement.class).id("DATEFIELD_DISABLED"),false,false);
  assertState($(LegacyPopupDateFieldElement.class).id("DATEFIELD_ENABLED_ENABLED"),true,true);
  assertState($(LegacyPopupDateFieldElement.class).id("DATEFIELD_ENABLED_DISABLED"),true,false);
  assertState($(LegacyPopupDateFieldElement.class).id("DATEFIELD_DISABLED_ENABLED"),false,false);
  assertState($(LegacyPopupDateFieldElement.class).id("DATEFIELD_DISABLED_DISABLED"),false,false);
}
