{
  main.removeAllComponents();
  for (int i=0; i < 10; i++) {
    final ExpandLayout el=new ExpandLayout(OrderedLayout.ORIENTATION_HORIZONTAL);
    for (int j=0; j < 10; j++) {
      final Label l=new Label("label" + i + ":"+ j);
      el.addComponent(l);
    }
    if (i > 0) {
      el.getSize().setHeight(1,Size.UNITS_EM);
    }
    main.addComponent(el);
  }
}
