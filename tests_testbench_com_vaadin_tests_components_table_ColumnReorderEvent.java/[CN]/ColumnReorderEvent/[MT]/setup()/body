{
  HorizontalLayout widths=new HorizontalLayout();
  widths.setSpacing(true);
  widths.setWidth("50%");
  order.setCaption("Column 1 width");
  widths.addComponent(order);
  addComponent(widths);
  Table table1=initTable();
  addComponent(table1);
  order.setValue(aToString(table1.getVisibleColumns()));
}
