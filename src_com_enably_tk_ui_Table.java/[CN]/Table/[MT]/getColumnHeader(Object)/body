{
  if (getColumnHeaderMode() == COLUMN_HEADER_MODE_HIDDEN)   return null;
  String header=(String)this.columnHeaders.get(propertyId);
  if ((header == null && this.getColumnHeaderMode() == COLUMN_HEADER_MODE_EXPLICIT_DEFAULTS_ID) || this.getColumnHeaderMode() == COLUMN_HEADER_MODE_ID) {
    header=propertyId.toString();
  }
  return header;
}
