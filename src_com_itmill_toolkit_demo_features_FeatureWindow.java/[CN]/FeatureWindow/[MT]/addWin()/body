{
  propertyPanel.commit();
  getApplication().getMainWindow().addWindow(demoWindow);
  demoWindow.removeAllComponents();
  demoWindow.addComponent(new Label("<br /><br />This is a new window created by " + "<em>Add window</em>" + " button's event.<br /><br />You may simply"+ " close this window or select "+ "<em>Remove window</em> from the Feature Browser window.",Label.CONTENT_XHTML));
  windowProperties.getField("name").setEnabled(false);
  windowProperties.getField("name").setReadOnly(true);
  demoWindow.setVisible(true);
  updateWinStatus();
}
