{
  openTestURL();
  selectMenuPath("Component","State","Selection mode","none");
  assertTrue("First column was selection column",getGridElement().getCell(0,0).getText().equals("(0, 0)"));
  selectMenuPath("Component","State","Selection mode","multi");
  assertTrue("First column was not selection column",getGridElement().getCell(0,1).getText().equals("(0, 0)"));
}
