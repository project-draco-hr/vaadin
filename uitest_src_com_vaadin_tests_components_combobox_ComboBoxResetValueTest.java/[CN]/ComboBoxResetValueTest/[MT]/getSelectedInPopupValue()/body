{
  try {
    WebElement selectedSpan=driver.findElement(By.cssSelector(".gwt-MenuItem-selected span"));
    return selectedSpan.getText();
  }
 catch (  NoSuchElementException e) {
    return null;
  }
catch (  WebDriverException e) {
    if (e.getMessage() != null && e.getMessage().contains("Unable to find element")) {
      return null;
    }
    throw e;
  }
}
