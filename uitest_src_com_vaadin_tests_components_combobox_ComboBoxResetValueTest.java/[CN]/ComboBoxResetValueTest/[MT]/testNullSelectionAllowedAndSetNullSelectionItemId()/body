{
  openTestURL();
  ComboBoxElement comboBoxWebElement=$(ComboBoxElement.class).id(ComboBoxResetValue.NULL_SELECTION_ALLOWED_WITH_SET_NULL_SELECTION_ITEM_ID);
  clickResetButton();
  openPopup(comboBoxWebElement);
  assertEquals("There should be selected: " + ComboBoxResetValue.EMPTY_VALUE,ComboBoxResetValue.EMPTY_VALUE,getSelectedInPopupValue());
}
