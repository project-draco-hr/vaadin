{
  openTestURL();
  ComboBoxElement comboBoxWebElement=$(ComboBoxElement.class).id(ComboBoxResetValue.NULL_SELECTION_ALLOWED_WITH_SET_NULL_SELECTION_ITEM_ID);
  clickResetButton();
  printFilterAndRemoveIt(getComboBoxInput(comboBoxWebElement));
  assertEquals("There should be " + ComboBoxResetValue.EMPTY_VALUE,ComboBoxResetValue.EMPTY_VALUE,getComboBoxValue(comboBoxWebElement));
}
