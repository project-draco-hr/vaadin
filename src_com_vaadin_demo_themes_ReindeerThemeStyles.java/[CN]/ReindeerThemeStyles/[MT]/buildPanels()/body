{
  Base l=new Base("Panels");
  Panel p=new Panel("Normal panel");
  p.setHeight("100px");
  p.addComponent(new Label("Panel content"));
  Panel p2=new Panel("Light style panel");
  p2.setStyleName("light");
  p2.addComponent(new Label("Panel content"));
  l.addComponent(p);
  l.addComponent(p2);
  return l;
}
