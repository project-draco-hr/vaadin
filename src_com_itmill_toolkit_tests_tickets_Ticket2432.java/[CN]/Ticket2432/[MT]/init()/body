{
  Window w=new Window();
  setMainWindow(w);
  w.getLayout().setSizeFull();
  ((SpacingHandler)w.getLayout()).setSpacing(true);
  Layout layout=new GridLayout(3,3);
  populateLayout(layout);
  w.addComponent(layout);
  layout=new HorizontalLayout();
  populateLayout(layout);
  w.addComponent(layout);
}
