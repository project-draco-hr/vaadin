{
  LegacyTextField tf1=new LegacyTextField("Tab index 0");
  tf1.setTabIndex(0);
  LegacyTextField tf2=new LegacyTextField("Tab index -1, focused initially");
  tf2.setTabIndex(-1);
  tf2.focus();
  addComponent(tf1);
  addComponent(tf2);
  addComponent(createButton(1));
  addComponent(createButton(5));
  addComponent(createNativeButton(3));
  addComponent(createButton(4));
  addComponent(createNativeButton(2));
}
