{
  colspan=cellUidl.hasAttribute("w") ? cellUidl.getIntAttribute("w") : 1;
  rowspan=cellUidl.hasAttribute("h") ? cellUidl.getIntAttribute("h") : 1;
  for (int i=0; i < colspan; i++) {
    for (int j=0; j < rowspan; j++) {
      if (i > 0 || j > 0) {
        cells[col + i][row + j]=null;
      }
    }
  }
  UIDL childUidl=cellUidl.getChildUIDL(0);
  hasContent=childUidl != null;
  if (hasContent) {
    ComponentConnector childConnector=client.getPaintable(childUidl);
    if (slot == null || slot.getChild() != childConnector) {
      slot=new ComponentConnectorLayoutSlot(CLASSNAME,childConnector,getConnector());
      if (childConnector.isRelativeWidth()) {
        slot.getWrapperElement().getStyle().setWidth(100,Unit.PCT);
      }
      Element slotWrapper=slot.getWrapperElement();
      getElement().appendChild(slotWrapper);
      Widget widget=childConnector.getWidget();
      insert(widget,slotWrapper,getWidgetCount(),false);
      Cell oldCell=widgetToCell.put(widget,this);
      if (oldCell != null) {
        oldCell.slot.getWrapperElement().removeFromParent();
        oldCell.slot=null;
      }
    }
    childConnector.updateFromUIDL(childUidl,client);
  }
}
