{
  Element rootElement=getElement();
  while (element != null && element != rootElement) {
    Paintable paintable=client.getPaintable(element);
    if (paintable != null) {
      Cell cell=paintableToCell.get(paintable);
      if (cell != null) {
        return paintable;
      }
    }
    element=DOM.getParent(element);
  }
  return null;
}
