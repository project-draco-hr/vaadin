{
  if (height != this.height || this.undefinedHeight != undefinedHeight) {
    this.undefinedHeight=undefinedHeight;
    this.height=height;
    if (rendering) {
      sizeChangedDuringRendering=true;
    }
 else {
      expandRows();
      layoutCells();
      for (      Widget w : widgetToCell.keySet()) {
        client.handleComponentRelativeSize(w);
      }
    }
  }
}
