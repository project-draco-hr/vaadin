{
  if (tagName == null)   throw new NullPointerException();
  if (this.closed)   throw new PaintException("Attempted to write to a closed PaintTarget.");
  ensureClosedTag();
  mOpenTags.push(tagName);
  tagBuffer=new StringBuffer(TAG_BUFFER_DEFAULT_SIZE);
  tagBuffer.append("<" + tagName);
  mTagArgumentListOpen=true;
}
