{
  VerticalLayout pl=new VerticalLayout();
  pl.setMargin(true);
  Panel p=new Panel("OrderedLayout",pl);
  p.setWidth("500px");
  p.setHeight("500px");
  pl.setWidth("100%");
  layout.addComponent(p);
  AbstractOrderedLayout ol=new VerticalLayout();
  ol.setMargin(true);
  ol.setWidth("100%");
  addButtons(ol);
  pl.addComponent(ol);
  ol=new HorizontalLayout();
  ol.setMargin(true);
  ol.setHeight("200px");
  addButtons(ol);
  pl.addComponent(ol);
}
