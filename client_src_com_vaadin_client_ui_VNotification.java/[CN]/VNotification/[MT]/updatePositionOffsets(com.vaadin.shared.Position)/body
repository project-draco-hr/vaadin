{
  final Element el=getElement();
  el.getStyle().clearTop();
  el.getStyle().clearLeft();
switch (position) {
case MIDDLE_LEFT:
case MIDDLE_RIGHT:
    center();
  el.getStyle().clearLeft();
break;
case TOP_CENTER:
case BOTTOM_CENTER:
center();
el.getStyle().clearTop();
break;
case MIDDLE_CENTER:
center();
break;
}
}
