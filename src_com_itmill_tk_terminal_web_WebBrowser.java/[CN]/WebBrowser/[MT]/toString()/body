{
  String localeString="[";
  for (Iterator i=this.locales.iterator(); i.hasNext(); localeString+=",") {
    localeString+=((Locale)i.next()).toString();
  }
  localeString+="]";
  return "Browser:" + this.browserApplication + ", "+ "Locales:"+ localeString+ ", "+ "Frames:"+ this.frameSupport+ ", "+ "JavaScript:"+ this.javaScriptVersion+ ", "+ "Java: "+ this.javaEnabled+ ", "+ "Markup:"+ this.markupVersion+ ", "+ "Height:"+ this.screenHeight+ ", "+ "Width:"+ this.screenWidth+ ", ClientCheck:"+ this.performClientCheck+ ", ClientCheckDone:"+ this.clientSideChecked;
}
