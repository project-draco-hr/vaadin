{
  String design="<v-tree-table>" + "<table>" + "<colgroup><col property-id=''></colgroup>"+ "<tbody>"+ "  <tr item-id='1'><td></tr>"+ "    <tr depth=1 item-id='1.1'><td></tr>"+ "    <tr depth=1 item-id='1.2'><td></tr>"+ "      <tr depth=2 item-id='1.2.1'><td></tr>"+ "        <tr depth=3 item-id='1.2.1.1'><td></tr>"+ "      <tr depth=2 item-id='1.2.2'><td></tr>"+ "  <tr item-id='2'><td></tr>"+ "    <tr depth=1 item-id='2.1'><td></tr>"+ "</tbody>"+ "</table>"+ "</v-tree-table>";
  TreeTable table=getTable();
  table.addContainerProperty("",String.class,"");
  table.addItem("1");
  table.addItem("1.1");
  table.setParent("1.1","1");
  table.addItem("1.2");
  table.setParent("1.2","1");
  table.addItem("1.2.1");
  table.setParent("1.2.1","1.2");
  table.addItem("1.2.1.1");
  table.setParent("1.2.1.1","1.2.1");
  table.addItem("1.2.2");
  table.setParent("1.2.2","1.2");
  table.addItem("2");
  table.addItem("2.1");
  table.setParent("2.1","2");
  testRead(design,table);
  testWrite(design,table,true);
}
