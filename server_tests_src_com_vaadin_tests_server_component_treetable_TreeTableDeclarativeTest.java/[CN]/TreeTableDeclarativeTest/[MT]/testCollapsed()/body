{
  String design="<v-tree-table>" + "  <table>" + "    <colgroup><col property-id=''></colgroup>"+ "    <tbody>"+ "      <tr item-id='1' collapsed=false><td></tr>"+ "        <tr depth=1 item-id='1.1'><td></tr>"+ "          <tr depth=2 item-id='1.1.1'><td></tr>"+ "    </tbody>"+ "  </table>"+ "</v-tree-table>";
  TreeTable table=getTable();
  table.addContainerProperty("",String.class,"");
  table.addItem("1");
  table.setCollapsed("1",false);
  table.addItem("1.1");
  table.setParent("1.1","1");
  table.addItem("1.1.1");
  table.setParent("1.1.1","1.1");
  testRead(design,table);
  testWrite(design,table,true);
}
