{
  final StringBuffer sb=new StringBuffer();
  for (final Iterator it=x.keySet().iterator(); it.hasNext(); ) {
    final String tag=(String)it.next();
    sb.append("<");
    sb.append(tag);
    sb.append(">");
    sb.append(x.get(tag).isString().stringValue());
    sb.append("</");
    sb.append(tag);
    sb.append(">");
  }
  return sb.toString();
}
