{
  menu.selectItem(newSelectedItem);
  String text=newSelectedItem != null ? newSelectedItem.getText() : "";
  FilterSelectSuggestion suggestion=(FilterSelectSuggestion)newSelectedItem.getCommand();
  setSelectedItemIcon(suggestion.getIconUri());
  tb.setText(text);
  tb.setSelectionRange(lastFilter.length(),text.length() - lastFilter.length());
  menu.updateKeyboardSelectedItem();
}
