{
  Class<? extends ComponentConnector> classType=resolveWidgetType(tag,conf);
  if (classType == null || classType == UnknownComponentConnector.class) {
    String serverSideName=conf.getUnknownServerClassNameByEncodedTagName(tag);
    UnknownComponentConnector c=GWT.create(UnknownComponentConnector.class);
    c.setServerSideClassName(serverSideName);
    return c;
  }
 else {
    return widgetMap.instantiate(classType);
  }
}
