{
  Object value=getValue();
  if (isMultiSelect() && (value instanceof Collection)) {
    Collection<?> collection=(Collection<?>)value;
    for (    Object id : collection) {
      if (!containsId(id)) {
        unselect(id);
      }
    }
  }
 else   if (!containsId(value)) {
    unselect(value);
  }
}
