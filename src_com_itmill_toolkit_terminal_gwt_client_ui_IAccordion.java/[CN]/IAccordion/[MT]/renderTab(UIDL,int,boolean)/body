{
  StackItem item;
  if (stack.size() <= index) {
    item=new StackItem(tabUidl);
    if (stack.size() == 0) {
      item.addStyleDependentName("first");
    }
    stack.add(item);
    add(item,getElement());
  }
 else {
    item=(StackItem)stack.get(index);
    item.updateCaption(tabUidl);
  }
  if (selected) {
    item.open();
    item.setContent(tabUidl.getChildUIDL(0));
  }
 else   if (tabUidl.getChildCount() > 0) {
    item.setContent(tabUidl.getChildUIDL(0));
  }
}
