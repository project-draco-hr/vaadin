{
  if (height == -1) {
    super.setHeight("");
    DOM.setStyleAttribute(content,"height","0px");
  }
 else {
    super.setHeight((height + getCaptionHeight()) + "px");
    DOM.setStyleAttribute(content,"height",height + "px");
    DOM.setStyleAttribute(content,"top",getCaptionHeight() + "px");
  }
}
