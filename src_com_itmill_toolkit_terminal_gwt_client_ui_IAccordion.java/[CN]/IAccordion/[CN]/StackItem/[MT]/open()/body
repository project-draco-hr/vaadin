{
  open=true;
  if (getPaintable() != null) {
    remove(getPaintable());
  }
  DOM.setStyleAttribute(content,"visibility","");
  DOM.setStyleAttribute(content,"position","");
  DOM.setStyleAttribute(content,"top","");
  addStyleDependentName("open");
  if (getPaintable() != null) {
    add(getPaintable(),content);
  }
}
