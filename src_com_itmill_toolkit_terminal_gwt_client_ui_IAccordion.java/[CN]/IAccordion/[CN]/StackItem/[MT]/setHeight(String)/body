{
  super.setHeight(height);
  if (!"".equals(height)) {
    int offsetHeight=getOffsetHeight();
    int captionHeight=DOM.getElementPropertyInt(captionNode,"offsetHeight");
    int contentSpace=offsetHeight - captionHeight;
    if (contentSpace > 0) {
      DOM.setStyleAttribute(content,"height",contentSpace + "px");
    }
  }
 else {
    DOM.setStyleAttribute(content,"height","");
  }
}
