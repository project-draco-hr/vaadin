{
  TooltipInfo info=null;
  if (element != getWidget().getElement()) {
    CustomMenuItem item=getWidget().getMenuItemWithElement((com.google.gwt.user.client.Element)element);
    if (item != null) {
      info=item.getTooltip();
    }
  }
  if (info == null) {
    info=super.getTooltipInfo(element);
  }
  return info;
}
