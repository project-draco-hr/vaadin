{
  Filterable filterable=(Container.Filterable)table.getContainerDataSource();
  Item i=table.addItem("abc");
  String res="";
  if (i == null) {
    res="FAILED";
  }
 else {
    res="OK!";
  }
  getMainWindow().showNotification("Tried to add item 'abc', " + res);
  filterable.addContainerFilter(new SimpleStringFilter(PROPERTY_1,"Row",true,false));
}
