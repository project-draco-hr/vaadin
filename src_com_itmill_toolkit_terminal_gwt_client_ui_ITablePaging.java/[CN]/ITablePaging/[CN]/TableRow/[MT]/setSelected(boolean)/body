{
  this.selected=sel;
  if (selected) {
    selectedRowKeys.add(key);
    DOM.setStyleAttribute(ITablePaging.this.tBody.getRowFormatter().getElement(rowIndex),"background","yellow");
  }
 else {
    selectedRowKeys.remove(key);
    DOM.setStyleAttribute(ITablePaging.this.tBody.getRowFormatter().getElement(rowIndex),"background","transparent");
  }
}
