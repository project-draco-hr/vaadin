{
  Iterator it=uidl.getChildIterator();
  int curRowIndex=1;
  while (it.hasNext()) {
    UIDL rowUidl=(UIDL)it.next();
    TableRow row=new TableRow(curRowIndex,String.valueOf(rowUidl.getIntAttribute("key")),rowUidl.hasAttribute("selected"));
    int colIndex=0;
    if (rowHeaders) {
      tBody.setWidget(curRowIndex,colIndex,new BodyCell(row,rowUidl.getStringAttribute("caption")));
      colIndex++;
    }
    Iterator cells=rowUidl.getChildIterator();
    while (cells.hasNext()) {
      Object cell=cells.next();
      if (cell instanceof String) {
        tBody.setWidget(curRowIndex,colIndex,new BodyCell(row,(String)cell));
      }
 else {
        Widget cellContent=client.getWidget((UIDL)cell);
        BodyCell bodyCell=new BodyCell(row);
        bodyCell.setWidget(cellContent);
        tBody.setWidget(curRowIndex,colIndex,bodyCell);
      }
      colIndex++;
    }
    curRowIndex++;
  }
}
