{
  Window w=new Window("New child window");
  Label desc=new Label("This is a new child window with a preset" + " width, height and position. Resizing has also been" + " enabled for this window.");
  w.addComponent(desc);
  w.addListener(this);
  w.setPositionX(300);
  w.setPositionY(300);
  w.setWidth(300,UNITS_PIXELS);
  w.setHeight(300,UNITS_PIXELS);
  w.setResizable(true);
  getApplication().getMainWindow().addWindow(w);
}
