{
  String widgetset=getDeploymentConfiguration().getApplicationOrSystemProperty(PARAMETER_WIDGETSET,null);
  if (widgetset == null) {
    widgetset=VaadinPortletRequest.cast(request).getPortalProperty(PORTAL_PARAMETER_VAADIN_WIDGETSET);
  }
  if (widgetset == null) {
    widgetset=DEFAULT_WIDGETSET;
  }
  return widgetset;
}
