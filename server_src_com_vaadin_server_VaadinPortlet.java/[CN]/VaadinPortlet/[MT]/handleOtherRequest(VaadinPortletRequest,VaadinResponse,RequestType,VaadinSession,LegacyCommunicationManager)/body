{
  if (requestType == RequestType.APP || requestType == RequestType.RENDER) {
    if (!new SessionRequestHandler().handleRequest(vaadinSession,request,response)) {
      response.sendError(HttpServletResponse.SC_NOT_FOUND,"Not found");
    }
  }
 else   if (requestType == RequestType.EVENT) {
  }
 else   if (requestType == RequestType.ACTION) {
  }
 else {
    throw new IllegalStateException("handleRequest() without anything to do - should never happen!");
  }
}
