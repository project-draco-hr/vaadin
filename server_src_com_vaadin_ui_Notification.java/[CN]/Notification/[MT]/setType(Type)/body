{
  styleName=type.getStyle();
switch (type) {
case WARNING_MESSAGE:
    delayMsec=1500;
  break;
case ERROR_MESSAGE:
delayMsec=-1;
break;
case TRAY_NOTIFICATION:
delayMsec=3000;
position=Position.BOTTOM_RIGHT;
break;
case ASSISTIVE_NOTIFICATION:
delayMsec=3000;
position=Position.ASSISTIVE;
break;
case HUMANIZED_MESSAGE:
default :
break;
}
}
