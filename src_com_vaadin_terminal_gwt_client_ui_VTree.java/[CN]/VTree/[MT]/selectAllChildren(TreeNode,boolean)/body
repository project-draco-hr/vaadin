{
  if (includeRootNode) {
    node.setSelected(true);
    selectedIds.add(node.key);
  }
  for (  TreeNode child : node.getChildren()) {
    if (!child.isLeaf() && child.getState()) {
      selectAllChildren(child,true);
    }
 else {
      child.setSelected(true);
      selectedIds.add(child.key);
    }
  }
}
