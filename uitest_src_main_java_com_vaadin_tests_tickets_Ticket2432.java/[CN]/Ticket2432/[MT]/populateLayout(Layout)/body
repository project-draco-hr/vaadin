{
  layout.setSizeFull();
  for (int i=0; i < 9; i++) {
    Label l=new Label("M");
    Alignment a=alignments[i];
    l.setCaption(a.getHorizontalAlignment() + " " + a.getVerticalAlignment()+ " "+ a.getBitMask());
    layout.addComponent(l);
    ((AlignmentHandler)layout).setComponentAlignment(l,a);
  }
}
