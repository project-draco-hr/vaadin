{
  disableContentsChangeEvents();
  final Object itemId=super.addItem();
  if (itemId == null) {
    return null;
  }
  if (!roots.contains(itemId)) {
    roots.add(itemId);
    if (filteredRoots != null) {
      if (passesFilters(itemId)) {
        filteredRoots.add(itemId);
      }
    }
  }
  enableAndFireContentsChangeEvents();
  return itemId;
}
