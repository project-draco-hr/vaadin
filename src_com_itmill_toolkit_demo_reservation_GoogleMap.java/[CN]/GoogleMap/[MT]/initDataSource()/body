{
  this.dataSource=new IndexedContainer();
  this.dataSource.addContainerProperty(this.itemMarkerHtmlPropertyId,String.class,null);
  this.dataSource.addContainerProperty(this.itemMarkerXPropertyId,Float.class,new Float(0));
  this.dataSource.addContainerProperty(this.itemMarkerYPropertyId,Float.class,new Float(0));
}
