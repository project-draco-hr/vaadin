{
  BeanItem beanItem=new BeanItem(value);
  componentEditor.setItemDataSource(beanItem,Arrays.asList(new String[]{"width","widthUnits","height","heightUnits","caption","styleName"}));
  beanItem=new BeanItem(l.getPosition(value));
  String c="Component properties for " + value.getClass().getSimpleName();
  if (value instanceof Label) {
    c+="(" + ((Label)value).getValue() + ")";
  }
  componentEditor.setCaption(c);
  positionEditor.setItemDataSource(beanItem);
}
