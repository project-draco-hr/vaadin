{
  if (this == obj) {
    return true;
  }
 else   if (obj == null) {
    return false;
  }
 else   if (getClass() != obj.getClass()) {
    return false;
  }
  SortOrder other=(SortOrder)obj;
  if (direction != other.direction) {
    return false;
  }
 else   if (!propertyId.equals(other.propertyId)) {
    return false;
  }
  return true;
}
