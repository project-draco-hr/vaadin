{
  Table t=new Table();
  addComponent(t);
  t.addContainerProperty("Name",LegacyTextField.class,null);
  for (int i=0; i < 3; i++) {
    Item item=t.addItem(i);
    LegacyTextField tf=new LegacyTextField("",String.valueOf(i + 1));
    tf.setColumns(10);
    item.getItemProperty("Name").setValue(tf);
  }
}
