{
  if (align != c) {
switch (c) {
case ALIGN_CENTER:
      captionContainer.getStyle().setTextAlign(TextAlign.CENTER);
    break;
case ALIGN_RIGHT:
  captionContainer.getStyle().setTextAlign(TextAlign.RIGHT);
break;
default :
captionContainer.getStyle().setTextAlign(TextAlign.LEFT);
break;
}
}
align=c;
}
