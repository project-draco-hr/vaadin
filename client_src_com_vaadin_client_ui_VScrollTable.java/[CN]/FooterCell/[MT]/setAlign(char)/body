{
  if (align != c) {
switch (c) {
case ALIGN_CENTER:
      DOM.setStyleAttribute(captionContainer,"textAlign","center");
    break;
case ALIGN_RIGHT:
  DOM.setStyleAttribute(captionContainer,"textAlign","right");
break;
default :
DOM.setStyleAttribute(captionContainer,"textAlign","left");
break;
}
}
align=c;
}
