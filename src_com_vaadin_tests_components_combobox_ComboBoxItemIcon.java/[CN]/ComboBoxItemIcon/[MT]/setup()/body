{
  ComboBox cb=new ComboBox();
  cb.addContainerProperty("icon",Resource.class,null);
  cb.setItemIconPropertyId("icon");
  getLayout().addComponent(cb);
  Item item=cb.addItem("FI");
  item.getItemProperty("icon").setValue(new ClassResource("fi.gif",ComboBoxItemIcon.this));
  item=cb.addItem("SE");
  item.getItemProperty("icon").setValue(new ClassResource("se.gif",ComboBoxItemIcon.this));
}
