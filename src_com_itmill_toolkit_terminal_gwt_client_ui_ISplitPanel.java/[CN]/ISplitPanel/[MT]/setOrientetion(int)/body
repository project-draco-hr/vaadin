{
  this.orientation=orientation;
  if (orientation == ORIENTATION_HORIZONTAL) {
    this.sph=new HorizontalSplitPanel();
    this.setWidget(sph);
    if (spv != null) {
      this.spv=null;
    }
  }
 else {
    this.spv=new VerticalSplitPanel();
    this.setWidget(spv);
    if (sph != null) {
      this.sph=null;
    }
  }
}
