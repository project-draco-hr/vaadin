{
  initProperties(table);
  final String[] sp=new String[]{"Fox","Dog","Cat","Moose","Penguin","Cow"};
  final String[] ty=new String[]{"Quick","Lazy","Sleepy","Fidgety","Crazy","Kewl"};
  final String[] ki=new String[]{"Jumping","Walking","Sleeping","Skipping","Dancing"};
  Random r=new Random(5);
  for (int i=0; i < 100; i++) {
    final String s=sp[(int)(r.nextDouble() * sp.length)];
    final String t=ty[(int)(r.nextDouble() * ty.length)];
    final String k=ki[(int)(r.nextDouble() * ki.length)];
    table.addItem(new Object[]{s,t,k,Boolean.FALSE},new Integer(i));
  }
}
