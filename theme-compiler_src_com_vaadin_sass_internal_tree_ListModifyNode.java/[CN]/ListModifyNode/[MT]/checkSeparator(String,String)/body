{
  String lowerCase="";
  if (separator == null || (lowerCase=separator.toLowerCase()).equals("auto")) {
    if (list.contains(",")) {
      this.separator=",";
    }
  }
 else   if (lowerCase.equals("comma")) {
    this.separator=",";
  }
 else   if (lowerCase.equals("space")) {
    this.separator=" ";
  }
}
