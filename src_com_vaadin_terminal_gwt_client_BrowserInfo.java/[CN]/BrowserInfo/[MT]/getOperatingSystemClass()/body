{
  if (browserDetails.isAndroid()) {
    return OS_ANDROID;
  }
 else   if (browserDetails.isIOS()) {
    return OS_IOS;
  }
 else   if (browserDetails.isWindows()) {
    return OS_WINDOWS;
  }
 else   if (browserDetails.isLinux()) {
    return OS_LINUX;
  }
 else   if (browserDetails.isMacOSX()) {
    return OS_MACOSX;
  }
  return null;
}
