{
  HorizontalLayout hl=new HorizontalLayout();
  hl.setSpacing(true);
  hl.setWidth("100%");
  Label l=new Label();
  l.setCaption("POTUS Database");
  l.setSizeUndefined();
  Button b=new Button("Add new");
  hl.addComponents(l,b);
  b.setStyleName("primary");
  hl.setExpandRatio(b,1);
  addComponent(hl);
}
