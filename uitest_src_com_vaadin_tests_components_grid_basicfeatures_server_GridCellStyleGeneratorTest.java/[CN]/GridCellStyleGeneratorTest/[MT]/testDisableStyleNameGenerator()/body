{
  openTestURL();
  selectStyleNameGenerator("Combined");
  GridRowElement row2=getGridElement().getRow(2);
  GridCellElement cell3_2=getGridElement().getCell(3,2);
  Assert.assertTrue(row2.getAttribute("class").contains("v-grid-row-row2"));
  Assert.assertTrue(cell3_2.getAttribute("class").contains("v-grid-cell-content-Column_2"));
  selectStyleNameGenerator("None");
  Assert.assertFalse(row2.getAttribute("class").contains("v-grid-row-row2"));
  Assert.assertFalse(cell3_2.getAttribute("class").contains("v-grid-cell-content-Column_2"));
}
