{
  super.setUp();
  servlet=new ApplicationServlet();
  Field f=AbstractApplicationServlet.class.getDeclaredField("applicationProperties");
  f.setAccessible(true);
  f.set(servlet,new Properties());
  getStaticFilesLocationMethod=AbstractApplicationServlet.class.getDeclaredMethod("getStaticFilesLocation",new Class[]{javax.servlet.http.HttpServletRequest.class});
  getStaticFilesLocationMethod.setAccessible(true);
}
