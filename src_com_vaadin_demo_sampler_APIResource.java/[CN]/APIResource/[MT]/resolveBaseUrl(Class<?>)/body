{
  String name=clazz.getName();
  if (name.startsWith("javax.servlet.")) {
    return SERVLET_BASE;
  }
 else   if (name.startsWith("javax.portlet.")) {
    return PORTLET_BASE;
  }
 else   if (name.startsWith("java.") || name.startsWith("javax.")) {
    return JAVA_BASE;
  }
  return VAADIN_BASE;
}
