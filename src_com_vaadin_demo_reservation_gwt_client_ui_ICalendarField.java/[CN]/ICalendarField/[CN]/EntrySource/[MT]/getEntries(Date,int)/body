{
  final ArrayList res=new ArrayList();
  if (date == null) {
    return res;
  }
  final List entries=(List)dates.get(date.getYear() + "" + date.getMonth()+ ""+ date.getDate());
  if (entries == null) {
    return res;
  }
  for (final Iterator it=entries.iterator(); it.hasNext(); ) {
    final CalendarEntry item=(CalendarEntry)it.next();
    if (DateTimeService.isInRange(date,item.getStart(),item.getEnd(),resolution)) {
      res.add(item);
    }
  }
  return res;
}
