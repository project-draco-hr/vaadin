{
  openTestURL();
  WebElement nextButton=driver.findElement(By.id(BackspaceKeyWithModalOpened.BTN_NEXT_ID));
  nextButton.click();
  WebElement openModalButton=driver.findElement(By.id(BackspaceKeyWithModalOpened.BTN_OPEN_MODAL_ID));
  openModalButton.click();
  new Actions(getDriver()).sendKeys(Keys.BACK_SPACE).perform();
  WebElement textField=driver.findElement(By.id(BackspaceKeyWithModalOpened.TEXT_FIELD_IN_MODAL));
  textField.sendKeys("textt");
  textField.sendKeys(Keys.BACK_SPACE);
  compareScreen(getScreenshotBaseName());
}
