{
  ITEMCLASS newItem=null;
  if (previousItemId == null) {
    newItem=internalAddAt(0,newItemId,item);
  }
 else   if (containsId(previousItemId)) {
    newItem=internalAddAt(internalIndexOf(previousItemId) + 1,newItemId,item);
  }
  if (newItem != null) {
    filterAll();
  }
  return newItem;
}
