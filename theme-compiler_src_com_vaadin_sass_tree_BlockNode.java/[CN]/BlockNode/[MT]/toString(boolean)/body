{
  StringBuilder string=new StringBuilder();
  string.append(SelectorUtil.toString(selectorList));
  string.append(" {\n");
  for (  Node child : children) {
    if (indent) {
      string.append("\t");
    }
    string.append("\t" + child.toString() + "\n");
  }
  if (indent) {
    string.append("\t");
  }
  string.append("}");
  return string.toString();
}
