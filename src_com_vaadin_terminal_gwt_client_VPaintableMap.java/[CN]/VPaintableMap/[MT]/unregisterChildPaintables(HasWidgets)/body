{
  final Iterator<Widget> it=container.iterator();
  while (it.hasNext()) {
    final Widget w=it.next();
    VPaintableWidget p=getPaintable(w);
    if (p != null) {
      unregisterPaintable(p);
    }
 else     if (w instanceof HasWidgets) {
      unregisterChildPaintables((HasWidgets)w);
    }
  }
}
