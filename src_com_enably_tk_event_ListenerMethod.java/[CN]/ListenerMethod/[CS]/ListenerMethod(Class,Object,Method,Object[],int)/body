{
  if (!method.getDeclaringClass().isAssignableFrom(object.getClass()))   throw new java.lang.IllegalArgumentException();
  if (eventArgumentIndex >= 0 && arguments[eventArgumentIndex] != null)   throw new java.lang.IllegalArgumentException();
  if (eventArgumentIndex >= 0 && !method.getParameterTypes()[eventArgumentIndex].isAssignableFrom(eventType))   throw new java.lang.IllegalArgumentException();
  this.eventType=eventType;
  this.object=object;
  this.method=method;
  this.arguments=arguments;
  this.eventArgumentIndex=eventArgumentIndex;
}
