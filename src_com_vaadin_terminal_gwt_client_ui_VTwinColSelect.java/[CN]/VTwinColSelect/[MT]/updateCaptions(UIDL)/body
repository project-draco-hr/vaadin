{
  String leftCaption=(uidl.hasAttribute(ATTRIBUTE_LEFT_CAPTION) ? uidl.getStringAttribute(ATTRIBUTE_LEFT_CAPTION) : null);
  String rightCaption=(uidl.hasAttribute(ATTRIBUTE_RIGHT_CAPTION) ? uidl.getStringAttribute(ATTRIBUTE_RIGHT_CAPTION) : null);
  boolean hasCaptions=(leftCaption != null || rightCaption != null);
  if (leftCaption == null) {
    removeOptionsCaption();
  }
 else {
    getOptionsCaption().setText(leftCaption);
  }
  if (rightCaption == null) {
    removeSelectionsCaption();
  }
 else {
    getSelectionsCaption().setText(rightCaption);
  }
  captionWrapper.setVisible(hasCaptions);
}
