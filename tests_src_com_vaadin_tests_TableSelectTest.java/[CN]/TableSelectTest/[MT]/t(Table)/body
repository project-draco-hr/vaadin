{
  t.setImmediate(true);
  t.setSelectable(true);
  Object id=null;
  for (int i=0; i < 5; i++) {
    id=t.addItem();
  }
  t.addContainerProperty("asd",String.class,"the asd thing");
  t.addContainerProperty("foo",String.class,"foo stuff");
  t.addContainerProperty("Alonger column header",String.class,"short");
  return id;
}
