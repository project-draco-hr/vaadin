{
  try {
    if (type.equals(Boolean.class))     return new Boolean(!(value.equals("") || value.equals("false")));
    if (type.equals(Integer.class))     return new Integer(value.trim());
    if (type.equals(String.class))     return value;
    throw new ClassCastException("Unsupported type: " + type.getName());
  }
 catch (  NumberFormatException e) {
    return null;
  }
}
