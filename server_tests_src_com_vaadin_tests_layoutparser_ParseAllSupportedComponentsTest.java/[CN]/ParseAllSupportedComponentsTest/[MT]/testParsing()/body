{
  try {
    DesignContext ctx=LayoutHandler.parse(new FileInputStream("server/tests/src/com/vaadin/tests/layoutparser/all-components.html"));
    assertNotNull("The returned design context can not be null",ctx);
    assertNotNull("the component root can not be null",ctx.getComponentRoot());
  }
 catch (  FileNotFoundException e) {
    e.printStackTrace();
    fail("Template parsing threw exception");
  }
}
