{
  setDebug(true);
  openTestURL();
  for (int i=0; i < 3; ++i) {
    removeGridRow();
  }
  for (int i=0; i < 3; ++i) {
    addGridRow();
    assertEquals("" + (100 + i),getGridElement().getCell(i,1).getText());
  }
  assertFalse("There was an unexpected error notification",isElementPresent(NotificationElement.class));
}
