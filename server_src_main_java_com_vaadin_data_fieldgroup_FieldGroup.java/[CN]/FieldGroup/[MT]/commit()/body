{
  if (!isBuffered()) {
    return;
  }
  startTransactions();
  try {
    firePreCommitEvent();
    Map<LegacyField<?>,InvalidValueException> invalidValueExceptions=commitFields();
    if (invalidValueExceptions.isEmpty()) {
      firePostCommitEvent();
      commitTransactions();
    }
 else {
      throw new FieldGroupInvalidValueException(invalidValueExceptions);
    }
  }
 catch (  Exception e) {
    rollbackTransactions();
    throw new CommitException("Commit failed",this,e);
  }
}
