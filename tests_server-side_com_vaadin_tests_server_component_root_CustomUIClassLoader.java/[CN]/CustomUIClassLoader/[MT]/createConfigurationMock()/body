{
  DeploymentConfiguration configurationMock=EasyMock.createMock(DeploymentConfiguration.class);
  EasyMock.expect(configurationMock.isProductionMode()).andReturn(false);
  EasyMock.expect(configurationMock.getInitParameters()).andReturn(new Properties());
  EasyMock.replay(configurationMock);
  return configurationMock;
}
