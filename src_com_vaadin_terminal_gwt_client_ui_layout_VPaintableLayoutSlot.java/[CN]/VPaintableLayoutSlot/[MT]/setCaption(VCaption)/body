{
  VCaption oldCaption=getCaption();
  if (oldCaption != null) {
    layoutManager.unregisterDependency((ManagedLayout)paintable.getParent(),oldCaption.getElement());
  }
  super.setCaption(caption);
  if (caption != null) {
    layoutManager.registerDependency((ManagedLayout)paintable.getParent(),caption.getElement());
  }
}
