{
  StringBuilder sb=new StringBuilder("You should not see 'Aborting layout after 100 passes.' error with debug mode and ");
  if (UI.getCurrent().getPage().getWebBrowser().isChrome()) {
    sb.append(" Zoom is ");
    sb.append("1.10");
  }
 else {
    sb.append(" zoom level 95%.");
  }
  return sb.toString();
}
