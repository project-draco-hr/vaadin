{
  openTestURL();
  List<String> texts=new ArrayList<String>(Arrays.asList("Init parameters:","widgetset: com.vaadin.Vaadin7WidgetSet","closeIdleSessions: true","productionMode: false","testParam: 42","heartbeatInterval: 301","resourceCacheTime: 3601"));
  for (  LabelElement label : $(LabelElement.class).all()) {
    assertTrue(label.getText() + " not found",texts.contains(label.getText()));
    texts.remove(label.getText());
  }
  assertTrue(texts.isEmpty());
}
