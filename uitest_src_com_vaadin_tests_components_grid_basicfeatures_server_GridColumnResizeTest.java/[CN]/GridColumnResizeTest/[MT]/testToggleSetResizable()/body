{
  selectMenuPath("Component","Columns","Column 1","Resizable");
  for (int i=0; i < GridBasicFeatures.COLUMNS; ++i) {
    assertResizable(i,i != 1);
  }
  selectMenuPath("Component","Columns","Column 1","Resizable");
  for (int i=0; i < GridBasicFeatures.COLUMNS; ++i) {
    assertResizable(i,true);
  }
}
