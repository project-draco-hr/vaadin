{
  while (cssProperty.contains("-")) {
    int indexOf=cssProperty.indexOf("-");
    cssProperty=cssProperty.substring(0,indexOf) + String.valueOf(cssProperty.charAt(indexOf + 1)).toUpperCase() + cssProperty.substring(indexOf + 2);
  }
  if ("float".equals(cssProperty)) {
    if (BrowserInfo.get().isIE()) {
      return "styleFloat";
    }
 else {
      return "cssFloat";
    }
  }
  return cssProperty;
}
