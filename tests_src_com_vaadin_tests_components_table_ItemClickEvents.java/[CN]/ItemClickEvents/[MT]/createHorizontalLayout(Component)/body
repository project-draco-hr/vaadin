{
  HorizontalLayout layout=new HorizontalLayout();
  Button b=new Button("immediate",new MethodProperty<Boolean>(c,"immediate"));
  layout.addComponent(b);
  b=new Button("selectable",new MethodProperty<Boolean>(c,"selectable"));
  layout.addComponent(b);
  b=new Button("nullsel",new MethodProperty<Boolean>(c,"nullSelectionAllowed"));
  layout.addComponent(b);
  b=new Button("multi",new MethodProperty<Boolean>(c,"multiSelect"));
  layout.addComponent(b);
  return layout;
}
