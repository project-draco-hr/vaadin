{
  if (value == null) {
    return "";
  }
  @SuppressWarnings("unchecked") Converter<String,Object> converter=(Converter<String,Object>)getFormatter().findConverterFor(sourceType);
  if (converter != null) {
    return converter.convertToPresentation(value,null);
  }
 else {
    return value.toString();
  }
}
