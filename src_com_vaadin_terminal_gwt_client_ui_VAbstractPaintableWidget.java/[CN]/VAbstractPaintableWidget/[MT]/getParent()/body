{
  if (parent != null) {
    return parent;
  }
  VPaintableMap paintableMap=VPaintableMap.get(getConnection());
  Widget w=getWidgetForPaintable();
  while (w != null) {
    w=w.getParent();
    if (paintableMap.isPaintable(w)) {
      parent=(VPaintableWidgetContainer)paintableMap.getPaintable(w);
      return parent;
    }
  }
  return null;
}
