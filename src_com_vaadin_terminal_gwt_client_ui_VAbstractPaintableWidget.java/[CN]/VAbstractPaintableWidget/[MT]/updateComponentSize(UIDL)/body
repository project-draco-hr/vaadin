{
  String w=uidl.hasAttribute("width") ? uidl.getStringAttribute("width") : "";
  String h=uidl.hasAttribute("height") ? uidl.getStringAttribute("height") : "";
  MeasuredSize measuredSize=getMeasuredSize();
  if (!measuredSize.isDirty() && (w.endsWith("%") != definedWidth.endsWith("%") || h.endsWith("%") != definedHeight.endsWith("%"))) {
    measuredSize.setDirty(true);
  }
  definedWidth=w;
  definedHeight=h;
  Widget component=getWidgetForPaintable();
  component.setHeight(h);
  component.setWidth(w);
}
