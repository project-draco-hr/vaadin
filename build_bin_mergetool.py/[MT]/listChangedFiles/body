def listChangedFiles():
    pin = os.popen('svn st', 'r')
    lines = pin.readlines()
    pin.close()
    changed = []
    for line in lines:
        line = line.rstrip()
        print line
        filestate = line[0:2].strip()
        filename = line[7:].strip()
        if ((filename.startswith('build/merge/') or filename.startswith('build/bin/mergetool.py') or filename.startswith('build/testing')) and (filestate == 'M')):
            continue
        if (filestate != '?'):
            changed.append(filename)
    return changed
