{
  try {
    VConsole.log("Variable burst to be sent to server:");
    String curId=null;
    ArrayList<String[]> vars=new ArrayList<String[]>();
    for (int i=0; i < loggedBurst.size(); i++) {
      String value=loggedBurst.get(i++);
      String[] split=loggedBurst.get(i).split(String.valueOf(ApplicationConnection.VAR_FIELD_SEPARATOR));
      String id=split[0];
      if (curId == null) {
        curId=id;
      }
 else       if (!curId.equals(id)) {
        printPaintablesVariables(vars,curId,c);
        vars.clear();
        curId=id;
      }
      split[0]=value;
      vars.add(split);
    }
    if (!vars.isEmpty()) {
      printPaintablesVariables(vars,curId,c);
    }
  }
 catch (  Exception e) {
    VConsole.error(e);
  }
}
