{
  if (itemId != null) {
    super.setItemIcon(itemId,icon);
    if (icon == null) {
      itemIconAlts.remove(itemId);
    }
 else     if (altText == null) {
      throw new IllegalArgumentException("Parameter 'altText' needs to be non null");
    }
 else {
      itemIconAlts.put(itemId,altText);
    }
    markAsDirty();
  }
}
