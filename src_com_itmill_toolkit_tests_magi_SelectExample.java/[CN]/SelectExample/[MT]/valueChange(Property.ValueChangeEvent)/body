  if (false) {
    status.setValue("Currently selected item ID: " + event.getProperty().getValue() + "<br/>"+ "Class of the Item ID: "+ event.getProperty().getValue().getClass().getName()+ "<br/>"+ "Caption: "+ select.getItemCaption(event.getProperty().getValue()));
