{
  TextField tfUndefWide=new TextField("Undefined wide text field with a very long caption, longer than the field and the layout.");
  TextField tf100pxWide=new TextField("100 px wide text field with a very long caption, longer than 100px.");
  tf100pxWide.setWidth("100px");
  components.add(tfUndefWide);
  components.add(tf100pxWide);
}
