{
  openTestURL();
  GridElement grid=getGridElement();
  assertTrue("Body cell 0, 0 is not active on init.",cellIsActive(grid,0,0));
  grid.getCell(5,2).click();
  assertFalse("Body cell 0, 0 was still active after clicking",cellIsActive(grid,0,0));
  assertTrue("Body cell 5, 2 is not active after clicking",cellIsActive(grid,5,2));
}
