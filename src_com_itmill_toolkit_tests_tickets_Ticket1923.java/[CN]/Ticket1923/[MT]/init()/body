{
  final Window main=new Window(getClass().getName().substring(getClass().getName().lastIndexOf(".") + 1));
  setMainWindow(main);
  p=new Panel("TestPanel");
  p.setScrollable(true);
  for (int i=0; i < ROWS; i++) {
    p.addComponent(new Label("Label" + i + " "));
  }
  p.setHeight("300px");
  p.setWidth("250px");
  p.setScrollTop(100);
  p.setScrollLeft(100);
  main.addComponent(p);
}
