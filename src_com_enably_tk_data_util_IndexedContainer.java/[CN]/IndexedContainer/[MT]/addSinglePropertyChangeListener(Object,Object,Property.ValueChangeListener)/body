{
  if (listener != null) {
    if (singlePropertyValueChangeListeners == null)     singlePropertyValueChangeListeners=new Hashtable();
    Hashtable propertySetToListenerListMap=(Hashtable)singlePropertyValueChangeListeners.get(propertyId);
    if (propertySetToListenerListMap == null) {
      propertySetToListenerListMap=new Hashtable();
      singlePropertyValueChangeListeners.put(propertyId,propertySetToListenerListMap);
    }
    LinkedList listenerList=(LinkedList)propertySetToListenerListMap.get(itemId);
    if (listenerList == null) {
      listenerList=new LinkedList();
      propertySetToListenerListMap.put(itemId,listenerList);
    }
    listenerList.addLast(listener);
  }
}
