{
  String url=getTestUrl();
  StringBuilder defaultValoUi=new StringBuilder(TextFieldBevel.class.getSimpleName());
  defaultValoUi.append('$');
  defaultValoUi.append(TextFieldBevel.ValoDefaultTextFieldBevel.class.getSimpleName());
  url=url.replace(TextFieldBevel.class.getSimpleName(),defaultValoUi.toString());
  getDriver().get(url);
  String defaultBoxShadow=$(TextFieldElement.class).first().getCssValue("box-shadow");
  if (url.contains("restartApplication")) {
    openTestURL();
  }
 else {
    openTestURL("restartApplication");
  }
  String boxShadow=$(TextFieldElement.class).first().getCssValue("box-shadow");
  Assert.assertNotEquals("Set v-bevel to 'false' doesn't affect 'v-textfield-bevel' value",defaultBoxShadow,boxShadow);
}
