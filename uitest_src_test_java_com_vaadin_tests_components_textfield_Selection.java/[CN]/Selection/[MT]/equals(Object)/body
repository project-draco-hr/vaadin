{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Selection other=(Selection)obj;
  if (length != other.length) {
    return false;
  }
  if (start != other.start) {
    return false;
  }
  return true;
}
