{
  if (!(value instanceof Address)) {
    return false;
  }
  Address a=(Address)value;
  if (a.getCity() == null || ("" + a.getCity()).length() < 1) {
    return false;
  }
  if (a.getStreet() == null || ("" + a.getStreet()).length() < 1) {
    return false;
  }
  if (a.getZip() == null || ("" + a.getZip()).length() < 5) {
    return false;
  }
  return true;
}
