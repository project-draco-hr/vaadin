{
  container.clear();
  if (uidl.getStringAttribute("orientation").equals("horizontal"))   panel=new HorizontalPanel();
 else   panel=new VerticalPanel();
  container.add(panel);
  for (Iterator i=uidl.getChildIterator(); i.hasNext(); ) {
    UIDL uidlForChild=(UIDL)i.next();
    Widget child=client.createWidgetFromUIDL(uidlForChild);
    panel.add(child);
  }
}
