{
  setElement(DOM.createDiv());
  caption=new VCaption(null,client);
  caption.addClickHandler(this);
  super.add(caption,captionNode);
  DOM.appendChild(captionNode,caption.getElement());
  DOM.appendChild(getElement(),captionNode);
  DOM.appendChild(getElement(),content);
  setStyleName(CLASSNAME + "-item");
  DOM.setElementProperty(content,"className",CLASSNAME + "-item-content");
  DOM.setElementProperty(captionNode,"className",CLASSNAME + "-item-caption");
  close();
}
