{
  caption.updateCaptionWithoutOwner(uidl.getStringAttribute(TabsheetBaseConnector.ATTRIBUTE_TAB_CAPTION),uidl.hasAttribute(TabsheetBaseConnector.ATTRIBUTE_TAB_DISABLED),uidl.hasAttribute(TabsheetBaseConnector.ATTRIBUTE_TAB_DESCRIPTION),uidl.getStringAttribute(TabsheetBaseConnector.ATTRIBUTE_TAB_ICON));
}
