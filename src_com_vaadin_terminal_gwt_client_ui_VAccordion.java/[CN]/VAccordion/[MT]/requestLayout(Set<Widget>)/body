{
  if (!isDynamicHeight() && !isDynamicWidth()) {
    for (    Widget widget : children) {
      client.handleComponentRelativeSize(widget);
    }
    return true;
  }
  updateOpenTabSize();
  if (renderInformation.updateSize(getElement())) {
    iLayout();
    client.runDescendentsLayout(this);
    return false;
  }
 else {
    return true;
  }
}
