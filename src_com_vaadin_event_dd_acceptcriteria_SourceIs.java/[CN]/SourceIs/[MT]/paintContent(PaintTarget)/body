{
  super.paintContent(target);
  int paintedComponents=0;
  for (int i=0; i < components.length; i++) {
    Component c=components[i];
    if (c.getApplication() != null) {
      target.addAttribute("component" + paintedComponents++,c);
    }
 else {
      logger.log(Level.WARNING,"SourceIs component {0} at index {1} is not attached to the component hierachy and will thus be ignored",new Object[]{c.getClass().getName(),Integer.valueOf(i)});
    }
  }
  target.addAttribute("c",paintedComponents);
}
