{
  Collection<Class<? extends Paintable>> paintables=ClassPathExplorer.getPaintablesHavingWidgetAnnotation();
  System.out.println("Found annotated paintables:");
  for (  Class<? extends Paintable> cls : paintables) {
    System.out.println(cls.getCanonicalName());
  }
  System.out.println();
  System.out.println("Searching available widgetsets...");
  Collection<String> availableWidgetSets=ClassPathExplorer.getAvailableWidgetSets();
  for (  String string : availableWidgetSets) {
    System.out.println(string);
  }
}
