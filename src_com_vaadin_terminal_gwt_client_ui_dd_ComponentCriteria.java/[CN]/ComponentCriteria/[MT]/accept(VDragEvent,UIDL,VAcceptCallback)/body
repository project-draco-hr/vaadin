{
  try {
    Paintable component=drag.getTransferable().getComponent();
    int c=configuration.getIntAttribute("c");
    for (int i=0; i < c; i++) {
      String requiredPid=configuration.getStringAttribute("component" + i);
      Paintable paintable=VDragAndDropManager.get().getCurrentDropHandler().getApplicationConnection().getPaintable(requiredPid);
      if (paintable == component) {
        callback.accepted(drag);
      }
    }
  }
 catch (  Exception e) {
  }
  return;
}
