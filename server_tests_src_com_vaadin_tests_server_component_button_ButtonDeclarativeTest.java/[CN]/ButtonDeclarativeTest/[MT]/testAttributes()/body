{
  String design="<v-button tabindex=3 plain-text='' icon-alt=OK " + "click-shortcut=ctrl-shift-o></v-button>";
  Button b=new Button("");
  b.setTabIndex(3);
  b.setIconAlternateText("OK");
  b.setClickShortcut(KeyCode.O,ModifierKey.CTRL,ModifierKey.SHIFT);
  testRead(design,b);
  testWrite(design,b);
}
