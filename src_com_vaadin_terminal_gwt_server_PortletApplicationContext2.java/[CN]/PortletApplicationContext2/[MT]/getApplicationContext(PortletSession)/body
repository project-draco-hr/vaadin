{
  PortletApplicationContext2 cx=(PortletApplicationContext2)session.getAttribute(PortletApplicationContext2.class.getName());
  if (cx == null) {
    cx=new PortletApplicationContext2();
    session.setAttribute(PortletApplicationContext2.class.getName(),cx);
  }
  if (cx.session == null) {
    cx.session=session;
  }
  return cx;
}
