{
  if (response instanceof MimeResponse) {
    ResourceURL resourceURL=((MimeResponse)response).createResourceURL();
    final String filename=resource.getFilename();
    if (filename == null) {
      resourceURL.setResourceID("APP/" + mapKey + "/");
    }
 else {
      resourceURL.setResourceID("APP/" + mapKey + "/"+ urlEncode(filename));
    }
    return resourceURL.toString();
  }
 else {
    return null;
  }
}
