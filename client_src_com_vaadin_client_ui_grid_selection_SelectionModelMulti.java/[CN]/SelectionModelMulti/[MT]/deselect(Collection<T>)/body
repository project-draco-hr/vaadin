{
  if (rows == null) {
    throw new IllegalArgumentException("Rows cannot be null");
  }
  Set<T> removed=new LinkedHashSet<T>();
  for (  T row : rows) {
    if (deselectByHandle(grid.getDataSource().getHandle(row))) {
      removed.add(row);
    }
  }
  if (removed.size() > 0) {
    grid.fireEvent(new SelectionChangeEvent<T>(grid,null,removed));
    return true;
  }
  return false;
}
