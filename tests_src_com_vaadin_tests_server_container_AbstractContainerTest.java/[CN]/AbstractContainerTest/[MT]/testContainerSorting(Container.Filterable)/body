{
  Container.Sortable sortable=(Sortable)container;
  initializeContainer(container);
  assertTrue(sortable.getSortableContainerPropertyIds().contains(PROP1));
  assertTrue(sortable.getSortableContainerPropertyIds().contains(PROP2));
  sortable.sort(new Object[]{PROP1},new boolean[]{true});
  validateContainer(container,"com.vaadin.Application","com.vaadin.util.SerializerHelper","com.vaadin.terminal.ApplicationResource","blah",sampleData.length);
  sortable.sort(new Object[]{PROP2},new boolean[]{true});
  validateContainer(container,"com.vaadin.terminal.gwt.server.ApplicationPortlet2","com.vaadin.data.util.ObjectProperty","com.vaadin.ui.BaseFieldFactory","blah",sampleData.length);
}
