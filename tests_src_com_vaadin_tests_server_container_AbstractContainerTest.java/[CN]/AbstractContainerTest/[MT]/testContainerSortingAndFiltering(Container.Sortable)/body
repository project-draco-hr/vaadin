{
  Filterable filterable=(Filterable)sortable;
  initializeContainer(sortable);
  filterable.addContainerFilter(FULLY_QUALIFIED_NAME,"ab",false,false);
  assertTrue(sortable.getSortableContainerPropertyIds().contains(FULLY_QUALIFIED_NAME));
  sortable.sort(new Object[]{FULLY_QUALIFIED_NAME},new boolean[]{true});
  validateContainer(sortable,"com.vaadin.data.BufferedValidatable","com.vaadin.ui.TableFieldFactory","com.vaadin.ui.TableFieldFactory","com.vaadin.data.util.BeanItem",20);
}
