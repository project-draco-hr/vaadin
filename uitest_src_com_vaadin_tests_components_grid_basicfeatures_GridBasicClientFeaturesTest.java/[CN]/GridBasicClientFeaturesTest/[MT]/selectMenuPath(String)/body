{
  new Actions(getDriver()).moveToElement(getMenuElement(menuCaptions[0])).click().perform();
  for (int i=1; i < menuCaptions.length - 1; ++i) {
    selectMenu(menuCaptions[i]);
  }
  new Actions(getDriver()).moveToElement(getMenuElement(menuCaptions[menuCaptions.length - 1])).click().perform();
}
