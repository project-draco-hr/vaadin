{
  Panel p=new Panel("");
  if (styleName != null) {
    p.setCaption(styleName);
  }
  p.setContent(new GridLayout());
  p.setSizeUndefined();
  p.getContent().setSizeUndefined();
  for (int i=0; i < labels; i++) {
    Label l=new Label("Label " + i);
    if (styleName != null) {
      l.setStyleName(styleName);
    }
    p.addComponent(l);
  }
  return p;
}
