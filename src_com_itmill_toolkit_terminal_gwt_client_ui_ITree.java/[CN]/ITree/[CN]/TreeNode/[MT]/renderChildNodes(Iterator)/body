{
  removeItems();
  while (i.hasNext()) {
    UIDL childUidl=(UIDL)i.next();
    if ("actions".equals(childUidl.getTag())) {
      updateActionMap(childUidl);
      continue;
    }
    TreeNode childTree=new TreeNode();
    this.addItem(childTree);
    childTree.updateFromUIDL(childUidl,client);
  }
  childrenLoaded=true;
}
