{
  if (!isEnabled() || isReadonly()) {
    return;
  }
  if (sender == prevYear) {
    focusPreviousYear(1);
  }
 else   if (sender == nextYear) {
    focusNextYear(1);
  }
 else   if (sender == prevMonth) {
    focusPreviousMonth();
  }
 else   if (sender == nextMonth) {
    focusNextMonth();
  }
}
