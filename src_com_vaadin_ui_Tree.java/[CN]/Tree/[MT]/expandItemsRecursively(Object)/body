{
  boolean result=true;
  final Stack todo=new Stack();
  todo.add(startItemId);
  while (!todo.isEmpty()) {
    final Object id=todo.pop();
    if (areChildrenAllowed(id) && !expandItem(id,false)) {
      result=false;
    }
    if (hasChildren(id)) {
      todo.addAll(getChildren(id));
    }
  }
  requestRepaint();
  return result;
}
