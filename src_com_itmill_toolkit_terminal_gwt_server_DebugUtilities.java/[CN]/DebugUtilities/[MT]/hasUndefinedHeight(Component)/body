{
  if (parent instanceof Window) {
    Window w=(Window)parent;
    if (w.getParent() == null) {
      return false;
    }
  }
  if (parent.getHeight() < 0) {
    return true;
  }
 else {
    if (hasRelativeHeight(parent) && parent.getParent() != null) {
      return hasUndefinedHeight(parent.getParent());
    }
 else {
      return false;
    }
  }
}
