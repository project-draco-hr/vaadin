{
  if (!classpathEntry.endsWith(".jar")) {
    return true;
  }
 else {
    if (classpathEntry.contains("vaadin-") || classpathEntry.contains(".vaadin.")) {
      return true;
    }
 else {
      URL url;
      try {
        url=new URL("file:" + new File(classpathEntry).getCanonicalPath());
        url=new URL("jar:" + url.toExternalForm() + "!/");
        JarURLConnection conn=(JarURLConnection)url.openConnection();
        getLogger().fine(url.toString());
        JarFile jarFile=conn.getJarFile();
        Manifest manifest=jarFile.getManifest();
        if (manifest != null) {
          Attributes mainAttributes=manifest.getMainAttributes();
          if (mainAttributes.getValue("Vaadin-Widgetsets") != null) {
            return true;
          }
          if (mainAttributes.getValue("Vaadin-Stylesheets") != null) {
            return true;
          }
        }
      }
 catch (      MalformedURLException e) {
        getLogger().log(Level.FINEST,"Failed to inspect JAR file",e);
      }
catch (      IOException e) {
        getLogger().log(Level.FINEST,"Failed to inspect JAR file",e);
      }
      return false;
    }
  }
}
