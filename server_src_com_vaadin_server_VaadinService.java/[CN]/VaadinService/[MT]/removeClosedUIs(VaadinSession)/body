{
  ArrayList<UI> uis=new ArrayList<UI>(session.getUIs());
  for (  final UI ui : uis) {
    if (ui.isClosing()) {
      ui.accessSynchronously(new Runnable(){
        @Override public void run(){
          getLogger().log(Level.FINER,"Removing closed UI {0}",ui.getUIId());
          session.removeUI(ui);
        }
      }
);
    }
  }
}
