{
  if (node != null) {
    int index=children.indexOf(after);
    if (index != -1) {
      children.add(index + 1,node);
      if (node.getParentNode() != null) {
        node.getParentNode().removeChild(node);
      }
      node.setParentNode(this);
    }
 else {
      throw new NullPointerException("after-node was not found");
    }
  }
}
