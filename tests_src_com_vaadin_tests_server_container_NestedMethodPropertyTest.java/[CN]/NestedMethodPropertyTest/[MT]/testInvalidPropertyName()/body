  try {
    new NestedMethodProperty(vaadin,".");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new NestedMethodProperty(vaadin,".manager");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
