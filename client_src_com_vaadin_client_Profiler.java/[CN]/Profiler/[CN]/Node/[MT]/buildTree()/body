{
  String message=getStringRepresentation("");
  if (getName() == null || !children.isEmpty()) {
    SimpleTree tree=new SimpleTree(message);
    for (    Node node : children.values()) {
      Widget child=node.buildTree();
      tree.add(child);
    }
    return tree;
  }
 else {
    return new Label(message);
  }
}
