{
  LinkedHashMap<String,Integer> options=new LinkedHashMap<String,Integer>();
  options.put("0",0);
  options.put("5",5);
  options.put("10",10);
  options.put("50",50);
  options.put("100",100);
  options.put("1000",1000);
  return super.createSelectAction("Columns in container",options,"10",new Command<Table,Integer>(){
    public void execute(    Table t,    Integer value){
      t.setContainerDataSource(createContainer(value,t.getContainerDataSource().size()));
    }
  }
);
}
