{
  LinkedHashMap<String,Integer> options=new LinkedHashMap<String,Integer>();
  options.put("0",0);
  options.put("5",5);
  options.put("10",10);
  options.put("20",20);
  options.put("50",50);
  return super.createSelectAction("PageLength",options,"10",new Command<Table,Integer>(){
    public void execute(    Table t,    Integer value){
      t.setPageLength(value);
    }
  }
);
}
