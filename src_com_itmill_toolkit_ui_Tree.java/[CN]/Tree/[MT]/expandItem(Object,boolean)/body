{
  if (isExpanded(itemId)) {
    return true;
  }
  if (!areChildrenAllowed(itemId)) {
    return false;
  }
  this.expanded.add(itemId);
  this.expandedItemId=itemId;
  if (this.initialPaint) {
    requestRepaint();
  }
 else   if (sendChildTree) {
    requestPartialRepaint();
  }
  fireExpandEvent(itemId);
  return true;
}
