{
  if (isReadOnly()) {
    throw new Property.ReadOnlyException();
  }
  if (newValue != null && !type.isAssignableFrom(newValue.getClass())) {
    throw new IllegalArgumentException("Invalid value type " + newValue.getClass().getName() + " for ObjectProperty of type "+ type.getName()+ ".");
  }
  this.value=(T)newValue;
  fireValueChange();
}
