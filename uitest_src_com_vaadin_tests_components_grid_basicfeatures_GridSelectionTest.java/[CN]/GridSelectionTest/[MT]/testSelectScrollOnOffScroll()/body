{
  openTestURL();
  setSelectionModelMulti();
  assertFalse("row shouldn't start out as selected",isSelected(getRow(0)));
  scrollGridVerticallyTo(10000);
  toggleFirstRowSelection();
  toggleFirstRowSelection();
  scrollGridVerticallyTo(0);
  assertFalse("row shouldn't be selected when scrolling " + "back into view",isSelected(getRow(0)));
}
