{
  super.readDesign(design,designContext);
  readTabIndex(design);
  removeAllComponents();
  for (  Element tab : design.children()) {
    if (!tab.tagName().equals("tab")) {
      throw new DesignException("Invalid tag name for tabsheet tab " + tab.tagName());
    }
    readTabFromDesign(tab,designContext);
  }
}
