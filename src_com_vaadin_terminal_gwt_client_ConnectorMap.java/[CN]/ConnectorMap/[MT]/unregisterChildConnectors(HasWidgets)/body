{
  final Iterator<Widget> it=container.iterator();
  while (it.hasNext()) {
    final Widget w=it.next();
    ComponentConnector p=getConnector(w);
    if (p != null) {
      unregisterConnector(p);
    }
 else     if (w instanceof HasWidgets) {
      unregisterChildConnectors((HasWidgets)w);
    }
  }
}
