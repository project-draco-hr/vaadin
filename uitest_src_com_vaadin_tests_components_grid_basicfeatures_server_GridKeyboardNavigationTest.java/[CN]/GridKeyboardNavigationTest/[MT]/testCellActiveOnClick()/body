{
  openTestURL();
  GridElement grid=getGridElement();
  assertTrue("Body cell 0, 0 is not active on init.",grid.getCell(0,0).isActive());
  grid.getCell(5,2).click();
  assertFalse("Body cell 0, 0 was still active after clicking",grid.getCell(0,0).isActive());
  assertTrue("Body cell 5, 2 is not active after clicking",grid.getCell(5,2).isActive());
}
