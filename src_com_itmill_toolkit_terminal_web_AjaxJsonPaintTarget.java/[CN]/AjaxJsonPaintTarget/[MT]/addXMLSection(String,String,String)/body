{
  if (this.closed)   throw new PaintException("Attempted to write to a closed PaintTarget.");
  startTag(sectionTagName);
  if (namespace != null)   addAttribute("xmlns",namespace);
  mTagArgumentListOpen=false;
  customLayoutArgumentsOpen=false;
  if (sectionData != null)   tag.addData("\"" + escapeJSON(sectionData) + "\"");
  endTag(sectionTagName);
}
