{
  Map<Integer,SpacerImpl> removedSpacers=rowIndexToSpacer.subMap(removedRange.getStart(),true,removedRange.getEnd(),false);
  for (  SpacerImpl spacer : removedSpacers.values()) {
    destroySpacerContent(spacer);
    spacer.setHeight(0);
    spacer.getRootElement().removeFromParent();
  }
  removedSpacers.clear();
}
