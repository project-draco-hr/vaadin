{
  touches=event.getNativeEvent().getTouches().length();
  if (touches != 1) {
    return;
  }
  escalator.scroller.cancelFlickScroll();
  lastX=event.getPageX();
  lastY=event.getPageY();
  flickPageX1=lastX;
  flickPageX2=-1;
  flickPageY1=lastY;
  flickPageY2=-1;
  flickStartTime=System.currentTimeMillis();
  flickTimestamp=0;
  snappedScrollEnabled=true;
}
