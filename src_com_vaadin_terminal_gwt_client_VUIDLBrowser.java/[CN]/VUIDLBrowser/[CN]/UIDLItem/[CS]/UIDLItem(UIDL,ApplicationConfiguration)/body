{
  this.uidl=uidl;
  try {
    String name=uidl.getTag();
    try {
      Integer.parseInt(name);
      Class<? extends Paintable> widgetClassByDecodedTag=conf.getWidgetClassByEncodedTag(name);
      name=widgetClassByDecodedTag.getName();
    }
 catch (    Exception e) {
    }
    setText(name);
    addItem("LOADING");
  }
 catch (  Exception e) {
    setText(uidl.toString());
  }
}
