{
  LinkedHashMap<String,String> options=new LinkedHashMap<String,String>();
  options.put("<none>",null);
  options.put("Please enter date","Please enter date");
  options.put("????????????","????????????");
  return createSelectAction("Input prompt",options,"<none>",new Command<LegacyPopupDateField,String>(){
    @Override public void execute(    LegacyPopupDateField c,    String value,    Object data){
      c.setInputPrompt(value);
    }
  }
);
}
