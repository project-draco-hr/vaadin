{
  selected=sel;
  if (selected) {
    selectedRowKeys.add(key);
    DOM.setStyleAttribute(tBody.getRowFormatter().getElement(rowIndex),"background","yellow");
  }
 else {
    selectedRowKeys.remove(key);
    DOM.setStyleAttribute(tBody.getRowFormatter().getElement(rowIndex),"background","transparent");
  }
}
