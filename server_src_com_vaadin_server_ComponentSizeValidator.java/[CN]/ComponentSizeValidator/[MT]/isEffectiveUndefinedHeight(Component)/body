{
  if (parent == null) {
    return false;
  }
 else   if (parent.getHeight() < 0) {
    if (parent instanceof Window) {
      return false;
    }
    return true;
  }
 else   if (parent.getHeightUnits() == Unit.PERCENTAGE) {
    return isEffectiveUndefinedHeight(parent.getParent());
  }
  return false;
}
