{
  if (parent == null) {
    return false;
  }
 else   if (parent.getWidth() < 0) {
    if (parent instanceof Window) {
      return false;
    }
    return true;
  }
 else   if (parent.getWidthUnits() == Unit.PERCENTAGE) {
    return isEffectiveUndefinedWidth(parent.getParent());
  }
  return false;
}
