{
  if (this.uiId != -1) {
    throw new IllegalStateException("UI id has already been defined");
  }
  this.uiId=uiId;
  theme=request.getParameter("theme");
  getPage().init(request);
  init(request);
  Navigator navigator=getNavigator();
  if (navigator != null) {
    navigator.navigateTo(navigator.getState());
  }
}
