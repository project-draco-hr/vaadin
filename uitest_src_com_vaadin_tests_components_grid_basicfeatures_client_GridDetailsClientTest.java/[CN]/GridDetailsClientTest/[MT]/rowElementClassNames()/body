{
  selectMenuPath(SET_GENERATOR);
  toggleDetailsFor(0);
  toggleDetailsFor(1);
  List<WebElement> elements=getGridElement().findElements(By.className("v-grid-spacer"));
  assertEquals("v-grid-spacer",elements.get(0).getAttribute("class"));
  assertEquals("v-grid-spacer stripe",elements.get(1).getAttribute("class"));
}
