{
  Class<? extends Application> appClass=null;
  String baseName=getApplicationRunnerApplicationClassName(request.get());
  try {
    appClass=(Class<? extends Application>)getClass().getClassLoader().loadClass(baseName);
    return appClass;
  }
 catch (  Exception e) {
    if (defaultPackages != null) {
      for (int i=0; i < defaultPackages.length; i++) {
        try {
          appClass=(Class<? extends Application>)getClass().getClassLoader().loadClass(defaultPackages[i] + "." + baseName);
        }
 catch (        ClassNotFoundException ee) {
        }
catch (        Exception e2) {
          getLogger().log(Level.FINE,"Failed to find application class in the default package.",e2);
        }
        if (appClass != null) {
          return appClass;
        }
      }
    }
  }
  throw new ClassNotFoundException();
}
