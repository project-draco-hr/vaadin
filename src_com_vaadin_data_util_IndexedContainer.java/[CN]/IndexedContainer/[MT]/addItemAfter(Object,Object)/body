{
  if (previousItemId == null) {
    return addItemAt(0,newItemId);
  }
  int index=-1;
  if (previousItemId != null) {
    index=1 + indexOfId(previousItemId);
    if (index <= 0 || index > size()) {
      return null;
    }
  }
  if (index < 0) {
    index=0;
  }
  return addItemAt(index,newItemId);
}
