{
  final List<Object> list=new LinkedList<Object>();
  for (final Iterator<?> i=propertyIds.iterator(); i.hasNext(); ) {
    final Object id=i.next();
    final Class<?> type=getType(id);
    if (type != null && Comparable.class.isAssignableFrom(type)) {
      list.add(id);
    }
  }
  return list;
}
