{
  if (filteredItemIds != null) {
    try {
      final Object first=filteredItemIds.iterator().next();
      return (itemId != null && itemId.equals(first));
    }
 catch (    final NoSuchElementException e) {
      return false;
    }
  }
  return (size() >= 1 && itemIds.get(0).equals(itemId));
}
