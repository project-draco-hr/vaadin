{
  if (getFilteredItemIds() != null) {
    try {
      final Object first=getFilteredItemIds().iterator().next();
      return (itemId != null && itemId.equals(first));
    }
 catch (    final NoSuchElementException e) {
      return false;
    }
  }
  return (size() >= 1 && allItemIds.get(0).equals(itemId));
}
