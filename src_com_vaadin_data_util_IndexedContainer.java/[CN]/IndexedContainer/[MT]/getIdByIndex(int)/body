{
  if (getFilteredItemIds() != null) {
    if (index < 0) {
      throw new IndexOutOfBoundsException();
    }
    try {
      final Iterator<?> i=getFilteredItemIds().iterator();
      while (index-- > 0) {
        i.next();
      }
      return i.next();
    }
 catch (    final NoSuchElementException e) {
      throw new IndexOutOfBoundsException();
    }
  }
  return allItemIds.get(index);
}
