{
  if (filteredItemIds != null) {
    int index=0;
    if (itemId == null) {
      return -1;
    }
    final Iterator<?> i=filteredItemIds.iterator();
    while (i.hasNext()) {
      Object id=i.next();
      if (itemId.equals(id)) {
        return index;
      }
      index++;
    }
    return -1;
  }
  return itemIds.indexOf(itemId);
}
