{
  if (propertySetChangeListeners != null) {
    final Object[] l=propertySetChangeListeners.toArray();
    final Container.PropertySetChangeEvent event=new IndexedContainer.PropertySetChangeEvent(this);
    for (int i=0; i < l.length; i++) {
      ((Container.PropertySetChangeListener)l[i]).containerPropertySetChange(event);
    }
  }
}
