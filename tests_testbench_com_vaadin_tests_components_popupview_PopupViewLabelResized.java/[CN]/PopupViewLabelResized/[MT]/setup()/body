{
  GridLayout gl=new GridLayout(3,1);
  gl.setSizeFull();
  Label expander=new Label();
  gl.addComponent(expander,1,0);
  gl.setColumnExpandRatio(1,1);
  gl.addComponent(new PopupView("Click here to popup",new Label("test")),0,0);
  gl.addComponent(new PopupView("Click here to popup",new Label("test")),2,0);
  addComponent(gl);
}
