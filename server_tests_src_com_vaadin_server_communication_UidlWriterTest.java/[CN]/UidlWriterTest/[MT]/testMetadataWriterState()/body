{
  Assert.assertEquals("Metadata should contain redirect interval on first write",115,getRedirect(uidl(false,false)).optInt("interval"));
  Assert.assertNull("Metadata should not contain redirect interval on second write",getRedirect(uidl(false,false)));
  Assert.assertEquals("Metadata should contain redirect interval on repaintAll",115,getRedirect(uidl(true,false)).optInt("interval"));
  Assert.assertEquals("Metadata should contain redirect interval when changed in session",215,getRedirect(uidl(false,false)).optInt("interval"));
}
