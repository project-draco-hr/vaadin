{
  final Flot flot=new Flot();
  flot.setHeight("300px");
  flot.setWidth("400px");
  flot.addSeries(1,2,4,8,16);
  addComponent(flot);
  addComponent(new Button("Highlight point",new Button.ClickListener(){
    @Override public void buttonClick(    ClickEvent event){
      flot.highlight(0,3);
    }
  }
));
}
