{
  if (selectedKey != null && !selectedKey.equals("")) {
    performSelection(selectedKey,oldSuggestionTextMatchTheOldSelection,!isWaitingForFilteringResponse() || popupOpenerClicked);
    setSelectedCaption(null);
  }
 else   if (!isWaitingForFilteringResponse() && selectedCaption != null) {
    setSelectedCaption(selectedCaption);
  }
 else {
    if (!isWaitingForFilteringResponse() || popupOpenerClicked) {
      resetSelection(popupOpenerClicked);
    }
  }
}
