{
  if (!focused) {
    setPromptingOff("");
    if (enabled && !readonly) {
      setPromptingOn();
    }
  }
 else {
    if (selectedOptionKey != null || (allowNewItem && !tb.getValue().isEmpty())) {
      if (useSelectedCaption && getSelectedCaption() != null) {
        tb.setValue(getSelectedCaption());
        tb.selectAll();
      }
 else {
        tb.setValue("");
      }
    }
  }
  currentSuggestion=null;
  setSelectedItemIcon(null);
  selectedOptionKey=null;
}
