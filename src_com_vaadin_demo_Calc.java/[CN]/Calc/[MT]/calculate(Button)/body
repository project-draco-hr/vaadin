{
  char requestedOperation=buttonClicked.getCaption().charAt(0);
  if ('0' <= requestedOperation && requestedOperation <= '9') {
    current=current * 10 + Double.parseDouble("" + requestedOperation);
    return current;
  }
switch (lastOperationRequested) {
case '+':
    stored+=current;
  break;
case '-':
stored-=current;
break;
case '/':
stored/=current;
break;
case '*':
stored*=current;
break;
case 'C':
stored=current;
break;
}
lastOperationRequested=requestedOperation;
current=0.0;
if (requestedOperation == 'C') {
stored=0.0;
}
return stored;
}
