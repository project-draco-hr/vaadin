{
  if (isSecret()) {
    target.addAttribute("secret",true);
  }
  final int rows=getRows();
  if (rows != 0) {
    target.addAttribute("rows",rows);
    target.addAttribute("multiline",true);
    if (!isWordwrap()) {
      target.addAttribute("wordwrap",false);
    }
  }
  super.paintContent(target);
}
