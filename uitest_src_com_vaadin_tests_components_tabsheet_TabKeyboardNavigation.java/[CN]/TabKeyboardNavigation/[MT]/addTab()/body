{
  Layout content=new VerticalLayout();
  tabs.add(content);
  content.addComponent(new Label("Tab " + index));
  content.addComponent(new TextField());
  Tab tab=ts.addTab(content,"Tab " + index,null);
  if (index == 2) {
    tab.setClosable(true);
  }
  if (index == 4) {
    tab.setEnabled(false);
  }
  index++;
  return tab;
}
