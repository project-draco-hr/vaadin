{
  Element design=createDesign();
  AbstractComponent component=getComponent();
  Responsive.makeResponsive(component);
  component.writeDesign(design,ctx);
  assertTrue("Design attributes should have key 'responsive'",design.attributes().hasKey("responsive"));
  assertFalse("Responsive attribute should not be 'false'",design.attr("responsive").equalsIgnoreCase("false"));
}
