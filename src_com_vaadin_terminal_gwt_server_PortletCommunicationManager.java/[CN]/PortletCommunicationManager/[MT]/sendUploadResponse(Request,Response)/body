{
  if (response.getWrappedResponse() instanceof ActionResponse) {
    System.out.println("Redirecting to dummyURL: " + dummyURL);
    ((ActionResponse)response.getWrappedResponse()).sendRedirect(dummyURL == null ? "http://www.google.com" : dummyURL);
  }
 else   super.sendUploadResponse(request,response);
}
