{
  response.setContentType(JsonConstants.JSON_CONTENT_TYPE);
  response.setHeader("Cache-Control","no-cache");
  OutputStreamWriter outputWriter=new OutputStreamWriter(response.getOutputStream(),"UTF-8");
  try {
    outputWriter.write(json);
    outputWriter.flush();
  }
  finally {
    outputWriter.close();
  }
  return true;
}
