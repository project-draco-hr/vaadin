{
  File tmpFile;
  try {
    tmpFile=File.createTempFile(DEFAULT_THEME_TEMP_FILE_PREFIX,extension);
    FileOutputStream out=new FileOutputStream(tmpFile);
    byte[] buf=new byte[1024];
    int bytes=0;
    while ((bytes=stream.read(buf)) > 0) {
      out.write(buf,0,bytes);
    }
    out.close();
  }
 catch (  IOException e) {
    System.err.println("Failed to create temporary file for default theme: " + e);
    tmpFile=null;
  }
  return tmpFile;
}
