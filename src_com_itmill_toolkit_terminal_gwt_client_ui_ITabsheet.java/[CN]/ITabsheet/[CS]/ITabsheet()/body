{
  super(CLASSNAME);
  tb=new TabBar();
  tp=new ITabsheetPanel();
  tp.setStyleName(CLASSNAME + "-tabsheetpanel");
  contentNode=DOM.createDiv();
  deco=DOM.createDiv();
  addStyleDependentName("loading");
  tb.setStyleName(CLASSNAME + "-tabs");
  DOM.setElementProperty(contentNode,"className",CLASSNAME + "-content");
  DOM.setElementProperty(deco,"className",CLASSNAME + "-deco");
  add(tb,getElement());
  DOM.appendChild(getElement(),contentNode);
  add(tp,contentNode);
  DOM.appendChild(getElement(),deco);
  tb.addTabListener(tl);
  DOM.setStyleAttribute(DOM.getFirstChild(DOM.getFirstChild(DOM.getFirstChild(tb.getElement()))),"display","none");
}
