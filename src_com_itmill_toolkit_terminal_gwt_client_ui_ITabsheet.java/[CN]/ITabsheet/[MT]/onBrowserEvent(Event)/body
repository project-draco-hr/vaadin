{
  if (isScrolledTabs() && DOM.eventGetTarget(event) == scrollerPrev) {
    if (scrollerIndex > 0) {
      scrollerIndex--;
      DOM.setStyleAttribute(DOM.getChild(DOM.getFirstChild(DOM.getFirstChild(tb.getElement())),scrollerIndex),"display","");
      tb.updateCaptionSize(scrollerIndex);
      updateTabScroller();
    }
  }
 else   if (isClippedTabs() && DOM.eventGetTarget(event) == scrollerNext) {
    int tabs=tb.getTabCount();
    if (scrollerIndex + 1 <= tabs) {
      DOM.setStyleAttribute(DOM.getChild(DOM.getFirstChild(DOM.getFirstChild(tb.getElement())),scrollerIndex),"display","none");
      tb.updateCaptionSize(scrollerIndex);
      scrollerIndex++;
      updateTabScroller();
    }
  }
 else {
    super.onBrowserEvent(event);
  }
}
