{
  String oldWidth=this.width;
  this.width=width;
  if ("100%".equals(width)) {
    super.setWidth("");
  }
 else {
    super.setWidth(width);
  }
  if ((this.width != null && width == null) || (this.width == null && width != null) || !width.equals(oldWidth)) {
    client.runDescendentsLayout(this);
  }
}
