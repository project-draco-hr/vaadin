{
  VaadinSession application=createStubApplication();
  application.start(new SessionStartEvent(null,createConfigurationMock(),null));
  DefaultUIProvider uiProvider=new DefaultUIProvider();
  Class<? extends UI> uiClass=uiProvider.getUIClass(new UIClassSelectionEvent(createRequestMock(null)));
  assertEquals(MyUI.class,uiClass);
}
