{
  actions.clear();
  Iterator it=c.getChildIterator();
  while (it.hasNext()) {
    UIDL action=(UIDL)it.next();
    int[] modifiers=null;
    if (action.hasAttribute("mk"))     modifiers=action.getIntArrayAttribute("mk");
    ShortcutKeyCombination kc=new ShortcutKeyCombination(action.getIntAttribute("kc"),modifiers);
    String key=action.getStringAttribute("key");
    String caption=action.getStringAttribute("caption");
    actions.add(new ShortcutAction(key,kc,caption));
  }
}
