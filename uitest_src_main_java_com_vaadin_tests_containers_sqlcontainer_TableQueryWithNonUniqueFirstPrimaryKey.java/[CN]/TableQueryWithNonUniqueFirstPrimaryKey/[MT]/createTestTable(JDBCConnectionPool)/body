{
  Connection conn=null;
  try {
    conn=connectionPool.reserveConnection();
    Statement statement=conn.createStatement();
    try {
      statement.executeUpdate("DROP TABLE mytable2");
    }
 catch (    SQLException e) {
    }
    String q="CREATE TABLE mytable2 (id INTEGER GENERATED BY DEFAULT AS IDENTITY," + "  MYFIELD varchar(60)," + "  PFX varchar(1),"+ "  numero integer"+ ", PRIMARY KEY (pfx,numero));";
    statement.execute(q);
    statement.close();
    conn.commit();
  }
 catch (  SQLException e) {
    e.printStackTrace();
  }
 finally {
    connectionPool.releaseConnection(conn);
  }
}
