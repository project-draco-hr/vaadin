{
  if (filters.isEmpty()) {
    filteredItems=(ArrayList<BT>)allItems.clone();
  }
  for (  BeanItem<BT> item : beanToItem.values()) {
    addValueChangeListener(item,propertyId);
  }
  Filter f=new Filter(propertyId,filterString,ignoreCase,onlyMatchPrefix);
  filter(f);
  filters.add(f);
  fireItemSetChange();
}
