{
  ChildComponentContainer componentContainer=widgetToComponentContainer.remove(oldComponent);
  if (componentContainer == null) {
    return;
  }
  componentContainer.setPaintable(VPaintableMap.get(client).getPaintable(newComponent));
  client.unregisterPaintable(VPaintableMap.get(client).getPaintable(oldComponent));
  widgetToComponentContainer.put(newComponent,componentContainer);
}
