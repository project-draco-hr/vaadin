{
  this.client=client;
  id=uidl.getId();
  if (client.updateComponent(this,uidl,true))   return;
  immediate=uidl.getBooleanAttribute("immediate");
  if (uidl.hasAttribute("cols"))   setWidth(uidl.getStringAttribute("cols") + "em");
  if (uidl.hasAttribute("disabled"))   setEnabled(!uidl.getBooleanAttribute("disabled"));
 else   setEnabled(!uidl.getBooleanAttribute("readonly"));
  setVisible(!uidl.getBooleanAttribute("invisible"));
  setText(uidl.getStringVariable("text"));
}
