{
  MenuBar menubar=new MenuBar();
  menubar.setId("menubar");
  addComponent(menubar);
  menubar.addItem(textWithZwnj,null);
  Label label=new Label(textWithZwnj);
  label.setId("label");
  addComponent(label);
  TextField f=new TextField("Textfield",textWithZwnj);
  f.setId("textfield");
  addComponent(f);
}
