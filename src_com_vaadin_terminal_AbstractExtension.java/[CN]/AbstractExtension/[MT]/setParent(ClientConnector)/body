{
  Class<? extends ClientConnector> acceptedParentType=getAcceptedParentType();
  if (parent == null || acceptedParentType.isInstance(parent)) {
    super.setParent(parent);
  }
 else {
    throw new IllegalArgumentException(getClass().getName() + " can only be attached to parents of type " + acceptedParentType.getName()+ " but attach to "+ parent.getClass().getName()+ " was attempted.");
  }
}
