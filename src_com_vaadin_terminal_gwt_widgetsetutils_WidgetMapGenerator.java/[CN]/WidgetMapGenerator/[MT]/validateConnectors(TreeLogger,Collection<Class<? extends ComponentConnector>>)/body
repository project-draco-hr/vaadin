{
  Iterator<Class<? extends ComponentConnector>> iter=connectors.iterator();
  while (iter.hasNext()) {
    Class<? extends ComponentConnector> connectorClass=iter.next();
    Component annotation=connectorClass.getAnnotation(Component.class);
    if (!ClientConnector.class.isAssignableFrom(annotation.value())) {
      logger.log(Type.WARN,"Connector class " + annotation.value().getName() + " defined in @Component annotation is not a subclass of "+ ClientConnector.class.getName()+ ". The component connector "+ connectorClass.getName()+ " will not be included in the widgetset.");
      iter.remove();
    }
  }
}
