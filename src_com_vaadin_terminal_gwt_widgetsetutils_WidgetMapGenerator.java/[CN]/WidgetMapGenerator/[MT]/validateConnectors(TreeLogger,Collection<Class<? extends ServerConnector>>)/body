{
  Iterator<Class<? extends ServerConnector>> iter=connectors.iterator();
  while (iter.hasNext()) {
    Class<? extends ServerConnector> connectorClass=iter.next();
    Connect annotation=connectorClass.getAnnotation(Connect.class);
    if (!ClientConnector.class.isAssignableFrom(annotation.value())) {
      logger.log(Type.WARN,"Connector class " + annotation.value().getName() + " defined in @Connect annotation is not a subclass of "+ ClientConnector.class.getName()+ ". The component connector "+ connectorClass.getName()+ " will not be included in the widgetset.");
      iter.remove();
    }
  }
}
