{
  caption.updateCaptionWithoutOwner(uidl.getStringAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_CAPTION),uidl.hasAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_DISABLED),uidl.hasAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_DESCRIPTION),uidl.hasAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_ERROR_MESSAGE),uidl.getStringAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_ICON));
}
