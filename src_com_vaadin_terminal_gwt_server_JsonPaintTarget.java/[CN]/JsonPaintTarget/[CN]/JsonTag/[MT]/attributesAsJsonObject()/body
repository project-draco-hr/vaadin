{
  final StringBuilder buf=new StringBuilder();
  buf.append(startField());
  buf.append("{");
  for (final Iterator iter=attr.iterator(); iter.hasNext(); ) {
    final String element=(String)iter.next();
    buf.append(element);
    if (iter.hasNext()) {
      buf.append(",");
    }
  }
  buf.append(tag.variablesAsJsonObject());
  buf.append("}");
  return buf.toString();
}
