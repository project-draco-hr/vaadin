{
  try {
    ClientWidget annotation=class1.getAnnotation(ClientWidget.class);
    return annotation != null;
  }
 catch (  RuntimeException e) {
    if (e.getStackTrace()[0].getClassName().equals("org.glassfish.web.loader.WebappClassLoader")) {
      return true;
    }
 else {
      throw e;
    }
  }
}
