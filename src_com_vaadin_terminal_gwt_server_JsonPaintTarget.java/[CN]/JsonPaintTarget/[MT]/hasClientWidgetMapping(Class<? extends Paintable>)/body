{
  if (Root.class == class1) {
    return true;
  }
  try {
    return class1.isAnnotationPresent(ClientWidget.class);
  }
 catch (  NoClassDefFoundError e) {
    String stacktrace=getStacktraceString(e);
    if (stacktrace.contains("com.ibm.oti.reflect.AnnotationParser.parseClass")) {
      if (bytecodeContainsClientWidgetAnnotation(class1)) {
        return true;
      }
    }
 else {
      throw e;
    }
  }
catch (  LinkageError e) {
    String stacktrace=getStacktraceString(e);
    if (stacktrace.contains("org.jboss.modules.ModuleClassLoader.defineClass")) {
      if (bytecodeContainsClientWidgetAnnotation(class1)) {
        return true;
      }
    }
 else {
      throw e;
    }
  }
catch (  RuntimeException e) {
    if (e.getStackTrace()[0].getClassName().equals("org.glassfish.web.loader.WebappClassLoader")) {
      if (bytecodeContainsClientWidgetAnnotation(class1)) {
        return true;
      }
    }
 else {
      throw e;
    }
  }
  return false;
}
