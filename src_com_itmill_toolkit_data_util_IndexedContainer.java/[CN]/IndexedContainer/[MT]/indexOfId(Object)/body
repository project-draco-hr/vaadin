{
  if (filteredItemIds != null) {
    int index=0;
    if (itemId == null) {
      return -1;
    }
    try {
      for (Iterator i=filteredItemIds.iterator(); itemId.equals(i.next()); ) {
        index++;
      }
      return index;
    }
 catch (    NoSuchElementException e) {
      return -1;
    }
  }
  return itemIds.indexOf(itemId);
}
