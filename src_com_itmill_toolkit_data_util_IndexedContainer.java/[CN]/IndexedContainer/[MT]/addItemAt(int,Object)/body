{
  if (items.containsKey(newItemId)) {
    return null;
  }
  itemIds.add(index,newItemId);
  Hashtable t=new Hashtable();
  items.put(newItemId,t);
  addDefaultValues(t);
  if (filteredItemIds != null) {
    updateContainerFiltering();
  }
 else {
    fireContentsChange(index);
  }
  return getItem(newItemId);
}
