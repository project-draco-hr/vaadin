{
  for (int i=0; i < URIS.length; i++) {
    assertEquals(URIS[i],ApplicationConnection.addGetParameters(URIS[i],""));
    assertEquals(URIS_WITH_ABCD_PARAM[i],ApplicationConnection.addGetParameters(URIS[i],"a=b&c=d"));
    if (URIS_WITH_ABCD_PARAM_AND_FRAGMENT[i].length() > 0) {
      assertEquals(URIS_WITH_ABCD_PARAM_AND_FRAGMENT[i],ApplicationConnection.addGetParameters(URIS[i] + "#fragment","a=b&c=d"));
      assertEquals(URIS_WITH_ABCD_PARAM_AND_FRAGMENT[i].replace("#fragment","#"),ApplicationConnection.addGetParameters(URIS[i] + "#","a=b&c=d"));
    }
  }
}
