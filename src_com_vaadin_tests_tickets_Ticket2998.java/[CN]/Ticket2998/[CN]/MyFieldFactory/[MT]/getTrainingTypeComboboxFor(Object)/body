{
  ComboBox cb=workoutIdToCombobox.get(itemId);
  if (cb == null) {
    final ComboBox cb2=new ComboBox();
    cb2.addItem("value1");
    cb2.addItem("value2");
    cb2.addItem("value3");
    cb2.addItem("value4");
    cb2.setNewItemsAllowed(true);
    workoutIdToCombobox.put(itemId,cb2);
    cb=cb2;
  }
  return cb;
}
