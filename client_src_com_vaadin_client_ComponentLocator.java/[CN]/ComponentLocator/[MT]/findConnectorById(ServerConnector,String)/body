{
  SharedState state=root.getState();
  if (state instanceof ComponentState && id.equals(((ComponentState)state).id)) {
    return root;
  }
  for (  ServerConnector child : root.getChildren()) {
    ServerConnector found=findConnectorById(child,id);
    if (found != null) {
      return found;
    }
  }
  return null;
}
