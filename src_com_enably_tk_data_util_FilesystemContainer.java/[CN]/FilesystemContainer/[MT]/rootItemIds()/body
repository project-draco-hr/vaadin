{
  File[] f;
  if (roots.length == 1) {
    if (this.filter != null)     f=roots[0].listFiles(this.filter);
 else     f=roots[0].listFiles();
  }
 else {
    f=this.roots;
  }
  if (f == null)   return Collections.unmodifiableCollection(new LinkedList());
  List l=Arrays.asList(f);
  Collections.sort(l);
  return Collections.unmodifiableCollection(l);
}
