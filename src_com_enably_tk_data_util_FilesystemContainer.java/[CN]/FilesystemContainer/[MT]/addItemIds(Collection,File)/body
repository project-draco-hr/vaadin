{
  File[] l;
  if (this.filter != null)   l=f.listFiles(this.filter);
 else   l=f.listFiles();
  List ll=Arrays.asList(l);
  Collections.sort(ll);
  for (Iterator i=ll.iterator(); i.hasNext(); ) {
    File lf=(File)i.next();
    if (lf.isDirectory())     addItemIds(col,lf);
 else     col.add(lf);
  }
}
