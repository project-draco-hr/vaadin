{
  Token n;
  n=jj_consume_token(HASH);
  Condition d=conditionFactory.createIdCondition(n.image.substring(1));
  if (pred == null) {
{
      if (true)       return d;
    }
  }
 else {
{
      if (true)       return conditionFactory.createAndCondition(pred,d);
    }
  }
  throw new Error("Missing return statement in function");
}
