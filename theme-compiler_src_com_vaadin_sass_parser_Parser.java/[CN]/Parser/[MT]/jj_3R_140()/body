{
  if (jj_3R_153())   return true;
  if (jj_scan_token(COLON))   return true;
  Token xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_scan_token(1)) {
      jj_scanpos=xsp;
      break;
    }
  }
  xsp=jj_scanpos;
  if (jj_3R_154()) {
    jj_scanpos=xsp;
    if (jj_3R_155())     return true;
  }
  return false;
}
