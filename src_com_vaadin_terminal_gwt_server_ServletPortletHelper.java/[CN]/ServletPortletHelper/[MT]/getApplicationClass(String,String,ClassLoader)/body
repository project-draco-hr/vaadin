{
  if (applicationParameter == null) {
    verifyRootClass(rootParameter,classLoader);
    return Application.class;
  }
  try {
    return (Class<? extends Application>)classLoader.loadClass(applicationParameter);
  }
 catch (  final ClassNotFoundException e) {
    throw new ApplicationClassException("Failed to load application class: " + applicationParameter,e);
  }
}
