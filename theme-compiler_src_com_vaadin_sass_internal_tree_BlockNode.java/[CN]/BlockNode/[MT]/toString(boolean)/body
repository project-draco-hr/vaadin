{
  StringBuilder string=new StringBuilder();
  int i=0;
  for (  final String s : selectorList) {
    string.append(s);
    if (i != selectorList.size() - 1) {
      string.append(", ");
    }
    i++;
  }
  string.append(" {\n");
  for (  Node child : children) {
    if (indent) {
      string.append("\t");
    }
    string.append("\t" + child.toString() + "\n");
  }
  if (indent) {
    string.append("\t");
  }
  string.append("}");
  return string.toString();
}
