{
  if (currentDrag != null) {
    ApplicationConnection.getConsole().log("Drag operation interrupted");
    if (currentDropHandler != null) {
      currentDrag.setCurrentGwtEvent(null);
      currentDropHandler.dragLeave(currentDrag);
      currentDropHandler=null;
      serverCallback=null;
      visitId=-1;
    }
    currentDrag=null;
  }
}
