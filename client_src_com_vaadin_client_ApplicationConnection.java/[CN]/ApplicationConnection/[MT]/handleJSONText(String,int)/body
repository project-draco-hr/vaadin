{
  final Date start=new Date();
  final ValueMap json;
  try {
    json=parseJSONResponse(jsonText);
  }
 catch (  final Exception e) {
    endRequest();
    showCommunicationError(e.getMessage() + " - Original JSON-text:" + jsonText,statusCode);
    return;
  }
  VConsole.log("JSON parsing took " + (new Date().getTime() - start.getTime()) + "ms");
  if (isApplicationRunning()) {
    handleReceivedJSONMessage(start,jsonText,json);
  }
 else {
    if (!cssLoaded) {
      setApplicationRunning(true);
      handleWhenCSSLoaded(jsonText,json);
    }
 else {
      getLogger().warning("Ignored received message because application has already been stopped");
      return;
    }
  }
}
