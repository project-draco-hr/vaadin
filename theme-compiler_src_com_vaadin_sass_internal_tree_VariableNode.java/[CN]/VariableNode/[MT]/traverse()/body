{
  if (ArithmeticExpressionEvaluator.get().containsArithmeticalOperator(expr)) {
    replaceVariables(ScssStylesheet.getVariables());
    expr=ArithmeticExpressionEvaluator.get().evaluate(expr);
  }
 else {
    replaceVariables(ScssStylesheet.getVariables());
  }
  VariableNodeHandler.traverse(this);
}
