{
  try {
    UIDL child=uidl.getChildUIDL(0).getChildUIDL(0);
    if (child.hasAttribute("xmlns") && child.getStringAttribute("xmlns").equals("http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd")) {
      setContent(child.getChildString(0));
    }
 else {
      setContent("Terminals Label compoent can't handle this content type.");
    }
  }
 catch (  Exception e) {
    setContent(uidl.getChildString(0));
  }
  if (uidl.hasAttribute("caption"))   setCaption(uidl.getStringAttribute("caption"));
 else   caption.setVisible(false);
}
