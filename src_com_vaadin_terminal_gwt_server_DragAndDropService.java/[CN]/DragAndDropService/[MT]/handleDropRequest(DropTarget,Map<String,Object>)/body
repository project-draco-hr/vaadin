{
  DropHandler dropHandler=(dropTarget).getDropHandler();
  if (dropHandler == null) {
    System.err.println("DropTarget.getDropHandler() returned null for owner: " + dropTarget);
    return;
  }
  Transferable transferable=constructTransferable(dropTarget,variables);
  DragDropDetails dropData=constructDragDropDetails(dropTarget,variables);
  dropHandler.drop(transferable,dropData);
}
