{
  Map<String,Object> rawDragDropDetails=(Map<String,Object>)variables.get("evt");
  TargetDetails dropData=dropTarget.translateDragDropDetails(rawDragDropDetails);
  if (dropData == null) {
    dropData=new TargetDetailsImpl(rawDragDropDetails);
  }
  dropData.setData(DROPTARGET_KEY,dropTarget);
  return dropData;
}
