{
  Map<String,Object> rawDragDropDetails=(Map<String,Object>)variables.get("evt");
  DragDropDetails dropData=null;
  if (dropTarget instanceof DragDropDataTranslator) {
    dropData=((DragDropDataTranslator)dropTarget).translateDragDropDetails(rawDragDropDetails);
  }
  if (dropData == null) {
    dropData=new DragDropDetailsImpl(rawDragDropDetails);
  }
  dropData.put(DROPTARGET_KEY,dropTarget);
  return dropData;
}
