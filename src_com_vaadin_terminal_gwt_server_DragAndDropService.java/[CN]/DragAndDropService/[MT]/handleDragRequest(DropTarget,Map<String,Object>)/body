{
  lastVisitId=(Integer)variables.get("visitId");
  DropHandler dropHandler=(dropTarget).getDropHandler();
  if (!(dropHandler instanceof DragDropHandler)) {
    System.err.println("DragRequest could not be send to handler as DropHandle does not implement DragDropHandler");
    return;
  }
  DragDropHandler dragDropHandler=(DragDropHandler)dropHandler;
  Transferable transferable=constructTransferable(dropTarget,variables);
  DragDropDetails dragDropDetails=constructDragDropDetails(dropTarget,variables);
  dragDropHandler.handleDragRequest(constructDragRequest(variables,transferable),transferable,dragDropDetails);
}
