{
  LegacyWindow w=new LegacyWindow(getClass().getSimpleName());
  setMainWindow(w);
  w.getContent().setSizeFull();
  Embedded em=new Embedded();
  em.setType(Embedded.TYPE_BROWSER);
  em.setSource(new ExternalResource("../statictestfiles/ticket2095.html"));
  em.setId("MYIFRAME");
  em.setSizeFull();
  w.addComponent(em);
}
