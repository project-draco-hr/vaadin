{
  int pos=0;
  if (InsertPosition.ICON.equals(element)) {
    return pos;
  }
  if (icon != null) {
    pos++;
  }
  if (InsertPosition.CAPTION.equals(element)) {
    return pos;
  }
  if (captionText != null) {
    pos++;
  }
  if (InsertPosition.REQUIRED.equals(element)) {
    return pos;
  }
  if (requiredFieldIndicator != null) {
    pos++;
  }
  return pos;
}
