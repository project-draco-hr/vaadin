{
  final HeaderCell c=getHeaderCell(cid);
  if (!c.isEnabled() || getHeaderCell(index) != c) {
    setHeaderCell(index,c);
    if (c.getWidth() == -1) {
      if (initializedAndAttached) {
        initializedAndAttached=false;
        initialContentReceived=false;
        isNewBody=true;
      }
    }
  }
}
