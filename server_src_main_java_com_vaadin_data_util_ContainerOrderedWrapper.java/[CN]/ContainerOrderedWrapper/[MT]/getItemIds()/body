{
  if (ordered) {
    return ((Container.Ordered)container).getItemIds();
  }
 else   if (first == null) {
    return new ArrayList<Object>();
  }
 else {
    List<Object> itemIds=new ArrayList<Object>();
    itemIds.add(first);
    Object current=first;
    while (next.containsKey(current)) {
      current=next.get(current);
      itemIds.add(current);
    }
    return itemIds;
  }
}
