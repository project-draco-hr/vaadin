{
  Element e=createElement("v-label",content,caption);
  Label l=(Label)ctx.readDesign(e);
  if (content != null) {
    assertEquals("The label has wrong text content.",content,l.getValue());
  }
 else {
    assertTrue("The label has wrong text content.",l.getValue() == null || "".equals(l.getValue()));
  }
  if (caption != null) {
    assertEquals("The label has wrong caption.",caption,l.getCaption());
  }
 else {
    assertTrue("The label has wrong caption.",l.getCaption() == null || "".equals(l.getCaption()));
  }
}
