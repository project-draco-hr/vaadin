{
  try {
    Type stateType=getStateType(this);
    Object stateInstance=stateType.createInstance();
    return (SharedState)stateInstance;
  }
 catch (  NoDataException e) {
    throw new IllegalStateException("There is no information about the state for " + Util.getSimpleName(this) + ". Did you remember to compile the right widgetset?",e);
  }
}
