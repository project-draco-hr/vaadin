{
  openTestURL();
  selectMenuPath("Component","Columns","Column2","Freeze");
  WebElement cell=getDriver().findElement(By.xpath("//tbody[contains(@class, 'v-escalator-body')]/tr[1]/td[1]"));
  assertTrue(cell.getAttribute("class").contains("frozen"));
  cell=getDriver().findElement(By.xpath("//tbody[contains(@class, 'v-escalator-body')]/tr[1]/td[2]"));
  assertTrue(cell.getAttribute("class").contains("frozen"));
}
