{
  openTestURL();
  selectMenuPath("Component","Column groups","Add group row");
  selectMenuPath("Component","Column groups","Column group row 1","Group Column 0 & 1");
  selectMenuPath("Component","Column groups","Column group row 1","Group Column 1 & 2");
  assertTrue(getLogRow(0).contains("Exception occured, java.lang.IllegalArgumentExceptionColumn Column1 already belongs to another group."));
}
