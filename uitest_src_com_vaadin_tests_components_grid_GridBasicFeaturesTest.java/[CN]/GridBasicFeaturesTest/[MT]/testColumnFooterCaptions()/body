{
  openTestURL();
  String footerCellPath="//tfoot[contains(@class, 'v-escalator-footer')]" + "//td[contains(@class, 'v-escalator-cell')]";
  assertEquals(0,getDriver().findElements(By.xpath(footerCellPath)).size());
  selectMenuPath("Component","Footers","Visible");
  List<WebElement> cells=getDriver().findElements(By.xpath(footerCellPath));
  assertEquals(10,cells.size());
  assertEquals("Footer 0",cells.get(0).getText());
  assertEquals("Footer 1",cells.get(1).getText());
  assertEquals("Footer 2",cells.get(2).getText());
}
