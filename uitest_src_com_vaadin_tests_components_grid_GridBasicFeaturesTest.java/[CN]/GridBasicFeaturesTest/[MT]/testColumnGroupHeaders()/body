{
  openTestURL();
  selectMenuPath("Component","Headers","Visible");
  String headerCellPath="//thead[contains(@class, 'v-escalator-header')]//th";
  assertEquals(0,getDriver().findElements(By.xpath(headerCellPath)).size());
  selectMenuPath("Component","Column groups","Add group row");
  assertEquals(10,getDriver().findElements(By.xpath(headerCellPath)).size());
  selectMenuPath("Component","Column groups","Column group row 1","Group Column 0 & 1");
  List<WebElement> cells=getDriver().findElements(By.xpath(headerCellPath));
  assertEquals("Column 0 & 1",cells.get(0).getText());
  assertEquals("Column 0 & 1",cells.get(1).getText());
}
