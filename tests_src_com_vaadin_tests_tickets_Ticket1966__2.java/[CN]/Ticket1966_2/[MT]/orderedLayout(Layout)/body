{
  Panel p=new Panel("OrderedLayout");
  p.setWidth("500px");
  p.setHeight("500px");
  p.getContent().setWidth("100%");
  layout.addComponent(p);
  AbstractOrderedLayout ol=new VerticalLayout();
  ol.setWidth("100%");
  addButtons(ol);
  p.addComponent(ol);
  ol=new HorizontalLayout();
  ol.setHeight("200px");
  addButtons(ol);
  p.addComponent(ol);
}
