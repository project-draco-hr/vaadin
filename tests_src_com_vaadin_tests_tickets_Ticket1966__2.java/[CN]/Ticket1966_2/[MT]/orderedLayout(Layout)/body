{
  Panel p=new Panel("OrderedLayout");
  p.setWidth(500);
  p.setHeight(500);
  p.getLayout().setWidth("100%");
  layout.addComponent(p);
  OrderedLayout ol=new OrderedLayout();
  ol.setWidth("100%");
  addButtons(ol);
  p.addComponent(ol);
  ol=new OrderedLayout(OrderedLayout.ORIENTATION_HORIZONTAL);
  ol.setHeight(200);
  addButtons(ol);
  p.addComponent(ol);
}
