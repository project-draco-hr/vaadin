{
  Button b=new Button("Reindeer");
  Layout layout=new VerticalLayout();
  layout.addComponent(b);
  b=new Button("important");
  b.addStyleName("important");
  layout.addComponent(b);
  b=new Button("More important");
  b.setPrimaryStyleName("my-button");
  layout.addComponent(b);
  setContent(layout);
}
