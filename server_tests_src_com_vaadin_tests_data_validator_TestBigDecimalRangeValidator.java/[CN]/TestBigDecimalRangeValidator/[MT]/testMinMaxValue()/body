{
  assertTrue("Didn't accept valid value",minMaxValidator.isValid(new BigDecimal(10.5)));
  assertTrue("Didn't accept valid value",minMaxValidator.isValid(new BigDecimal(100.5)));
  assertFalse("Accepted too small value",minMaxValidator.isValid(new BigDecimal(10.4)));
  assertFalse("Accepted too large value",minMaxValidator.isValid(new BigDecimal(100.6)));
}
