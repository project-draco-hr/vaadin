{
  final Class<? extends Paintable> classType=resolveWidgetType(uidl,conf);
  if (classType == null || classType == VUnknownComponent.class) {
    String serverSideName=conf.getUnknownServerClassNameByEncodedTagName(uidl.getTag());
    return new VUnknownComponent(serverSideName);
  }
  return map.instantiate(classType);
}
