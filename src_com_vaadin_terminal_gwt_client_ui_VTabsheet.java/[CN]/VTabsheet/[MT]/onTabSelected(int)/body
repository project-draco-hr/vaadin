{
  if (disabled || waitingForResponse) {
    return false;
  }
  final Object tabKey=tabKeys.get(tabIndex);
  if (disabledTabKeys.contains(tabKey)) {
    return false;
  }
  if (client != null && activeTabIndex != tabIndex) {
    tb.selectTab(tabIndex);
    if (focusedTab != null) {
      focusedTab=tb.getTab(tabIndex);
    }
    addStyleDependentName("loading");
    Widget currentlyDisplayedWidget=tp.getWidget(tp.getVisibleWidget());
    currentlyDisplayedWidget.getElement().getParentElement().getStyle().setVisibility(Visibility.HIDDEN);
    client.updateVariable(id,"selected",tabKeys.get(tabIndex).toString(),true);
    waitingForResponse=true;
    return true;
  }
  return false;
}
