{
  super.setHeight(newHeight);
  if (newHeight != null && !newHeight.equals("")) {
    if (!newHeight.equals(height)) {
      height=newHeight;
      if (newHeight.indexOf("%") > 0) {
        relativeHeight=true;
        DOM.setStyleAttribute(getElement(),"overflow","hidden");
      }
 else {
        relativeHeight=false;
        DOM.setStyleAttribute(getElement(),"overflow","");
      }
      setInternalHeight();
    }
  }
 else {
    if (newHeight != null) {
      DOM.setStyleAttribute(getElement(),"overflow","");
      DOM.setStyleAttribute(DOM.getFirstChild(margin),"height","");
      newHeight=null;
      relativeHeight=false;
    }
  }
}
