{
  final int ITEMS_PER_PAGE=10;
  WebElement dropDownComboBox=driver.findElement(By.className("v-filterselect-input"));
  dropDownComboBox.sendKeys(Keys.DOWN);
  for (int i=0; i < ITEMS_PER_PAGE + 1; i++) {
    dropDownComboBox.sendKeys(Keys.DOWN);
  }
  dropDownComboBox.sendKeys(Keys.UP);
  String expected="item " + (ITEMS_PER_PAGE - 1);
  List<WebElement> items=driver.findElements(By.className("gwt-MenuItem-selected"));
  String actual=items.get(0).getText();
  Assert.assertEquals(expected,actual);
}
