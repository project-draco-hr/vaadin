{
  final String q="SELECT DISTINCT(" + Resource.PROPERTY_ID_CATEGORY + ") FROM "+ Resource.TABLE+ " ORDER BY "+ Resource.PROPERTY_ID_CATEGORY;
  try {
    return new QueryContainer(q,connection,ResultSet.TYPE_SCROLL_INSENSITIVE,ResultSet.CONCUR_READ_ONLY);
  }
 catch (  final SQLException e) {
    throw new RuntimeException(e);
  }
}
