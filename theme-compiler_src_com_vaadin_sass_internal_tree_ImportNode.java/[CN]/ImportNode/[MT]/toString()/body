{
  StringBuilder builder=new StringBuilder("@import ");
  if (isURL) {
    builder.append("url(").append(uri).append(")");
  }
 else {
    builder.append("\"").append(uri).append("\"");
  }
  if (hasMediaQueries()) {
    for (int i=0; i < ml.getLength(); i++) {
      builder.append(" ").append(ml.item(i));
    }
  }
  builder.append(";");
  return builder.toString();
}
