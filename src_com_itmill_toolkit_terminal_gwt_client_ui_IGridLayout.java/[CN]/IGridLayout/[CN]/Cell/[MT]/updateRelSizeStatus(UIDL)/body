{
  if (uidl != null && !uidl.getBooleanAttribute("cached")) {
    if (uidl.hasAttribute("height") && uidl.getStringAttribute("height").contains("%")) {
      relHeight=true;
    }
 else {
      relHeight=false;
    }
    if (uidl.hasAttribute("width")) {
      widthCanAffectHeight=relWidth=uidl.getStringAttribute("width").contains("%");
      if (uidl.hasAttribute("height")) {
        widthCanAffectHeight=false;
      }
    }
 else {
      widthCanAffectHeight=true;
      relWidth=false;
    }
  }
}
