{
  if (uidl != null && !uidl.getBooleanAttribute("cached")) {
    if (uidl.hasAttribute("height") && uidl.getStringAttribute("height").contains("%")) {
      relHeight=true;
    }
 else {
      relHeight=false;
    }
    if (uidl.hasAttribute("width") && uidl.getStringAttribute("width").contains("%")) {
      relWidth=true;
    }
 else {
      relWidth=false;
    }
  }
}
