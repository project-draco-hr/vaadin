{
  assertTrue("Foo default renderer should be a type of ObjectRenderer",foo.getRenderer() instanceof ObjectRenderer);
  assertTrue("Bar default renderer should be a type of ObjectRenderer",bar.getRenderer() instanceof ObjectRenderer);
  assertNull(bar.getConverter());
  assertTrue("Baz default renderer should be a type of ObjectRenderer",baz.getRenderer() instanceof ObjectRenderer);
  assertNull(baz.getConverter());
}
