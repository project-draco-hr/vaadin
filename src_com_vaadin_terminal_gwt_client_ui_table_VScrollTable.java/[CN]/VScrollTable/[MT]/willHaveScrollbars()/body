{
  if (isDynamicHeight()) {
    if (pageLength < totalRows) {
      return true;
    }
  }
 else {
    if (scrollBody == null) {
      return false;
    }
    int fakeheight=(int)Math.round(scrollBody.getRowHeight() * totalRows);
    int availableHeight=scrollBodyPanel.getElement().getPropertyInt("clientHeight");
    if (fakeheight > availableHeight) {
      return true;
    }
  }
  return false;
}
