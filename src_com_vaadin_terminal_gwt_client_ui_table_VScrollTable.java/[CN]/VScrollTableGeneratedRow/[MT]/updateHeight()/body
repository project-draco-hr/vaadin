{
  setContainerHeight();
  if (initializedAndAttached) {
    updatePageLength();
  }
  if (!rendering) {
    Scheduler.get().scheduleDeferred(new Command(){
      @Override public void execute(){
        Util.runWebkitOverflowAutoFix(scrollBodyPanel.getElement());
      }
    }
);
  }
  triggerLazyColumnAdjustment(false);
  setProperTabIndex();
}
