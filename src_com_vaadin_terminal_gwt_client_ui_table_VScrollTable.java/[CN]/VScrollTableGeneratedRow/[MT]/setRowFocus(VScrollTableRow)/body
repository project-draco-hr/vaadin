{
  if (!isSelectable()) {
    return false;
  }
  if (focusedRow != null && focusedRow != row) {
    focusedRow.removeStyleName(CLASSNAME_SELECTION_FOCUS);
  }
  if (row != null) {
    row.addStyleName(CLASSNAME_SELECTION_FOCUS);
    if (row == focusedRow) {
      return false;
    }
    focusedRow=row;
    ensureRowIsVisible(row);
    return true;
  }
  return false;
}
