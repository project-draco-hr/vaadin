{
  if (notime) {
    Date d=new Date(start.getTime());
    d.setSeconds(0);
    d.setMinutes(0);
    this.start=d;
    if (end != null) {
      d=new Date(end.getTime());
      d.setSeconds(0);
      d.setMinutes(0);
      this.end=d;
    }
 else {
      end=start;
    }
  }
 else {
    this.start=start;
    this.end=end;
  }
  this.title=title;
  this.description=description;
  this.notime=notime;
}
