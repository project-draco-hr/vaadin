{
  property=new ObjectProperty<Integer>(123);
  field.setConverter(new ConvertTo42());
  field.setPropertyDataSource(property);
  field.addValidator(new LegacyRangeValidator<Integer>("Incorrect value",Integer.class,42,42));
  field.setValue("a");
  field.setValue("42");
  property.setValue(42);
  property.setValue(null);
  field.validate();
  field.setValue(null);
}
