{
  final LegacyWindow mainWindow=new LegacyWindow("Forumtests Application");
  setMainWindow(mainWindow);
  String mathml="<math mode='display' xmlns='http://www.w3.org/1998/Math/MathML'>" + "<mrow>" + "    <msup>"+ "      <mi>x</mi>"+ "      <mn>2</mn>"+ "    </msup>"+ "    <msup>"+ "      <mi>c</mi>"+ "      <mn>2</mn>"+ "    </msup>"+ "  </mrow>"+ "</math>";
  Label mathLabel=new Label(mathml,ContentMode.XML);
  mainWindow.addComponent(mathLabel);
}
