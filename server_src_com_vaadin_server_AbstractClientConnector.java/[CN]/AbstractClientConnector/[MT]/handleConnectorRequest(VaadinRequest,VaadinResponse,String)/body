{
  DownloadStream stream=null;
  String[] parts=path.split("/",2);
  String key=parts[0];
  getSession().lock();
  try {
    ConnectorResource resource=(ConnectorResource)getResource(key);
    if (resource == null) {
      return false;
    }
    stream=resource.getStream();
  }
  finally {
    getSession().unlock();
  }
  stream.writeResponse(request,response);
  return true;
}
