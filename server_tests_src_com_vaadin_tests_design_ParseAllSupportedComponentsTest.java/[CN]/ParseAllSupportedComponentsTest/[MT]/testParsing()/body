{
  try {
    DesignContext ctx=Design.read(new FileInputStream("server/tests/src/com/vaadin/tests/design/all-components.html"),null);
    assertNotNull("The returned design context can not be null",ctx);
    assertNotNull("the component root can not be null",ctx.getRootComponent());
  }
 catch (  FileNotFoundException e) {
    e.printStackTrace();
    fail("Template parsing threw exception");
  }
}
