{
  if (requestType == RequestType.UIDL && !isRepaintAll(request)) {
    if (request.getRequestedSessionId() == null) {
      criticalNotification(request,response,getSystemMessages().getCookiesDisabledCaption(),getSystemMessages().getCookiesDisabledMessage(),null,getSystemMessages().getCookiesDisabledURL());
      return false;
    }
  }
  return true;
}
