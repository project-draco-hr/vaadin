{
  long currentTime=System.currentTimeMillis();
  HashSet keys=new HashSet();
  keys.addAll(this.transformerSpool.keySet());
  for (Iterator i=keys.iterator(); i.hasNext(); ) {
    UIDLTransformerType type=(UIDLTransformerType)i.next();
    TransformerList l=(TransformerList)this.transformerSpool.get(type);
    if (l != null) {
      if (l.lastUsed > 0 && l.lastUsed < (currentTime - this.cacheTime)) {
        if (webAdapterServlet.isDebugMode()) {
          Log.info("Removed transformer: " + type + " Not used since "+ new Date(l.lastUsed));
        }
        this.transformerSpool.remove(type);
      }
    }
  }
}
