{
  for (int i=0; i < coverList.size(); i++) {
    HashMap<String,String> set=coverList.get(i);
    try {
      addCover(uidlId,set.get("caption"),set.get("icon"));
    }
 catch (    Exception e) {
    }
  }
  flashInited=true;
  if (_selected != null) {
    selectCover(uidlId,_selected.toString());
  }
}
