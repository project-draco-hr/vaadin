{
  int[] rgb=calculateRgb(hsl);
  LexicalUnitImpl hslParam=hsl.getParameters();
  LexicalUnitImpl rgbParams=createRgbParameters(rgb[0],rgb[1],rgb[2],hslParam.getLineNumber(),hslParam.getColumnNumber(),hslParam.getPreviousLexicalUnit());
  return LexicalUnitImpl.createFunction(hsl.getLineNumber(),hsl.getColumnNumber(),hsl.getPreviousLexicalUnit(),"rgb",rgbParams);
}
