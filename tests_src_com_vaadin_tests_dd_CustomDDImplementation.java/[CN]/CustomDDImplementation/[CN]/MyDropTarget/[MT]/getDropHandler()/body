{
  return new DragDropHandler(){
    public void handleDragRequest(    DragRequest dragRequest,    Transferable transferable,    DragDropDetails dragDropDetails){
      DragEventType type=dragRequest.getType();
switch (type) {
case ENTER:
        if (transferable.getDataFlawors().contains("Foo")) {
          dragRequest.getResponseData().put("valueFor","clientSideCallBack");
        }
      break;
case OVER:
    break;
case LEAVE:
  break;
default :
break;
}
}
public boolean drop(Transferable transferable,DragDropDetails dropDetails){
return true;
}
}
;
}
