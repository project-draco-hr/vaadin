{
  try {
    Widget w=Util.findWidget(element,null);
    if (w == null) {
      return null;
    }
    while (!(w instanceof VHasDropHandler) || !isDropEnabled((VHasDropHandler)w)) {
      w=w.getParent();
      if (w == null) {
        break;
      }
    }
    if (w == null) {
      return null;
    }
 else {
      VDropHandler dh=((VHasDropHandler)w).getDropHandler();
      return dh;
    }
  }
 catch (  Exception e) {
    return null;
  }
}
