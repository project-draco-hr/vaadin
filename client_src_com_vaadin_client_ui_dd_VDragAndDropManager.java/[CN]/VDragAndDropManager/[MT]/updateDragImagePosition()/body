{
  if (currentDrag.getCurrentGwtEvent() != null && dragElement != null) {
    Style style=dragElement.getStyle();
    int clientY=WidgetUtil.getTouchOrMouseClientY(currentDrag.getCurrentGwtEvent());
    int clientX=WidgetUtil.getTouchOrMouseClientX(currentDrag.getCurrentGwtEvent());
    style.setTop(clientY,Unit.PX);
    style.setLeft(clientX,Unit.PX);
  }
}
