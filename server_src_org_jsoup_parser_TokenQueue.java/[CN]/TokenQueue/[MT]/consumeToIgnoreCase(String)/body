{
  int start=pos;
  String first=seq.substring(0,1);
  boolean canScan=first.toLowerCase().equals(first.toUpperCase());
  while (!isEmpty()) {
    if (matches(seq)) {
      break;
    }
    if (canScan) {
      int skip=queue.indexOf(first,pos) - pos;
      if (skip == 0) {
        pos++;
      }
 else       if (skip < 0) {
        pos=queue.length();
      }
 else {
        pos+=skip;
      }
    }
 else {
      pos++;
    }
  }
  String data=queue.substring(start,pos);
  return data;
}
