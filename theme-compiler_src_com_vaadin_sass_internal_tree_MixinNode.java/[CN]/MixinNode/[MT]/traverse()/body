{
  try {
    Map<String,VariableNode> variableScope=ScssStylesheet.openVariableScope();
    replaceVariables(ScssStylesheet.getVariables());
    replaceVariablesForChildren();
    MixinNodeHandler.traverse(this);
    ScssStylesheet.closeVariableScope(variableScope);
  }
 catch (  Exception e) {
    Logger.getLogger(MixinNode.class.getName()).log(Level.SEVERE,null,e);
  }
}
