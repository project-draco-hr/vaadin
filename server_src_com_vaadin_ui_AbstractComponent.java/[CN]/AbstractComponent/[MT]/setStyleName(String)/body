{
  if (style == null || "".equals(style)) {
    getState().styles=null;
    return;
  }
  if (getState().styles == null) {
    getState().styles=new ArrayList<String>();
  }
  List<String> styles=getState().styles;
  styles.clear();
  String[] styleParts=style.split(" +");
  for (  String part : styleParts) {
    if (part.length() > 0) {
      styles.add(part);
    }
  }
}
