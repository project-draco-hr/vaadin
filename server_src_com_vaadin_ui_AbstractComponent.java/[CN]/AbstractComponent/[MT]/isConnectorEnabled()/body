{
  if (!isVisible()) {
    return false;
  }
 else   if (!isEnabled()) {
    return false;
  }
 else   if (!super.isConnectorEnabled()) {
    return false;
  }
 else   if ((getParent() instanceof SelectiveRenderer) && !((SelectiveRenderer)getParent()).isRendered(this)) {
    return false;
  }
 else {
    return true;
  }
}
