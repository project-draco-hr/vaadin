{
  if (style == null || "".equals(style)) {
    return;
  }
  if (style.contains(" ")) {
    for (    String realStyle : style.split(" ")) {
      addStyleName(realStyle);
    }
    return;
  }
  if (getState().styles == null) {
    getState().styles=new ArrayList<String>();
  }
  List<String> styles=getState().styles;
  if (!styles.contains(style)) {
    styles.add(style);
  }
}
