{
  if (focusedTab == null && event.getSource() instanceof Tab) {
    focusedTab=(Tab)event.getSource();
    if (client.hasEventListeners(this,EventId.FOCUS)) {
      client.updateVariable(id,EventId.FOCUS,"",true);
    }
    if (focusedTab.hasTooltip()) {
      focusedTab.setAssistiveDescription(getApplicationConnection().getVTooltip().getUniqueId());
      getApplicationConnection().getVTooltip().showAssistive(focusedTab.getTooltipInfo());
    }
  }
}
