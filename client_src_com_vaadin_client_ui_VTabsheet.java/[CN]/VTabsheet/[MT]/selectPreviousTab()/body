{
  int newTabIndex=activeTabIndex;
  do {
    newTabIndex--;
  }
 while (newTabIndex >= 0 && !onTabSelected(newTabIndex));
  if (newTabIndex >= 0) {
    activeTabIndex=newTabIndex;
    if (isScrolledTabs()) {
      while (!tb.getTab(activeTabIndex).isVisible()) {
        scrollerIndex=tb.scrollLeft(scrollerIndex);
      }
      updateTabScroller();
    }
  }
}
