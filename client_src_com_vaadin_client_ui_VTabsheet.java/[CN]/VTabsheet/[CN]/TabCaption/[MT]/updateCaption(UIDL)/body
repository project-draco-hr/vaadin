{
  if (uidl.hasAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_DESCRIPTION) || uidl.hasAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_ERROR_MESSAGE)) {
    setTooltipInfo(new TooltipInfo(uidl.getStringAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_DESCRIPTION),uidl.getStringAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_ERROR_MESSAGE)));
  }
 else {
    setTooltipInfo(null);
  }
  boolean ret=updateCaptionWithoutOwner(uidl.getStringAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_CAPTION),uidl.hasAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_DISABLED),uidl.hasAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_DESCRIPTION),uidl.hasAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_ERROR_MESSAGE),uidl.getStringAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_ICON),uidl.getStringAttribute(TabsheetBaseConstants.ATTRIBUTE_TAB_ICON_ALT));
  setClosable(uidl.hasAttribute("closable"));
  return ret;
}
