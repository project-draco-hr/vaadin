{
  setDebug(true);
  openTestURL();
  $(ButtonElement.class).caption("Select on server").first().click();
  assertTrue("Row should be selected.",$(LegacyGridElement.class).first().getRow(0).isSelected());
  $(ButtonElement.class).caption("Deselect on server").first().click();
  assertFalse("Row should not be selected.",$(LegacyGridElement.class).first().getRow(0).isSelected());
  assertNoErrorNotifications();
}
