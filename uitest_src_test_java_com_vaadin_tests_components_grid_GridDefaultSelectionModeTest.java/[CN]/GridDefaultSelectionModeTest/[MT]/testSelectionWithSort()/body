{
  setDebug(true);
  openTestURL();
  GridElement grid=$(LegacyGridElement.class).first();
  grid.getCell(0,0).click();
  GridCellElement header=grid.getHeaderCell(0,1);
  header.click();
  header.click();
  assertTrue("Row should be selected.",grid.getRow(1).isSelected());
  assertNoErrorNotifications();
}
