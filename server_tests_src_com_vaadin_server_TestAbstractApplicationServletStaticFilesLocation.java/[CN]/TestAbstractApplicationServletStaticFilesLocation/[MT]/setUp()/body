{
  super.setUp();
  servlet=new VaadinServlet();
  Field f=VaadinServlet.class.getDeclaredField("servletService");
  f.setAccessible(true);
  VaadinServletService service=new VaadinServletService(servlet,new DefaultDeploymentConfiguration(servlet.getClass(),new Properties()));
  service.init();
  f.set(servlet,service);
}
