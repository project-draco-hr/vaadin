{
  super.setUp();
  servlet=new VaadinServlet();
  Field f=VaadinServlet.class.getDeclaredField("applicationProperties");
  f.setAccessible(true);
  f.set(servlet,new Properties());
  getStaticFilesLocationMethod=VaadinServlet.class.getDeclaredMethod("getStaticFilesLocation",new Class[]{javax.servlet.http.HttpServletRequest.class});
  getStaticFilesLocationMethod.setAccessible(true);
}
