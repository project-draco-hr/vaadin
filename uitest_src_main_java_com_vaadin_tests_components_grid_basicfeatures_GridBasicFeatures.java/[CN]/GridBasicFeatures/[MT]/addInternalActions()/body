{
  createClickAction("Update column order without updating client","Internals",new Command<LegacyGrid,Void>(){
    @Override public void execute(    LegacyGrid grid,    Void value,    Object data){
      List<Column> columns=grid.getColumns();
      grid.setColumnOrder(columns.get(1).getPropertyId(),columns.get(0).getPropertyId());
      grid.getUI().getConnectorTracker().markClean(grid);
    }
  }
,null);
}
