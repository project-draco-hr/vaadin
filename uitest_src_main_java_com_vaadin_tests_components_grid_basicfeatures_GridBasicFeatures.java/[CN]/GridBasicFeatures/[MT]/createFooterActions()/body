{
  createCategory("Footer",null);
  createBooleanAction("Visible","Footer",false,new Command<LegacyGrid,Boolean>(){
    @Override public void execute(    LegacyGrid grid,    Boolean value,    Object data){
      grid.setFooterVisible(value);
    }
  }
);
  createClickAction("Prepend row","Footer",new Command<LegacyGrid,Object>(){
    @Override public void execute(    LegacyGrid grid,    Object value,    Object data){
      grid.prependFooterRow();
    }
  }
,null);
  createClickAction("Append row","Footer",new Command<LegacyGrid,Object>(){
    @Override public void execute(    LegacyGrid grid,    Object value,    Object data){
      grid.appendFooterRow();
    }
  }
,null);
  createClickAction("Remove top row","Footer",new Command<LegacyGrid,Object>(){
    @Override public void execute(    LegacyGrid grid,    Object value,    Object data){
      grid.removeFooterRow(0);
    }
  }
,null);
  createClickAction("Remove bottom row","Footer",new Command<LegacyGrid,Object>(){
    @Override public void execute(    LegacyGrid grid,    Object value,    Object data){
      grid.removeFooterRow(grid.getFooterRowCount() - 1);
    }
  }
,null);
}
