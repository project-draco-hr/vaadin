{
  registerRpc(EditorRowClientRpc.class,new EditorRowClientRpc(){
    @Override public void bind(    int rowIndex){
      serverInitiated=true;
      GridConnector.this.getWidget().getEditorRow().editRow(rowIndex);
    }
    @Override public void discard(    int rowIndex){
      serverInitiated=true;
      GridConnector.this.getWidget().getEditorRow().discard();
    }
    @Override public void cancel(    int rowIndex){
      serverInitiated=true;
      GridConnector.this.getWidget().getEditorRow().cancel();
    }
    @Override public void confirmBind(){
      endRequest();
    }
    @Override public void confirmCommit(){
      endRequest();
    }
  }
);
}
