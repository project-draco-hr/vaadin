{
  registerRpc(EditorRowClientRpc.class,new EditorRowClientRpc(){
    @Override public void bind(    int rowIndex){
      serverInitiated=true;
      GridConnector.this.getWidget().editRow(rowIndex);
    }
    @Override public void cancel(    int rowIndex){
      serverInitiated=true;
      GridConnector.this.getWidget().cancelEditorRow();
    }
    @Override public void confirmBind(){
      endRequest();
    }
    @Override public void confirmSave(){
      endRequest();
    }
  }
);
}
