{
  VButtonState state=GWT.create(VButtonState.class);
  JSONArray jsonHeight=(JSONArray)jsonValue.get("height");
  state.setHeight((String)JsonDecoder.convertValue(jsonHeight,idMapper));
  JSONArray jsonWidth=(JSONArray)jsonValue.get("width");
  state.setWidth((String)JsonDecoder.convertValue(jsonWidth,idMapper));
  JSONArray jsonDisableOnClick=(JSONArray)jsonValue.get("disableOnClick");
  state.setDisableOnClick((Boolean)JsonDecoder.convertValue(jsonDisableOnClick,idMapper));
  JSONArray jsonClickShortcutKeyCode=(JSONArray)jsonValue.get("clickShortcutKeyCode");
  state.setClickShortcutKeyCode((Integer)JsonDecoder.convertValue(jsonClickShortcutKeyCode,idMapper));
  return state;
}
