{
  VPaintableWidget paintable=VPaintableMap.get(client).getPaintable(this);
  if (paintable.isUndefinedWidth()) {
    int w=Util.getRequiredWidth(this);
    if ((!initDone || currentPage + 1 < 0) && suggestionPopupMinWidth > w) {
      setWidth(suggestionPopupMinWidth + "px");
    }
    if (!tb.getElement().getStyle().getWidth().endsWith("px")) {
      tb.setWidth((tb.getOffsetWidth() - selectedItemIcon.getOffsetWidth()) + "px");
    }
  }
}
