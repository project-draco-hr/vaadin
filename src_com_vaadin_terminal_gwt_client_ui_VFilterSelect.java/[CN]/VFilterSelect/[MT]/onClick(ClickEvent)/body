{
  if (enabled && !readonly) {
    if (!suggestionPopup.isJustClosed()) {
      filterOptions(-1,"");
      popupOpenerClicked=true;
      lastFilter="";
    }
 else     if (selectedOptionKey == null) {
      tb.setText(inputPrompt);
      prompting=true;
    }
    DOM.eventPreventDefault(DOM.eventGetCurrentEvent());
    tb.setFocus(true);
    tb.selectAll();
  }
}
