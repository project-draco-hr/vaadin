{
switch (keyCode) {
case KeyboardListener.KEY_DOWN:
case KeyboardListener.KEY_UP:
case KeyboardListener.KEY_PAGEDOWN:
case KeyboardListener.KEY_PAGEUP:
    if (suggestionPopup.isAttached()) {
      break;
    }
 else {
      filterOptions(-1,"");
      lastFilter="";
      tb.selectAll();
      break;
    }
}
}
