{
  int r=0;
  if (o1 != null && o2 != null) {
    if ((o1 instanceof Boolean) && (o2 instanceof Boolean)) {
      if (o1.equals(o2)) {
        r=0;
      }
 else       if (((Boolean)o1).booleanValue()) {
        return 1;
      }
 else {
        return -1;
      }
    }
 else {
      r=((Comparable)o1).compareTo(o2);
    }
  }
 else   if (o1 == o2) {
    r=0;
  }
 else {
    if (o1 == null) {
      r=-1;
    }
 else {
      r=1;
    }
  }
  return r;
}
