{
  if (css == null) {
    throw new IllegalArgumentException("Cannot inject null CSS string");
  }
  if (!stringInjections.isEmpty() && injectedStringPointer > 0) {
    String lastInjection=stringInjections.get(injectedStringPointer - 1);
    if (lastInjection.equals(css.trim())) {
      return;
    }
  }
  stringInjections.add(css.trim());
  ui.markAsDirty();
}
