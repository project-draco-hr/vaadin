{
  if (proxySession != null) {
    return;
  }
  for (int i=0; i < 10; i++) {
    try {
      createProxy(getProxyPort());
      break;
    }
 catch (    JSchException e) {
      try {
        sleep(500);
      }
 catch (      InterruptedException e1) {
      }
      if (i == 9) {
        throw new RuntimeException("All 10 attempts to connect a proxy failed",e);
      }
    }
  }
}
