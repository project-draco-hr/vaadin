{
  Button b1=new Button(content);
  b1.setHtmlContentAllowed(true);
  Element e1=ctx.createElement(b1);
  assertEquals("Wrong tag name for button.","v-button",e1.tagName());
  assertEquals("Unexpected content in the v-button element.",content,e1.html());
  assertTrue("The v-button element should not have attributes.",e1.attributes().size() == 0);
  NativeButton b2=new NativeButton(content);
  b2.setHtmlContentAllowed(true);
  Element e2=ctx.createElement(b2);
  assertEquals("Wrong tag name for button.","v-native-button",e2.tagName());
  assertEquals("Unexpected content in the v-button element.",content,e2.html());
  assertTrue("The v-button element should not have attributes.",e2.attributes().size() == 0);
}
