{
  this.height=height;
  super.setHeight(height);
  if (height.endsWith("px")) {
    try {
      borderPaddingVertical=getElement().getOffsetHeight() - Integer.parseInt(height.substring(0,height.length() - 2));
    }
 catch (    Exception e) {
      ClientExceptionHandler.displayError(e);
    }
  }
}
