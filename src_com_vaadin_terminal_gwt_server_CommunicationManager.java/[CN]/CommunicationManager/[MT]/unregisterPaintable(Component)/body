{
  if (pidToNameToReceiver != null) {
    Map<String,StreamVariable> removed=pidToNameToReceiver.remove(getPaintableId(p));
    if (removed != null) {
      for (      String key : removed.keySet()) {
        receiverToSeckey.remove(removed.get(key));
      }
    }
  }
  super.unregisterPaintable(p);
}
