{
  if (isEmpty()) {
    if (isRequired()) {
      return false;
    }
 else {
      return true;
    }
  }
  if (validators == null) {
    return true;
  }
  final Object value=getValue();
  for (final Iterator<Validator> i=validators.iterator(); i.hasNext(); ) {
    if (!(i.next()).isValid(value)) {
      return false;
    }
  }
  return true;
}
