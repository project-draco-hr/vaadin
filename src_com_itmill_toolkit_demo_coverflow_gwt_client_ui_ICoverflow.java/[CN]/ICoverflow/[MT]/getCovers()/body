{
  for (int i=0; i < coverList.size(); i++) {
    HashMap set=(HashMap)coverList.get(i);
    try {
      addCover(uidlId,set.get("caption").toString(),set.get("icon").toString());
    }
 catch (    Exception e) {
    }
  }
  flashInited=true;
  if (_selected != null) {
    selectCover(uidlId,_selected.toString());
  }
}
