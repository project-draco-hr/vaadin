{
  addUIProvider(new UIProvider(){
    @Override public UI instantiateUI(    Application application,    Class<? extends UI> type,    WrappedRequest request){
      return getUI(request);
    }
    @Override public Class<? extends UI> getUIClass(    Application application,    WrappedRequest request){
      return getUI(request).getClass();
    }
  }
);
}
