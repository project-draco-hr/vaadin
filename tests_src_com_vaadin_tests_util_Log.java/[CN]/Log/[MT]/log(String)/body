{
  int nr=eventLabels.size();
  for (int i=nr - 1; i > 0; i--) {
    eventLabels.get(i).setValue(eventLabels.get(i - 1).getValue());
  }
  String msg=event;
  if (numberLogRows) {
    msg=nextLogNr + ". " + msg;
    nextLogNr++;
  }
  eventLabels.get(0).setValue(msg);
  System.out.println(event);
}
