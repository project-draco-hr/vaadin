{
  String design="<v-grid><table>" + "<colgroup>" + "   <col sortable=true width='100' property-id='Column1'>"+ "   <col sortable=true max-width='200' expand='2'>"+ "   <col sortable=true min-width='15' expand='1' property-id='Column3'>"+ "</colgroup>"+ "</table></v-grid>";
  Grid grid=new Grid();
  grid.addColumn("Column1",String.class).setWidth(100);
  grid.addColumn("property-1",String.class).setMaximumWidth(200).setExpandRatio(2);
  grid.addColumn("Column3",String.class).setMinimumWidth(15).setExpandRatio(1);
  testRead(design,grid);
}
