{
  return new BrowserDetails(){
    @Override public String getUriFragment(){
      String fragment=secondRequest.getParameter("fr");
      if (fragment == null) {
        return "";
      }
 else {
        return fragment;
      }
    }
    @Override public String getWindowName(){
      return secondRequest.getParameter("wn");
    }
    @Override public WebBrowser getWebBrowser(){
      WebApplicationContext context=(WebApplicationContext)Application.getCurrent().getContext();
      return context.getBrowser();
    }
  }
;
}
