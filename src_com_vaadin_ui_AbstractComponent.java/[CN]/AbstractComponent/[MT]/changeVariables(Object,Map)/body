{
  if (variables.containsKey(ComponentEventHandler.HANDLER_TRIGGER_VARIABLE)) {
    String[] firedEvent=(String[])variables.get(ComponentEventHandler.HANDLER_TRIGGER_VARIABLE);
    String[] params=new String[firedEvent.length - 1];
    for (int i=0; i < params.length; i++) {
      params[i]=firedEvent[i + 1];
    }
    String event=firedEvent[0];
    handleEvent(event,params);
  }
}
