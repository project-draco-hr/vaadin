{
  if (getHeight() >= 0 && (getHeightUnits() != Unit.PERCENTAGE || ComponentSizeValidator.parentCanDefineHeight(this))) {
    getState().setHeight("" + getCSSHeight());
  }
 else {
    getState().setHeight("");
  }
  if (getWidth() >= 0 && (getWidthUnits() != Unit.PERCENTAGE || ComponentSizeValidator.parentCanDefineWidth(this))) {
    getState().setWidth("" + getCSSWidth());
  }
 else {
    getState().setWidth("");
  }
  ErrorMessage error=getErrorMessage();
  if (null != error) {
    getState().setErrorMessage(error.getFormattedHtmlMessage());
  }
 else {
    getState().setErrorMessage(null);
  }
}
