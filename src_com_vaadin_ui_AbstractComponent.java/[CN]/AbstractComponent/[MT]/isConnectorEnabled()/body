{
  if (getParent() == null) {
    return false;
  }
 else {
    boolean thisEnabledAndVisible=isEnabled() && isVisible();
    if (!thisEnabledAndVisible) {
      return false;
    }
    if (!getParent().isConnectorEnabled()) {
      return false;
    }
    return getParent().isComponentVisible(this);
  }
}
