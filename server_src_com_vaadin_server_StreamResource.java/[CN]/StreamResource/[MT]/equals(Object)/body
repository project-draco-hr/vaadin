{
  if (this == obj) {
    return true;
  }
 else   if (obj instanceof StreamResource) {
    StreamResource that=(StreamResource)obj;
    return getStreamSource().equals(that.getStreamSource()) && getMIMEType().equals(that.getMIMEType()) && String.valueOf(getFilename()).equals(String.valueOf(that.getFilename()))&& getBufferSize() == that.getBufferSize() && getCacheTime() == that.getCacheTime();
  }
 else {
    return false;
  }
}
