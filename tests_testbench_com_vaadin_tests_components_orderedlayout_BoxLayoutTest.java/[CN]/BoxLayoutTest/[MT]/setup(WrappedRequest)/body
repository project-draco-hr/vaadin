{
  view=new VerticalLayout();
  view.setSizeFull();
  view.setMargin(true);
  view.setSpacing(true);
  view.addComponent(createControls(false));
  view.addComponent(createTestLayout(false));
  view.setExpandRatio(view.getComponent(1),1);
  setContent(view);
  getApplication().setRootPreserved(true);
}
