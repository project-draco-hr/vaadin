{
  List<WebElement> rows=$(TableElement.class).first().findElement(By.className("v-table-body")).findElements(By.tagName("tr"));
  WebElement firstrow=rows.get(0);
  List<WebElement> cells=firstrow.findElements(By.className("v-table-cell-content"));
  int cellwidth=cells.get(0).getSize().getWidth();
  return cellwidth;
}
