{
  if (getItemPropertyIds() != null) {
    for (    Object id : getItemPropertyIds()) {
      if (id != null) {
        Field<?> field=getField(id);
        if (field.isEnabled() && !field.isReadOnly()) {
          return field;
        }
      }
    }
    Object id=getItemPropertyIds().iterator().next();
    if (id != null) {
      return getField(id);
    }
  }
  return null;
}
