{
  click(cbDefault);
  click(cbOther);
  click(cbCtrl);
  click(cbShift);
  assertFalse("Default was selected when it shouldn't have been.",isChecked(cbDefault));
  assertFalse("Other was selected when it shouldn't have been.",isChecked(cbOther));
  assertFalse("Ctrl+A was selected when it shouldn't have been.",isChecked(cbCtrl));
  assertFalse("Shift+H was selected when it shouldn't have been.",isChecked(cbShift));
}
