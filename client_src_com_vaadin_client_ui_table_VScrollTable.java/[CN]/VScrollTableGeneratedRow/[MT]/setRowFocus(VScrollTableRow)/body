{
  if (!isSelectable()) {
    return false;
  }
  if (focusedRow != null && focusedRow != row) {
    focusedRow.removeStyleName(getStylePrimaryName() + "-focus");
  }
  if (row != null) {
    row.addStyleName(getStylePrimaryName() + "-focus");
    if (row == focusedRow) {
      return false;
    }
    focusedRow=row;
    ensureRowIsVisible(row);
    return true;
  }
  return false;
}
