{
  String resultPath=null;
  resultPath=servletContext.getRealPath(path);
  if (resultPath != null) {
    return resultPath;
  }
 else {
    try {
      final URL url=servletContext.getResource(path);
      resultPath=url.getFile();
    }
 catch (    final Exception e) {
    }
  }
  return resultPath;
}
