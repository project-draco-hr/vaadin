{
  String ext=getTestingToolsUri(request);
  ext=ext.substring(0,ext.lastIndexOf('/'));
  page.write("<script src=\"" + ext + "/ext/ATF.js"+ "\" type=\"text/javascript\"></script>\n");
  page.write("<link rel=\"stylesheet\" href=\"" + ext + "/ext/ATF.css"+ "\" type=\"text/css\" />\n");
  if (request.getParameter("ATF-TC") != null || request.getParameter("ATF-TS") != null) {
    proxyTestCases(request.getParameter("ATF-TC"),request.getParameter("ATF-TS"),request.getParameter("ATF-TS-RUN-ID"),page,getTestingToolsUri(request));
  }
}
