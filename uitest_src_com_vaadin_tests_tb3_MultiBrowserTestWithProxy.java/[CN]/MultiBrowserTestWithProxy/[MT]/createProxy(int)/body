{
  JSch j=new JSch();
  String keyFile=System.getProperty("sshkey.file");
  if (keyFile == null) {
    keyFile="~/.ssh/id_rsa";
  }
  j.addIdentity(keyFile);
  proxySession=j.getSession("localhost");
  proxySession.setConfig("StrictHostKeyChecking","no");
  proxySession.setPortForwardingL("0.0.0.0",proxyPort,super.getDeploymentHostname(),super.getDeploymentPort());
  proxySession.connect();
}
