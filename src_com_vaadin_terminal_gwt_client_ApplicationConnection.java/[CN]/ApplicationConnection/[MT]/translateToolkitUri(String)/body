{
  if (toolkitUri == null) {
    return null;
  }
  if (toolkitUri.startsWith("theme://")) {
    final String themeUri=configuration.getThemeUri();
    if (themeUri == null) {
      console.error("Theme not set: ThemeResource will not be found. (" + toolkitUri + ")");
    }
    toolkitUri=themeUri + toolkitUri.substring(7);
  }
  return toolkitUri;
}
