{
  if (applicationRunning) {
    checkForPendingVariableBursts();
    runPostRequestHooks(configuration.getRootPanelId());
  }
  activeRequests--;
  Scheduler.get().scheduleDeferred(new Command(){
    public void execute(){
      if (activeRequests == 0) {
        hideLoadingIndicator();
      }
    }
  }
);
}
