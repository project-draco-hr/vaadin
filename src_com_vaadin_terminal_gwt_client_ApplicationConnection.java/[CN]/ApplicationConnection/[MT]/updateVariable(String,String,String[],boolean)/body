{
  final StringBuffer buf=new StringBuffer();
  if (values != null) {
    for (int i=0; i < values.length; i++) {
      if (i > 0) {
        buf.append(VAR_ARRAYITEM_SEPARATOR);
      }
      buf.append(values[i]);
    }
  }
  addVariableToQueue(paintableId,variableName,buf.toString(),immediate,'c');
}
