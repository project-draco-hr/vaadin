{
  for (Iterator i=parameters.keySet().iterator(); i.hasNext(); ) {
    String name=(String)i.next();
    String[] values=(String[])parameters.get(name);
    String v="";
    for (int j=0; j < values.length; j++) {
      if (v.length() > 0) {
        v+=", ";
      }
      v+="'" + values[j] + "'";
    }
    params.addItem(new Object[]{v},name);
  }
}
