{
  GridColumnState state=getState().columns.get(columnIndex);
  @SuppressWarnings("unchecked") CustomGridColumn column=new CustomGridColumn(state.id,((AbstractRendererConnector<Object>)state.rendererConnector));
  columnIdToColumn.put(state.id,column);
  getWidget().addColumn(column,columnIndex);
  updateColumnFromState(column,state);
}
