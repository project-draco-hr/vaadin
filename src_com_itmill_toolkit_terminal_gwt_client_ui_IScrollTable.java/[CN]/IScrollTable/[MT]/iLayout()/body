{
  if (height != null) {
    if (height.contains("%")) {
      mySetHeight(height);
    }
    int contentH=(DOM.getElementPropertyInt(getElement(),"clientHeight") - tHead.getOffsetHeight());
    if (contentH < 0) {
      contentH=0;
    }
    bodyContainer.setHeight(contentH + "px");
  }
  if (width != null) {
    setRelativeWidth(width);
  }
}
