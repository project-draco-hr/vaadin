{
  if (!initializedAndAttached) {
    tHead.addAvailableCell(new RowHeadersHeaderCell());
    Iterator it=uidl.getChildIterator();
    while (it.hasNext()) {
      UIDL col=(UIDL)it.next();
      String cid=col.getStringAttribute("cid");
      HeaderCell c=new HeaderCell(cid,col.getStringAttribute("caption"));
      tHead.addAvailableCell(c);
      if (col.hasAttribute("sortable")) {
        c.setSortable(true);
        if (cid.equals(sortColumn))         c.setSorted(true);
 else         c.setSorted(false);
      }
    }
  }
 else {
  }
}
