{
  if (!initializedAndAttached) {
    tHead.addAvailableCell(new HeaderCell("0",""));
    Iterator it=c.getChildIterator();
    while (it.hasNext()) {
      UIDL col=(UIDL)it.next();
      tHead.addAvailableCell(new HeaderCell(col.getStringAttribute("cid"),col.getStringAttribute("caption")));
    }
  }
 else {
  }
}
