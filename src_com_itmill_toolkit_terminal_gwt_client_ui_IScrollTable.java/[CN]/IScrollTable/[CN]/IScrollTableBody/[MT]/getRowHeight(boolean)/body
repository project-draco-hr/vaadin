{
  if (tBodyMeasurementsDone && !forceUpdate) {
    return rowHeight;
  }
 else {
    if (tBodyElement.getRows().getLength() > 0) {
      rowHeight=getTableHeight() / tBodyElement.getRows().getLength();
    }
 else {
      return DEFAULT_ROW_HEIGHT;
    }
    tBodyMeasurementsDone=true;
    return rowHeight;
  }
}
