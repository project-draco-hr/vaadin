{
  if (tBodyMeasurementsDone) {
    NodeList<TableRowElement> rows=tBodyElement.getRows();
    if (rows.getLength() == 0) {
      return 0;
    }
 else {
      com.google.gwt.dom.client.Element wrapperdiv=rows.getItem(0).getCells().getItem(columnIndex).getFirstChildElement();
      return wrapperdiv.getOffsetWidth();
    }
  }
 else {
    return 0;
  }
}
