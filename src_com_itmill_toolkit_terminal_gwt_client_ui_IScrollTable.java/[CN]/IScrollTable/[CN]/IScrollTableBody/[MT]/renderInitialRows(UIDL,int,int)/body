{
  firstRendered=firstIndex;
  lastRendered=firstIndex + rows - 1;
  Iterator it=rowData.getChildIterator();
  aligns=tHead.getColumnAlignments();
  while (it.hasNext()) {
    IScrollTableRow row=new IScrollTableRow((UIDL)it.next(),aligns);
    addRow(row);
  }
  if (isAttached()) {
    fixSpacers();
  }
}
