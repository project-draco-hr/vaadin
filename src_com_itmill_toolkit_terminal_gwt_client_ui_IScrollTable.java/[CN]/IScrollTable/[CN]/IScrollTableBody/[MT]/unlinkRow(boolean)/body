{
  if (lastRendered - firstRendered < 0)   return;
  int index;
  if (fromBeginning) {
    index=0;
    firstRendered++;
  }
 else {
    index=renderedRows.size() - 1;
    lastRendered--;
  }
  IScrollTableRow toBeRemoved=(IScrollTableRow)renderedRows.get(index);
  this.disown(toBeRemoved);
  renderedRows.remove(index);
  fixSpacers();
}
