{
  if (focusedSlot < 0) {
    return;
  }
  if (focusedSlot == 0) {
    DOM.setElementProperty(DOM.getFirstChild(DOM.getChild(tr,focusedSlot)),"className",CLASSNAME + "-resizer");
  }
 else   if (focusedSlot > 0) {
    DOM.setElementProperty(DOM.getFirstChild(DOM.getChild(tr,focusedSlot - 1)),"className",CLASSNAME + "-resizer");
  }
  focusedSlot=-1;
}
