{
  if (isDefinedWidth()) {
    return width;
  }
 else {
    if (naturalWidth == 0) {
      final int hw=getOffsetWidth();
      int i=0;
      for (Iterator<Widget> it=tHead.iterator(); it.hasNext(); i++) {
        if (it.next() == this) {
          break;
        }
      }
      final int cw=tBody.getColWidth(i);
      naturalWidth=(hw > cw ? hw : cw) + IScrollTableBody.CELL_EXTRA_WIDTH;
    }
    return naturalWidth;
  }
}
