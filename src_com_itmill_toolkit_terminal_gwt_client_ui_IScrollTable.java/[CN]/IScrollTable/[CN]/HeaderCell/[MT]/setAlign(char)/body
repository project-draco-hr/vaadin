{
  if (align != c) {
switch (c) {
case ALIGN_CENTER:
      DOM.setStyleAttribute(captionContainer,"text-align","center");
    break;
case ALIGN_RIGHT:
  DOM.setStyleAttribute(captionContainer,"text-align","right");
break;
default :
DOM.setStyleAttribute(captionContainer,"text-align","");
break;
}
}
align=c;
}
