{
  TextField textField=new TextField("TextField " + numberOfTextFields++,"This textfield has a validator");
  textField.setImmediate(true);
  textField.setIcon(new ThemeResource("icons/16/document-image.png"));
  textField.setRequired(true);
  textField.addValidator(new StringLengthValidator("5 < String.length() < 10",5,10,false));
  return textField;
}
