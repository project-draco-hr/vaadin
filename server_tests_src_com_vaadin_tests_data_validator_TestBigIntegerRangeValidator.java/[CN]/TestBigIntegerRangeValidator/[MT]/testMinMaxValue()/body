{
  assertTrue("Didn't accept valid value",minMaxValidator.isValid(BigInteger.valueOf(15)));
  assertTrue("Didn't accept valid value",minMaxValidator.isValid(BigInteger.valueOf(99)));
  assertFalse("Accepted too small value",minMaxValidator.isValid(BigInteger.valueOf(9)));
  assertFalse("Accepted too large value",minMaxValidator.isValid(BigInteger.valueOf(110)));
}
