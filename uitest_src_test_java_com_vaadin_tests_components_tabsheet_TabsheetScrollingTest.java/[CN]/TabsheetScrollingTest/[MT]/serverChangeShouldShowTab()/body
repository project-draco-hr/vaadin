{
  openTestURL();
  $(ButtonElement.class).id(TabsheetScrolling.SELECT_LAST).click();
  TabSheetElement tabsheetFixed=$(TabSheetElement.class).first();
  Assert.assertTrue("Select last should scroll last tab into view",tabsheetFixed.getTabCaptions().contains("Tab 99"));
  $(ButtonElement.class).id(TabsheetScrolling.SELECT_FIRST).click();
  Assert.assertTrue("Select first should scroll first tab into view",tabsheetFixed.getTabCaptions().contains("Tab 1"));
}
