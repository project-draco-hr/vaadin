{
switch (type) {
case TYPE_WARNING_MESSAGE:
    delayMsec=1500;
  styleName="warning";
break;
case TYPE_ERROR_MESSAGE:
delayMsec=-1;
styleName="error";
break;
case TYPE_TRAY_NOTIFICATION:
delayMsec=3000;
position=POSITION_BOTTOM_RIGHT;
styleName="tray";
case TYPE_HUMANIZED_MESSAGE:
default :
break;
}
}
