{
  DownloadStream result=null;
  if (uriHandlerList != null) {
    Object[] handlers;
synchronized (uriHandlerList) {
      handlers=uriHandlerList.toArray();
    }
    for (int i=0; i < handlers.length; i++) {
      final DownloadStream ds=((URIHandler)handlers[i]).handleURI(context,relativeUri);
      if (ds != null) {
        if (result != null) {
          throw new RuntimeException("handleURI for " + context + " uri: '"+ relativeUri+ "' returns ambigious result.");
        }
        result=ds;
      }
    }
  }
  return result;
}
