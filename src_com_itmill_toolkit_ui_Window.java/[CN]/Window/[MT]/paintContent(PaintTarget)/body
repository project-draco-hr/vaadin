{
  String name=getName();
  target.addAttribute("name",name == null ? "" : name);
  String theme=getTheme();
  target.addAttribute("theme",theme == null ? "" : theme);
  if (getApplication() != null && this == getApplication().getMainWindow())   target.addAttribute("main",true);
synchronized (openList) {
    if (!openList.isEmpty()) {
      for (Iterator i=openList.iterator(); i.hasNext(); )       ((OpenResource)i.next()).paintContent(target);
      openList.clear();
    }
  }
  super.paintContent(target);
  target.addVariable(this,"positionx",getPositionX());
  target.addVariable(this,"positiony",getPositionY());
  target.addVariable(this,"scrolltop",getScrollTop());
  target.addVariable(this,"scrollleft",getScrollLeft());
  target.addVariable(this,"close",false);
  if (this.focusedComponent != null)   target.addVariable(this,"focused","" + this.focusedComponent.getFocusableId());
 else   target.addVariable(this,"focused","");
  for (Iterator i=subwindows.iterator(); i.hasNext(); ) {
    Window w=(Window)i.next();
    w.paint(target);
  }
}
