{
  for (  Class<?> type : new Class<?>[]{String.class,Boolean.class,Integer.class,Float.class,Byte.class,Short.class,Double.class,ShortcutAction.class,Date.class,FileResource.class,ExternalResource.class,ThemeResource.class,Resource.class,TimeZone.class}) {
    assertTrue("not supported " + type.getSimpleName(),formatter.canConvert(type));
  }
}
