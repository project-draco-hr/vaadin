{
  ShortcutAction action=new ShortcutAction(null,ShortcutAction.KeyCode.D,new int[]{ShortcutAction.ModifierKey.ALT,ShortcutAction.ModifierKey.CTRL});
  String formatted=formatter.format(action);
  assertEquals("alt-ctrl-d",formatted);
  ShortcutAction result=formatter.parse(formatted,ShortcutAction.class);
  assertTrue(equals(action,result));
}
