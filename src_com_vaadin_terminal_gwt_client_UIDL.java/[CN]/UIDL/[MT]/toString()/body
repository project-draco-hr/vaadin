{
  String s="<" + getTag();
  for (final Iterator i=getAttributeNames().iterator(); i.hasNext(); ) {
    final String name=i.next().toString();
    s+=" " + name + "=";
    final JSONValue v=((JSONObject)json.get(1)).get(name);
    if (v.isString() != null) {
      s+=v;
    }
 else {
      s+="\"" + v + "\"";
    }
  }
  s+=">\n";
  final Iterator i=getChildIterator();
  while (i.hasNext()) {
    final Object c=i.next();
    s+=c.toString();
  }
  s+="</" + getTag() + ">\n";
  return s;
}
