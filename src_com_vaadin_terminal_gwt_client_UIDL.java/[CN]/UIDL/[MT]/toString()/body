{
  String s="<" + getTag();
  Set<String> attributeNames=getAttributeNames();
  for (  String name : attributeNames) {
    s+=" " + name + "=";
    final JSONValue v=((JSONObject)json.get(1)).get(name);
    if (v.isString() != null) {
      s+=v;
    }
 else {
      s+="\"" + v + "\"";
    }
  }
  s+=">\n";
  final Iterator<Object> i=getChildIterator();
  while (i.hasNext()) {
    final Object c=i.next();
    s+=c.toString();
  }
  s+="</" + getTag() + ">\n";
  return s;
}
