{
  final StringBuffer sb=new StringBuffer();
  Set<String> keySet=x.keySet();
  for (  String tag : keySet) {
    sb.append("<");
    sb.append(tag);
    sb.append(">");
    sb.append(x.get(tag).isString().stringValue());
    sb.append("</");
    sb.append(tag);
    sb.append(">");
  }
  return sb.toString();
}
