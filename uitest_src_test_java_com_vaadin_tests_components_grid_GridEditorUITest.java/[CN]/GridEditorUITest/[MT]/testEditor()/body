{
  assertFalse("Sanity check",isElementPresent(LegacyPasswordFieldElement.class));
  openEditor(5);
  new Actions(getDriver()).sendKeys(Keys.ESCAPE).perform();
  openEditor(10);
  assertTrue("Editor should be opened with a password field",isElementPresent(LegacyPasswordFieldElement.class));
  assertFalse("Notification was present",isElementPresent(NotificationElement.class));
}
