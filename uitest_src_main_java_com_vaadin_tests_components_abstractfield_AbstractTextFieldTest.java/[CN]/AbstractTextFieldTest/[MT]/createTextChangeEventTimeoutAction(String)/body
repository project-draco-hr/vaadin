{
  LinkedHashMap<String,Integer> options=new LinkedHashMap<String,Integer>();
  options.put("0",0);
  options.put("100ms",100);
  options.put("500ms",500);
  options.put("1s",1000);
  options.put("2s",2000);
  options.put("5s",5000);
  createSelectAction("TextChange timeout",category,options,"0",textChangeTimeoutCommand);
}
