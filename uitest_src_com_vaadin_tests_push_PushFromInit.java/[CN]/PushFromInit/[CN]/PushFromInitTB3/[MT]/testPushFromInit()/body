{
  for (int second=0; ; second++) {
    if (second >= 30) {
      Assert.fail("timeout");
    }
    try {
      if ("1. Logged in init".equals(vaadinElementById("Log_row_1").getText())) {
        break;
      }
    }
 catch (    Exception e) {
    }
    try {
      Thread.sleep(200);
    }
 catch (    InterruptedException e) {
    }
  }
  Assert.assertEquals("2. Logged from background thread started in init",vaadinElementById("Log_row_0").getText());
}
