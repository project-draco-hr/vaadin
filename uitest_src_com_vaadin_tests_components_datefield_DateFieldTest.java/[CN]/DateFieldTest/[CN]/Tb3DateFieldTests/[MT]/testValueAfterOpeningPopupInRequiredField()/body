{
  Thread.sleep(1000);
  menu("Component");
  menuSub("State");
  menu("Required");
  menu("Component");
  menuSub("Features");
  menuSub("Resolution");
  menu("Month");
  menu("Component");
  menuSub("Listeners");
  menu("Value change listener");
