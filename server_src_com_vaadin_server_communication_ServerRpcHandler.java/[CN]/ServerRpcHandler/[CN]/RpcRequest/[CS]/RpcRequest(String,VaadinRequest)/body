{
  json=new JSONObject(jsonString);
  csrfToken=json.getString(ApplicationConstants.CSRF_TOKEN);
  if (request.getService().getDeploymentConfiguration().isSyncIdCheckEnabled()) {
    syncId=json.getInt(ApplicationConstants.SERVER_SYNC_ID);
  }
 else {
    syncId=-1;
  }
  invocations=new JSONArray(json.getString(ApplicationConstants.RPC_INVOCATIONS));
}
