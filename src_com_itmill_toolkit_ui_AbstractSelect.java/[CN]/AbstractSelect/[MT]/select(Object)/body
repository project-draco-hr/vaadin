{
  if (!isSelected(itemId) && items.containsId(itemId)) {
    if (isMultiSelect()) {
      final Set s=new HashSet((Set)getValue());
      s.add(itemId);
      setValue(s);
    }
 else     if (itemId.equals(getNullSelectionItemId())) {
      setValue(null);
    }
 else {
      setValue(itemId);
    }
  }
}
