{
  String[] event;
  boolean transmit=forceTransmission || ((!(eventRegistrations == null)) && eventRegistrations.contains(eventIdentifier));
  if (transmit) {
    if (parameters != null) {
      event=new String[parameters.length + 1];
      event[0]=eventIdentifier;
      for (int i=0; i < parameters.length; i++) {
        event[i + 1]=parameters[i];
      }
    }
 else {
      event=new String[]{eventIdentifier};
    }
    client.updateVariable(detail.getPid(),HANDLER_TRIGGER_VARIABLE,event,true);
  }
}
