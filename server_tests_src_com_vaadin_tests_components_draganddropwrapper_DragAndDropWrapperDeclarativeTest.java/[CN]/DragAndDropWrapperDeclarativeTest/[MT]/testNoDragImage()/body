{
  Button okButton=new Button("OK");
  String input="<v-drag-and-drop-wrapper drag-start-mode='wrapper'>" + new DesignContext().createElement(okButton) + "</v-drag-and-drop-wrapper>";
  DragAndDropWrapper wrapper=new DragAndDropWrapper(okButton);
  wrapper.setDragStartMode(DragStartMode.WRAPPER);
  testWrite(input,wrapper);
  testRead(input,wrapper);
}
