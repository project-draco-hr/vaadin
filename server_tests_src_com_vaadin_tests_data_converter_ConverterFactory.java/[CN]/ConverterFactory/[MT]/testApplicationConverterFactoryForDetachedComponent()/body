{
  final VaadinServiceSession appWithCustomIntegerConverter=new VaadinServiceSession(null);
  appWithCustomIntegerConverter.setConverterFactory(new ConverterFactory42());
  VaadinServiceSession.setCurrent(appWithCustomIntegerConverter);
  TextField tf=new TextField("","123");
  tf.setConverter(Integer.class);
  assertEquals(42,tf.getConvertedValue());
}
