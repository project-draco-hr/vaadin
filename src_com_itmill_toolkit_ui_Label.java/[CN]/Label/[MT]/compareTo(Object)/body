{
  String thisValue;
  String otherValue;
  if (contentMode == CONTENT_XML || contentMode == CONTENT_UIDL || contentMode == CONTENT_XHTML) {
    thisValue=stripTags(toString());
  }
 else {
    thisValue=toString();
  }
  if (other instanceof Label && (((Label)other).getContentMode() == CONTENT_XML || ((Label)other).getContentMode() == CONTENT_UIDL || ((Label)other).getContentMode() == CONTENT_XHTML)) {
    otherValue=stripTags(other.toString());
  }
 else {
    otherValue=other.toString();
  }
  return thisValue.compareTo(otherValue);
}
