{
  Assert.assertEquals("May 18, 2033",localeConverter.convertToPresentation(true,Locale.US));
  Assert.assertEquals("January 24, 2065",localeConverter.convertToPresentation(false,Locale.US));
  Assert.assertEquals("18. Mai 2033",localeConverter.convertToPresentation(true,Locale.GERMANY));
  Assert.assertEquals("24. Januar 2065",localeConverter.convertToPresentation(false,Locale.GERMANY));
}
