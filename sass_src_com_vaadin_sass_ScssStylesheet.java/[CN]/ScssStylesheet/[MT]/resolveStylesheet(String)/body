{
  if (resolvers == null) {
    setStylesheetResolvers(new VaadinResolver());
  }
  for (  ScssStylesheetResolver resolver : resolvers) {
    InputSource source=resolver.resolve(identifier);
    if (source != null) {
      File f=new File(source.getURI());
      setFileName(f.getParent());
      return source;
    }
  }
  return null;
}
