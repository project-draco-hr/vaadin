{
  if (androidWithBrokenScrollTop) {
    if (scrolledElement.getPropertyJSO("_vScrollTop") != null) {
      return scrolledElement.getPropertyInt("_vScrollTop");
    }
 else {
      return 0;
    }
  }
 else {
    if (transitionOn) {
      return scrolledElement.getScrollTop();
    }
 else {
      return scrolledElement.getScrollTop();
    }
  }
}
