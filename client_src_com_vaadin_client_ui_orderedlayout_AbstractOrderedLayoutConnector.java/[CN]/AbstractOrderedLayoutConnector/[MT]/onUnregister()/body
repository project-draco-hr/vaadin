{
  for (  ComponentConnector child : getChildComponents()) {
    Slot slot=getWidget().getSlot(child.getWidget());
    if (slot.hasCaption()) {
      removeResizeListener(slot.getCaptionElement(),slotCaptionResizeListener);
    }
    if (slot.getSpacingElement() != null) {
      removeResizeListener(slot.getSpacingElement(),spacingResizeListener);
    }
    removeResizeListener(slot.getWidget().getElement(),childComponentResizeListener);
  }
  super.onUnregister();
}
