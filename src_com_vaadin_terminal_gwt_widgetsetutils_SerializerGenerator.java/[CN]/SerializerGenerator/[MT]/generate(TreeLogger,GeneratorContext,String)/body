{
  JClassType beanType=context.getTypeOracle().findType(beanTypeName);
  String beanSerializerClassName=getSerializerSimpleClassName(beanType);
  try {
    generateClass(logger,context,beanType,beanSerializerPackageName,beanSerializerClassName);
  }
 catch (  Exception e) {
    logger.log(TreeLogger.ERROR,"SerializerGenerator failed for " + beanType.getQualifiedSourceName(),e);
    throw new UnableToCompleteException();
  }
  return getFullyQualifiedSerializerClassName(beanType);
}
