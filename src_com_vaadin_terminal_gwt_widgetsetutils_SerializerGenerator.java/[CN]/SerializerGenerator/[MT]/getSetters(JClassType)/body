{
  List<JMethod> setterMethods=new ArrayList<JMethod>();
  while (!beanType.getQualifiedSourceName().equals(Object.class.getName())) {
    for (    JMethod method : beanType.getMethods()) {
      if (!method.isPublic() || method.isStatic() || !method.getName().startsWith("set")|| method.getParameterTypes().length != 1) {
        continue;
      }
      setterMethods.add(method);
    }
    beanType=beanType.getSuperclass();
  }
  return setterMethods;
}
