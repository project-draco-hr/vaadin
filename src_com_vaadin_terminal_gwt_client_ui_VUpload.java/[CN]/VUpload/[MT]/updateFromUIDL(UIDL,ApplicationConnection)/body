{
  if (client.updateComponent(this,uidl,true)) {
    return;
  }
  setImmediate(uidl.getBooleanAttribute("immediate"));
  this.client=client;
  paintableId=uidl.getId();
  setAction(client.getAppUri());
  submitButton.setHTML("<span class=\"v-button-caption\">" + uidl.getStringAttribute("buttoncaption") + "</span>");
  fu.setName(paintableId + "_file");
  if (uidl.hasAttribute("disabled") || uidl.hasAttribute("readonly")) {
    disableUpload();
  }
 else   if (uidl.getBooleanAttribute("state")) {
    enableUploaod();
  }
}
