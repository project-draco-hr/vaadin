{
  if (!(itemId instanceof File))   return Collections.unmodifiableCollection(new LinkedList());
  File[] f;
  if (this.filter != null)   f=((File)itemId).listFiles(this.filter);
 else   f=((File)itemId).listFiles();
  if (f == null)   return Collections.unmodifiableCollection(new LinkedList());
  List l=Arrays.asList(f);
  Collections.sort(l);
  return Collections.unmodifiableCollection(l);
}
