{
  String s="<" + getTag();
  for (Iterator i=getAttributeNames().iterator(); i.hasNext(); ) {
    String name=i.next().toString();
    s+=" " + name + "="+ ((JSONObject)json.get(1)).get(name);
  }
  s+=">\n";
  Iterator i=getChildIterator();
  while (i.hasNext()) {
    Object c=i.next();
    s+=c.toString();
  }
  s+="</" + getTag() + ">\n";
  return s;
}
