{
  String s="<" + getTag();
  for (Iterator i=getAttributeNames().iterator(); i.hasNext(); ) {
    String name=i.next().toString();
    s+=" " + name + "=";
    JSONValue v=((JSONObject)json.get(1)).get(name);
    if (v.isString() != null) {
      s+=v;
    }
 else {
      s+="\"" + v + "\"";
    }
  }
  s+=">\n";
  Iterator i=getChildIterator();
  while (i.hasNext()) {
    Object c=i.next();
    s+=c.toString();
  }
  s+="</" + getTag() + ">\n";
  return s;
}
