{
  selectMenuPath(EDIT_ITEM_5);
  assertFalse(logContainsText("Exception occured, java.lang.IllegalStateException"));
  GridEditorElement editor=getGridElement().getEditor();
  WebElement intField=editor.getField(7);
  intField.clear();
  intField.sendKeys("banana phone");
  editor.save();
  WebElement n=$(NotificationElement.class).first();
  assertEquals("Column 7: Could not convert value to Integer",n.getText());
  n.click();
  editor.cancel();
  selectMenuPath(EDIT_ITEM_100);
  assertFalse("Exception should not exist",isElementPresent(NotificationElement.class));
}
