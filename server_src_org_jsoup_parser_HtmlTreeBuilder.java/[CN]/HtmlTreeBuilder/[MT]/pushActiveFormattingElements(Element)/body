{
  int numSeen=0;
  Iterator<Element> iter=formattingElements.descendingIterator();
  while (iter.hasNext()) {
    Element el=iter.next();
    if (el == null) {
      break;
    }
    if (isSameFormattingElement(in,el)) {
      numSeen++;
    }
    if (numSeen == 3) {
      iter.remove();
      break;
    }
  }
  formattingElements.add(in);
}
