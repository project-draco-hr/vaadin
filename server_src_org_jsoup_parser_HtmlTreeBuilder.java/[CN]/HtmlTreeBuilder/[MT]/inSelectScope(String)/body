{
  Iterator<Element> it=stack.descendingIterator();
  while (it.hasNext()) {
    Element el=it.next();
    String elName=el.nodeName();
    if (elName.equals(targetName)) {
      return true;
    }
    if (!StringUtil.in(elName,"optgroup","option")) {
      return false;
    }
  }
  Validate.fail("Should not be reachable");
  return false;
}
