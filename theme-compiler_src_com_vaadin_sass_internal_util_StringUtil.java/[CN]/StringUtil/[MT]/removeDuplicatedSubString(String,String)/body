{
  List<String> subStrings=Arrays.asList(motherString.split(Pattern.quote(splitter)));
  LinkedHashSet<String> uniqueSubStrings=new LinkedHashSet<String>(subStrings);
  StringBuilder builder=new StringBuilder();
  int count=0;
  for (  String uniqueSubString : uniqueSubStrings) {
    count++;
    builder.append(uniqueSubString);
    if (count < uniqueSubStrings.size()) {
      builder.append(splitter);
    }
  }
  return builder.toString();
}
