{
  if (isInternalType(targetType)) {
    throw new JSONException("decodeCustomType cannot be used for " + targetType + ", which is an internal type");
  }
  String transportType=getCustomTransportType(getClassForType(targetType));
  String encodedTransportType=valueAndType.getString(0);
  if (!encodedTransportType.equals(transportType)) {
    throw new JSONException("Expected a value of type " + transportType + ", received "+ encodedTransportType);
  }
  return decodeObject(targetType,(JSONObject)valueAndType.get(1),application);
}
