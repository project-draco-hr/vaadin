{
  for (final Iterator i=propertyIds.iterator(); i.hasNext(); ) {
    try {
      AbstractComponent field=(AbstractComponent)fields.get(i.next());
      ErrorMessage e=field.getErrorMessage();
      if (e != null) {
        if ("".equals(e.toString())) {
          continue;
        }
        return e;
      }
    }
 catch (    ClassCastException ignored) {
    }
  }
  return null;
}
