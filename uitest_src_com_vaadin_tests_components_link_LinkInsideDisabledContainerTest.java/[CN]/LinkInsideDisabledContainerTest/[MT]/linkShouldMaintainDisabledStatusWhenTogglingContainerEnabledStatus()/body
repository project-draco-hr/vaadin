{
  toggleLinkEnabledStatus();
  clickLink();
  assertFalse(isLinkEnabled());
  assertThat(clicksOnLink(),is(0));
  disableContainer();
  clickLink();
  assertFalse(isLinkEnabled());
  assertThat(clicksOnLink(),is(0));
  enableContainer();
  clickLink();
  assertFalse(isLinkEnabled());
  assertThat(clicksOnLink(),is(0));
}
