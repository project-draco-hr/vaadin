{
  String filter=tb.getText();
  if (filter.equals(lastFilter) && currentPage == page) {
    return;
  }
  if (!filter.equals(lastFilter)) {
    page=0;
  }
  filtering=true;
  client.updateVariable(paintableId,"filter",filter,false);
  client.updateVariable(paintableId,"page",page,true);
  lastFilter=filter;
  currentPage=page;
}
