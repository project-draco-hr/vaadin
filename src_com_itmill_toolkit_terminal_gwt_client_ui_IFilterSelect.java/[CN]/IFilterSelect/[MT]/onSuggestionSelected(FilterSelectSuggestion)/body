{
  currentSuggestion=suggestion;
  String newKey=String.valueOf(suggestion.getOptionKey());
  tb.setText(suggestion.getReplacementString());
  if (newKey.equals(selectedOptionKey))   return;
  selectedOptionKey=newKey;
  client.updateVariable(paintableId,"selected",new String[]{selectedOptionKey},immediate);
  lastFilter=tb.getText();
  suggestionPopup.hide();
}
