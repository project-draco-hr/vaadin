{
  target.addAttribute("margins",margins.getBitMask());
  if (getHeight() >= 0) {
    target.addAttribute("height","" + getHeight() + Sizeable.UNIT_SYMBOLS[getHeightUnits()]);
  }
  if (getWidth() >= 0) {
    target.addAttribute("width","" + getWidth() + Sizeable.UNIT_SYMBOLS[getWidthUnits()]);
  }
}
