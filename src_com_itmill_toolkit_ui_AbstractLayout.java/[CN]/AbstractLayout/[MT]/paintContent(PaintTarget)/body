{
  if (margins == null)   setMargin(false);
  if (margins[0])   target.addAttribute("marginTop",margins[0]);
  if (margins[1])   target.addAttribute("marginRight",margins[1]);
  if (margins[2])   target.addAttribute("marginBottom",margins[2]);
  if (margins[3])   target.addAttribute("marginLeft",margins[3]);
}
