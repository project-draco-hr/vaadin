{
  String missingTests="";
  for (  Class<? extends Component> component : componentsWithoutTests) {
    String cls="missing";
    if (component.getAnnotation(Deprecated.class) != null) {
      cls="missing-deprecated";
    }
    missingTests+="<font class=\"" + cls + "\">"+ component.getSimpleName()+ "</font><br/>";
  }
  return new Label("<b>Components without a test:</B><br/>" + missingTests,ContentMode.HTML);
}
