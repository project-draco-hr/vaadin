{
  String script;
  if (BrowserUtil.isIE8(getDesiredCapabilities())) {
    script="return arguments[0].clientHeight;";
  }
 else {
    script="var cs = window.getComputedStyle(arguments[0]);" + "return Math.ceil(parseFloat(cs.height)+parseFloat(cs.paddingTop)+parseFloat(cs.paddingBottom));";
  }
  return ((Number)executeScript(script,e)).intValue();
}
