{
  if (event.getSource() instanceof VCheckBox) {
    VCheckBox source=(VCheckBox)event.getSource();
    if (!source.isEnabled()) {
      return;
    }
    final boolean selected=source.getValue();
    JsonObject item=optionsToItems.get(source);
    if (selected) {
      this.selected=item;
    }
 else {
      this.selected=null;
    }
  }
}
