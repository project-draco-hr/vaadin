{
  boolean optionGroupEnabled=isEnabled() && !isReadonly();
  for (  Map.Entry<VCheckBox,JsonObject> entry : optionsToItems.entrySet()) {
    VCheckBox checkBox=entry.getKey();
    JsonObject value=entry.getValue();
    Boolean isOptionEnabled=!value.getBoolean(CheckBoxGroupConstants.JSONKEY_ITEM_DISABLED);
    checkBox.setEnabled(optionGroupEnabled && isOptionEnabled);
  }
}
