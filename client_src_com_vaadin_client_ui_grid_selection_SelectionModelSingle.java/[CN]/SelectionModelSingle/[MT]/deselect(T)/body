{
  if (row == null) {
    throw new IllegalArgumentException("Row cannot be null");
  }
  if (isSelected(row)) {
    T removed=selectedRow.getRow();
    selectedRow.unpin();
    selectedRow=null;
    grid.fireEvent(new SelectionChangeEvent<T>(grid,null,removed));
    return true;
  }
  return false;
}
