{
  if ((equalsToMinor != -1) && (equalsToMinor != minorVersion))   throw new LicenseViolation("Product minor version is " + minorVersion + " but license requires it to be "+ equalsToMinor);
  if ((equalsToOrIsLessThanMinor != -1) && (equalsToOrIsLessThanMinor < minorVersion))   throw new LicenseViolation("Product minor version is " + minorVersion + " but license requires it to be equal or less than"+ equalsToOrIsLessThanMinor);
  if ((equalsToOrIsMoreThanMinor != -1) && (equalsToOrIsMoreThanMinor > minorVersion))   throw new LicenseViolation("Product minor version is " + minorVersion + " but license requires it to be equal or more than"+ equalsToOrIsMoreThanMinor);
  if ((equalsToMajor != -1) && (equalsToMajor != majorVersion))   throw new LicenseViolation("Product major version is " + majorVersion + " but license requires it to be "+ equalsToMajor);
  if ((equalsToOrIsLessThanMajor != -1) && (equalsToOrIsLessThanMajor < majorVersion))   throw new LicenseViolation("Product major version is " + majorVersion + " but license requires it to be equal or less than"+ equalsToOrIsLessThanMajor);
  if ((equalsToOrIsMoreThanMajor != -1) && (equalsToOrIsMoreThanMajor > majorVersion))   throw new LicenseViolation("Product major version is " + majorVersion + " but license requires it to be equal or more than"+ equalsToOrIsMoreThanMajor);
}
