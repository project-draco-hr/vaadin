{
  File f=new File("/Users/phoenix/Documents/workspace/itmill-toolkit/doc/project/license-system/license-file-template.xml");
  try {
    License.readLicenseFile(new FileInputStream(f));
    System.out.println(getNormalizedLisenceData());
    System.out.println("Signature='" + getSignature() + "'");
  }
 catch (  FileNotFoundException e) {
    e.printStackTrace();
  }
catch (  SAXException e) {
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
catch (  LicenseFileCanNotBeRead e) {
    e.printStackTrace();
  }
catch (  InvalidLicenseFile e) {
    e.printStackTrace();
  }
}
