{
  NodeList licenseeL=licenseXML.getElementsByTagName("licensee");
  if (licenseeL == null || licenseeL.getLength() == 0)   return null;
  NodeList nameL=((Element)licenseeL.item(0)).getElementsByTagName("name");
  if (nameL == null || nameL.getLength() == 0)   return null;
  String name=getTextContent(nameL.item(0));
  if (name == null || name.length() == 0)   return null;
  return name;
}
