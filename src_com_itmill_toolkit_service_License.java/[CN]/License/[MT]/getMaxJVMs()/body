{
  NodeList cuL=licenseXML.getElementsByTagName("concurrent-jvms");
  if (cuL == null && cuL.getLength() == 0)   return -1;
  String limit=((Element)cuL.item(0)).getAttribute("limit");
  if (limit != null && limit.length() > 0 && !limit.equalsIgnoreCase("unlimited"))   return Integer.parseInt(limit);
  return -1;
}
