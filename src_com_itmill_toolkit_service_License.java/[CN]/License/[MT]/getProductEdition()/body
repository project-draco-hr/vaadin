{
  Element prod=(Element)licenseXML.getElementsByTagName("product").item(0);
  if (prod == null)   throw new InvalidLicenseFile("product not found in license-file");
  NodeList editionE=(NodeList)prod.getElementsByTagName("edition");
  if (editionE == null || editionE.getLength() == 0)   return null;
  return getTextContent(editionE.item(0));
}
