{
  NodeList appL=licenseXML.getElementsByTagName("application");
  if (appL != null && appL.getLength() > 0) {
    String classPrefix=((Element)appL.item(0)).getAttribute("classPrefix");
    if (classPrefix != null && classPrefix.length() > 0 && !applicationClass.getName().startsWith(classPrefix))     throw new LicenseViolation("License limits application class prefix to '" + classPrefix + "' but requested application class is '"+ applicationClass.getName()+ "'");
  }
}
