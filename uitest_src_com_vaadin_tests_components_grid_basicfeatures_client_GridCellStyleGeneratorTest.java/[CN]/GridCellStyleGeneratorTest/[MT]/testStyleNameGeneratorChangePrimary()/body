{
  openTestURL();
  selectStyleNameGenerator("Combined");
  GridRowElement row2=getGridElement().getRow(2);
  GridCellElement cell4_2=getGridElement().getCell(4,2);
  Assert.assertTrue(row2.getAttribute("class").contains("v-grid-row-2"));
  Assert.assertTrue(cell4_2.getAttribute("class").contains("v-grid-cell-content-4_2"));
  selectMenuPath("Component","State","Primary Stylename","v-escalator");
  Assert.assertFalse(row2.getAttribute("class").contains("v-grid-row-2"));
  Assert.assertFalse(cell4_2.getAttribute("class").contains("v-grid-cell-content-4_2"));
  Assert.assertTrue(row2.getAttribute("class").contains("v-escalator-row-2"));
  Assert.assertTrue(cell4_2.getAttribute("class").contains("v-escalator-cell-content-4_2"));
}
