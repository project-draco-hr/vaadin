{
  StringBuffer buf=new StringBuffer();
  buf.append(startField());
  buf.append("{");
  for (Iterator iter=attr.iterator(); iter.hasNext(); ) {
    String element=(String)iter.next();
    buf.append(element);
    if (iter.hasNext()) {
      buf.append(",");
    }
  }
  buf.append(tag.variablesAsJsonObject());
  buf.append("}");
  return buf.toString();
}
