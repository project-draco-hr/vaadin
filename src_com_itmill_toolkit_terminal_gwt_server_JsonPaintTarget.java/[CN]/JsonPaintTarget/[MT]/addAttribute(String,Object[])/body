{
  if ((values == null) || (name == null))   throw new NullPointerException("Parameters must be non-null strings");
  StringBuffer buf=new StringBuffer();
  buf.append("\"" + name + "\":[");
  for (int i=0; i < values.length; i++) {
    if (i > 0)     buf.append(",");
    buf.append("\"");
    buf.append(escapeJSON(values[i].toString()));
    buf.append("\"");
  }
  buf.append("]");
  tag.addAttribute(buf.toString());
}
