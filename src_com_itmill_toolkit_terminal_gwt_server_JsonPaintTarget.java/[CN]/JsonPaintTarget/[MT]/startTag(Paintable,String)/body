{
  startTag(tagName,true);
  final boolean isPreviouslyPainted=manager.hasPaintableId(paintable) && (identifiersCreatedDueRefPaint == null || !identifiersCreatedDueRefPaint.contains(paintable));
  final String id=manager.getPaintableId(paintable);
  paintable.addListener(manager);
  addAttribute("id",id);
  paintedComponents.add(paintable);
  return cacheEnabled && isPreviouslyPainted;
}
