{
  selectMenuPath("Component","Editor row","Edit row 100");
  WebElement textField=getEditorRow().findElements(By.className("gwt-TextBox")).get(0);
  textField.sendKeys(" changed");
  WebElement saveButton=getEditorRow().findElement(By.className("v-editor-row-save"));
  saveButton.click();
  assertEquals("(100, 0) changed",getGridElement().getCell(100,0).getText());
}
