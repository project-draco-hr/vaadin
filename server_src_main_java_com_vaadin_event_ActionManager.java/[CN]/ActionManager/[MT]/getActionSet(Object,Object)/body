{
  LinkedHashSet<Action> actions=new LinkedHashSet<Action>();
  if (ownActions != null) {
    actions.addAll(ownActions);
  }
  if (actionHandlers != null) {
    for (    Action.Handler h : actionHandlers) {
      Action[] as=h.getActions(target,sender);
      if (as != null) {
        for (        Action a : as) {
          actions.add(a);
        }
      }
    }
  }
  return actions;
}
