{
  openTestURL();
  sleep(500);
  compareScreen("initial");
  String[] states={"expand_100_0","expand_50_50","expand_25_75"};
  List<ButtonElement> buttons=$(ButtonElement.class).all();
  int index=0;
  for (  ButtonElement btn : buttons) {
    btn.click();
    sleep(500);
    compareScreen(states[index]);
    index++;
  }
}
