{
  String trackerId=uidl.getStringAttribute("trackerid");
  String pageId=uidl.getStringAttribute("pageid");
  String domainName=uidl.getStringAttribute("domain");
  String res=trackPageview(trackerId,pageId,domainName);
  if (null != res) {
    ApplicationConnection.getConsole().log("WebAnalytics.trackPageview(" + trackerId + ","+ pageId+ ","+ domainName+ ") FAILED: "+ res);
  }
 else {
    ApplicationConnection.getConsole().log("WebAnalytics.trackPageview(" + trackerId + ","+ pageId+ ","+ domainName+ ") SUCCESS.");
  }
}
