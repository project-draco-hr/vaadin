{
  if (variables.size() == 0) {
    return "";
  }
  final StringBuffer buf=new StringBuffer();
  buf.append(startField());
  buf.append("\"v\":{");
  final Iterator iter=variables.iterator();
  while (iter.hasNext()) {
    final Variable element=(Variable)iter.next();
    buf.append(element.getJsonPresentation());
    if (iter.hasNext()) {
      buf.append(",");
    }
  }
  buf.append("}");
  return buf.toString();
}
