{
  if (checkable && !isSeparator) {
    this.checked=checked;
    if (checked) {
      addStyleDependentName("checked");
      removeStyleDependentName("unchecked");
    }
 else {
      addStyleDependentName("unchecked");
      removeStyleDependentName("checked");
    }
  }
 else {
    this.checked=false;
  }
}
