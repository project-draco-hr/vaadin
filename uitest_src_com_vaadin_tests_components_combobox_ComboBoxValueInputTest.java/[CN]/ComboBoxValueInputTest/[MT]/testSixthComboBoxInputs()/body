{
  ComboBoxElement comboBox=$(ComboBoxElement.class).all().get(5);
  assertEquals("correct selection not found","Null item",comboBox.findElement(By.tagName("input")).getAttribute("value"));
  assertEquals("correct selection not found","true",comboBox.findElement(By.tagName("input")).getAttribute("readonly"));
  assertFalse("button visible on readonly combobox",comboBox.findElement(By.vaadin("#button")).isDisplayed());
}
