{
  Element activeRow=null;
  final Element activeElement=Util.getFocusedElement();
  if (root.isOrHasChild(activeElement)) {
    Element e=activeElement;
    while (e != null && e != root) {
      if (e.getTagName().equalsIgnoreCase("TR")) {
        activeRow=e;
      }
      e=e.getParentElement();
    }
  }
  return activeRow;
}
