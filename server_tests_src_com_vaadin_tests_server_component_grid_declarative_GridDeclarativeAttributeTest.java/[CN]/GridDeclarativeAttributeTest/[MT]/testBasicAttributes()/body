{
  String design="<v-grid editable='true' rows=20 frozen-columns=-1 " + "editor-save-caption='Tallenna' editor-cancel-caption='Peruuta'>";
  Grid grid=new Grid();
  grid.setEditorEnabled(true);
  grid.setHeightMode(HeightMode.ROW);
  grid.setHeightByRows(20);
  grid.setFrozenColumnCount(-1);
  grid.setEditorSaveCaption("Tallenna");
  grid.setEditorCancelCaption("Peruuta");
  testRead(design,grid);
  testWrite(design,grid);
}
