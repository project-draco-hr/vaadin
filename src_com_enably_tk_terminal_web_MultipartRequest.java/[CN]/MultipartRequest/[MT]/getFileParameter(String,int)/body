{
  Object[] objArray=null;
  Object value=htFiles.get(strName);
  if (value instanceof Vector)   objArray=(Object[])((Vector)value).firstElement();
 else   objArray=(Object[])htFiles.get(strName);
  if (objArray != null && type >= FILENAME && type <= CONTENTS)   return objArray[type];
 else   return null;
}
