{
  if (strName == null)   return;
  Object objParms=htParameters.get(strName);
  if (objParms instanceof Vector)   ((Vector)objParms).addElement(value);
 else   if (objParms instanceof String) {
    Vector vecParms=new Vector();
    vecParms.addElement(objParms);
    vecParms.addElement(value);
    htParameters.put(strName,vecParms);
  }
 else   htParameters.put(strName,value);
}
