{
  if (intTotalRead >= intContentLength)   return -1;
  int length=bytesToBeRead.length;
  if (length > (intContentLength - intTotalRead))   length=(int)(intContentLength - intTotalRead);
  int result=readLine(in,bytesToBeRead,0,length);
  if (result > 0)   intTotalRead+=result;
  return result;
}
