{
  setCaption("Horizontally sortable layout via (ddwrappers): Try sorting blocks by dragging them");
  DragAndDropWrapper pane=new DragAndDropWrapper(layout);
  setContent(pane);
  pane.setSizeFull();
  setWidth("400px");
  setHeight("100px");
  for (int i=0; i < 4; i++) {
    layout.addComponent(new WrappedLabel("Block"));
  }
}
