{
  StringBuilder sb=new StringBuilder();
  boolean first=true;
  for (  Map.Entry<String,String> cookie : req.cookies().entrySet()) {
    if (!first) {
      sb.append("; ");
    }
 else {
      first=false;
    }
    sb.append(cookie.getKey()).append('=').append(cookie.getValue());
  }
  return sb.toString();
}
