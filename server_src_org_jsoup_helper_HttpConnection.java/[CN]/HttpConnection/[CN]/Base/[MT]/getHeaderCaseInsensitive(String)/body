{
  Validate.notNull(name,"Header name must not be null");
  String value=headers.get(name);
  if (value == null) {
    value=headers.get(name.toLowerCase());
  }
  if (value == null) {
    Map.Entry<String,String> entry=scanHeaders(name);
    if (entry != null) {
      value=entry.getValue();
    }
  }
  return value;
}
