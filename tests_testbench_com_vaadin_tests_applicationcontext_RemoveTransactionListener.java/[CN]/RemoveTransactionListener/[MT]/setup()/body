{
  getMainWindow().getApplication().getContext().addTransactionListener(new TransactionListener(){
    @Override public void transactionStart(    Application application,    Object transactionData){
    }
    @Override public void transactionEnd(    Application application,    Object transactionData){
      removeListener(this);
      log.log("Listener removed in transactionEnd");
    }
  }
);
  getMainWindow().getApplication().getContext().addTransactionListener(new TransactionListener(){
    @Override public void transactionStart(    Application application,    Object transactionData){
      removeListener(this);
      log.log("Listener removed in transactionStart");
    }
    @Override public void transactionEnd(    Application application,    Object transactionData){
    }
  }
);
  getMainWindow().getApplication().getContext().addTransactionListener(new TransactionListener(){
    @Override public void transactionStart(    Application application,    Object transactionData){
      log.log("transactionStart from last listener");
    }
    @Override public void transactionEnd(    Application application,    Object transactionData){
      log.log("transactionEnd from last listener");
    }
  }
);
  addComponent(log);
}
