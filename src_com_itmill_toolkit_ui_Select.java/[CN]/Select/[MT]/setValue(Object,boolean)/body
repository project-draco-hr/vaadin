{
  if (isMultiSelect()) {
    if (newValue == null)     super.setValue(new HashSet(),repaintIsNotNeeded);
 else     if (Collection.class.isAssignableFrom(newValue.getClass()))     super.setValue(new HashSet((Collection)newValue),repaintIsNotNeeded);
  }
 else   if (newValue == null || items.containsId(newValue))   super.setValue(newValue,repaintIsNotNeeded);
}
