{
  if (!"".equals(uri)) {
    DownloadStream ds=null;
    if (relativeUri.indexOf(uri + "/feedMoreItems/") != -1) {
      int i=0;
      String index=relativeUri.substring(relativeUri.lastIndexOf("/") + 1);
      try {
        i=Integer.parseInt(index);
      }
 catch (      NumberFormatException e) {
      }
      ds=createDownloadStream(13,i,"");
      return ds;
    }
 else     if (relativeUri.indexOf(uri) != -1) {
      String prefix=relativeUri.substring(relativeUri.lastIndexOf("/") + 1);
      ds=createDownloadStream(13,0,prefix.trim());
      return ds;
    }
  }
  return null;
}
