{
  String s="";
  if (top >= 0) {
    s+="top:" + top + UNIT_SYMBOLS[topUnits]+ ";";
  }
  if (right >= 0) {
    s+="right:" + right + UNIT_SYMBOLS[rightUnits]+ ";";
  }
  if (bottom >= 0) {
    s+="bottom:" + bottom + UNIT_SYMBOLS[bottomUnits]+ ";";
  }
  if (left >= 0) {
    s+="left:" + left + UNIT_SYMBOLS[leftUnits]+ ";";
  }
  if (zIndex >= 0) {
    s+="z-index:" + zIndex + ";";
  }
  return s;
}
