{
  String s="";
  if (topValue >= 0) {
    s+="top:" + topValue + UNIT_SYMBOLS[topUnits]+ ";";
  }
  if (rightValue >= 0) {
    s+="right:" + rightValue + UNIT_SYMBOLS[rightUnits]+ ";";
  }
  if (bottomValue >= 0) {
    s+="bottom:" + bottomValue + UNIT_SYMBOLS[bottomUnits]+ ";";
  }
  if (leftValue >= 0) {
    s+="left:" + leftValue + UNIT_SYMBOLS[leftUnits]+ ";";
  }
  if (zIndex >= 0) {
    s+="z-index:" + zIndex + ";";
  }
  return s;
}
