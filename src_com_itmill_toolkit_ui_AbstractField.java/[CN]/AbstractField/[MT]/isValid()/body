{
  if (isRequired()) {
    if (isEmpty()) {
      return false;
    }
  }
  if (validators == null) {
    return true;
  }
  final Object value=getValue();
  for (final Iterator i=validators.iterator(); i.hasNext(); ) {
    if (!((Validator)i.next()).isValid(value)) {
      return false;
    }
  }
  return true;
}
