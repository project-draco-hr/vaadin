{
  try {
    Paintable component=drag.getTransferrable().getComponent();
    String requiredPid=configuration.getStringAttribute("component");
    Paintable paintable=VDragAndDropManager.get().getCurrentDropHandler().getApplicationConnection().getPaintable(requiredPid);
    return paintable == component;
  }
 catch (  Exception e) {
  }
  return false;
}
