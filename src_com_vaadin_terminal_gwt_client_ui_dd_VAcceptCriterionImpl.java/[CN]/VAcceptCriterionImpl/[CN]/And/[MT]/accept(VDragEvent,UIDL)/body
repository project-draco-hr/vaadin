{
  UIDL childUIDL=configuration.getChildUIDL(0);
  UIDL childUIDL2=configuration.getChildUIDL(1);
  VAcceptCriteria acceptCriteria=VAcceptCriterion.get(childUIDL.getStringAttribute("name"));
  VAcceptCriteria acceptCriteria2=VAcceptCriterion.get(childUIDL2.getStringAttribute("name"));
  if (acceptCriteria == null || acceptCriteria2 == null) {
    ApplicationConnection.getConsole().log("And criteria didn't found a chidl criteria");
    return false;
  }
  boolean accept=acceptCriteria.accept(drag,childUIDL);
  boolean accept2=acceptCriteria2.accept(drag,childUIDL2);
  return accept && accept2;
}
