{
  return new BrowserDetails(){
    @Override public String getUriFragment(){
      return null;
    }
    @Override public String getWindowName(){
      return null;
    }
    @Override public WebBrowser getWebBrowser(){
      PortletApplicationContext2 context=(PortletApplicationContext2)Application.getCurrent().getContext();
      return context.getBrowser();
    }
  }
;
}
