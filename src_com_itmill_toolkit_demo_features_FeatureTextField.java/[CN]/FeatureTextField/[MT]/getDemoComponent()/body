{
  OrderedLayout l=new OrderedLayout(OrderedLayout.ORIENTATION_HORIZONTAL);
  TextField tf=new TextField("Caption");
  l.addComponent(tf);
  propertyPanel=new PropertyPanel(tf);
  Form f=propertyPanel.createBeanPropertySet(new String[]{"columns","rows","wordwrap","writeThrough","readThrough","nullRepresentation","nullSettingAllowed","secret"});
  propertyPanel.addProperties("Text field properties",f);
  return l;
}
