{
  Element parentOfVApp=getElement().getParentElement().getParentElement();
  if (parentOfVApp != null && parentOfVApp.getClassName().contains("portlet-body")) {
    for (    String direction : new String[]{"height","width"}) {
      if (childUidl.hasAttribute(direction) && childUidl.getStringAttribute(direction).contains("%")) {
        if ("".equals(parentOfVApp.getStyle().getProperty(direction))) {
          parentOfVApp.getStyle().setProperty(direction,"100%");
        }
      }
 else       if (parentOfVApp.getStyle().getHeight().contains("%")) {
        parentOfVApp.getStyle().setProperty(direction,"");
      }
    }
  }
}
