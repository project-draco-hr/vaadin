{
  if (i == 0) {
    if (widget != null) {
      return widget;
    }
 else     if (caption != null) {
      return caption;
    }
 else {
      throw new NoSuchElementException();
    }
  }
 else   if (i == 1) {
    if (widget != null && caption != null) {
      return caption;
    }
 else {
      throw new NoSuchElementException();
    }
  }
 else {
    throw new NoSuchElementException();
  }
}
