{
  int toRemove=id - 1;
  if (toRemove == 0) {
    if (widget != null) {
      ChildComponentContainer.this.remove(widget);
    }
 else     if (caption != null) {
      ChildComponentContainer.this.remove(caption);
    }
 else {
      throw new IllegalStateException();
    }
  }
 else   if (toRemove == 1) {
    if (widget != null && caption != null) {
      ChildComponentContainer.this.remove(caption);
    }
 else {
      throw new IllegalStateException();
    }
  }
 else {
    throw new IllegalStateException();
  }
  id--;
}
