{
  if (getRpcManager(ContextClickRpc.class.getName()) == null) {
    registerRpc(new ContextClickRpc(){
      @Override public void contextClick(      MouseEventDetails details){
        fireEvent(new ContextClickEvent(AbstractComponent.this,details));
      }
    }
);
  }
  addListener(EventId.CONTEXT_CLICK,ContextClickEvent.class,listener,ContextClickEvent.CONTEXT_CLICK_METHOD);
}
