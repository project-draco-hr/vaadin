{
  NodeList<Node> childNodes=scrolledElement2.getChildNodes();
  ArrayList<Element> l=new ArrayList<Element>();
  for (int i=0; i < childNodes.getLength(); i++) {
    Node item=childNodes.getItem(i);
    if (item.getNodeType() == Node.ELEMENT_NODE) {
      l.add((Element)item);
    }
  }
  return l;
}
