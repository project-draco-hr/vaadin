{
  return new Application(){
{
      addUIProvider(new DefaultRootProvider());
    }
    @Override public String getProperty(    String name){
      if (name.equals(UI_PARAMETER)) {
        return MyRoot.class.getName();
      }
 else {
        return super.getProperty(name);
      }
    }
    @Override public UI getUIForRequest(    WrappedRequest request) throws UIRequiresMoreInformationException {
      return getUI(request);
    }
  }
;
}
