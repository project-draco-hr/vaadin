{
  return new Application(){
{
      addRootProvider(new DefaultRootProvider());
    }
    @Override public String getProperty(    String name){
      if (name.equals(ROOT_PARAMETER)) {
        return MyRoot.class.getName();
      }
 else {
        return super.getProperty(name);
      }
    }
    @Override public UI getRootForRequest(    WrappedRequest request) throws RootRequiresMoreInformationException {
      return getRoot(request);
    }
  }
;
}
