{
  LoggingClassLoader loggingClassLoader=new LoggingClassLoader();
  Application application=createStubApplication();
  application.start(new ApplicationStartEvent(null,createConfigurationMock(),null));
  Root root=application.getRootForRequest(createRequestMock(loggingClassLoader));
  assertTrue(root instanceof MyRoot);
  assertEquals(1,loggingClassLoader.requestedClasses.size());
  assertEquals(MyRoot.class.getName(),loggingClassLoader.requestedClasses.get(0));
}
