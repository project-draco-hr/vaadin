{
  if (!isAttached()) {
    return;
  }
  contentAreaBorderPadding=Util.measureHorizontalPaddingAndBorder(contents,4);
  int wrapperPaddingBorder=Util.measureHorizontalPaddingAndBorder(wrapper,0) + Util.measureHorizontalPaddingAndBorder(wrapper2,0);
  contentAreaToRootDifference=wrapperPaddingBorder + contentAreaBorderPadding;
}
