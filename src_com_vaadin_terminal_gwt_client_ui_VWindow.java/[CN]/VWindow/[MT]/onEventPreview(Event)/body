{
  if (dragging) {
    onDragEvent(event);
    return false;
  }
 else   if (resizing) {
    onResizeEvent(event);
    return false;
  }
 else   if (vaadinModality) {
    final Element target=event.getTarget().cast();
    if (DOM.getCaptureElement() != null) {
      return true;
    }
    if (!DOM.isOrHasChild(getElement(),target)) {
      Console console=ApplicationConnection.getConsole();
      if (console instanceof VDebugConsole && DOM.isOrHasChild(((VDebugConsole)console).getElement(),target)) {
        return true;
      }
      return false;
    }
  }
  return true;
}
