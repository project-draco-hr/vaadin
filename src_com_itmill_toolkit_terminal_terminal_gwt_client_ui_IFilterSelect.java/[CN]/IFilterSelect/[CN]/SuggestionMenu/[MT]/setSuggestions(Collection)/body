{
  clearItems();
  final Iterator it=suggestions.iterator();
  while (it.hasNext()) {
    final FilterSelectSuggestion s=(FilterSelectSuggestion)it.next();
    final MenuItem mi=new MenuItem(s.getDisplayString(),true,s);
    this.addItem(mi);
    if (s == currentSuggestion) {
      selectItem(mi);
    }
  }
}
