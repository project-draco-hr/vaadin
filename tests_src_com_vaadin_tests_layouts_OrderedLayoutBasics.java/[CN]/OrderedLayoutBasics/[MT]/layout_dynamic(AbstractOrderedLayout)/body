{
  ol.setMargin(true);
  ol.setSpacing(true);
  ol.setCaption("Dynamic width, dynamic height");
  for (int i=0; i < 3; i++) {
    Button b=new Button("Button " + i);
    if (i == 2) {
      b.setHeight("200px");
    }
 else {
      b.setHeight("100%");
    }
    ol.addComponent(b);
  }
  return ol;
}
