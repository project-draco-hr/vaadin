{
  createSelectAction("Icon",category,createIconOptions(false),"-",new Command<ComboBox,Resource>(){
    @Override public void execute(    ComboBox c,    Resource value,    Object data){
      for (      Object id : c.getItemIds()) {
        if (value == null) {
          c.setItemIcon(id,null);
        }
 else {
          c.setItemIcon(id,value);
        }
      }
    }
  }
);
}
