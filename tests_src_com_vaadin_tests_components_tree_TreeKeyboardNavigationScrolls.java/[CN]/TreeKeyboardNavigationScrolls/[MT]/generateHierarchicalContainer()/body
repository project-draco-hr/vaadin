{
  HierarchicalContainer cont=new HierarchicalContainer();
  for (int i=1; i < 6; i++) {
    cont.addItem(i);
    for (int j=1; j < 3; j++) {
      String id=i + " foo bar baz make this node really wide so that we don't have to fiddle with resizing the browser window -> what would you do if you had one of your legs on backwards? it's legs time! everybody get your legs! " + j;
      cont.addItem(id);
      cont.setChildrenAllowed(id,false);
      cont.setParent(id,i);
    }
  }
  return cont;
}
