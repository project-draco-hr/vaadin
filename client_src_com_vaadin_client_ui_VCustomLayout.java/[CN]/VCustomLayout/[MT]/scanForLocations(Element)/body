{
  final String location=elem.getAttribute("location");
  if (!"".equals(location)) {
    locationToElement.put(location,elem);
    elem.setInnerHTML("");
  }
 else {
    final int len=DOM.getChildCount(elem);
    for (int i=0; i < len; i++) {
      scanForLocations(DOM.getChild(elem,i));
    }
  }
}
