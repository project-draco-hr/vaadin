{
  setDebug(true);
  openTestURL();
  startTimer();
  waitUntilServerCounterChanges();
  disconnectProxy();
  Assert.assertEquals(0,getClientCounter());
  getIncrementClientCounterButton().click();
  Assert.assertEquals(0,getClientCounter());
  Thread.sleep(1000);
  connectProxy();
  waitUntilServerCounterChanges();
  Assert.assertEquals(1,getClientCounter());
}
