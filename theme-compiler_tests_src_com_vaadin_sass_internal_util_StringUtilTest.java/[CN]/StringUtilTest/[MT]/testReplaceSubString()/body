{
  String sentence="var1 var2";
  String word="var";
  String value="abc";
  word="var1";
  Assert.assertEquals("abc var2",StringUtil.replaceSubString(sentence,word,value));
  String var2="var1 abc";
  Assert.assertEquals(sentence,StringUtil.replaceSubString(sentence,var2,value));
  Assert.assertEquals(".foo",StringUtil.replaceSubString(".foo","oo","aa"));
}
