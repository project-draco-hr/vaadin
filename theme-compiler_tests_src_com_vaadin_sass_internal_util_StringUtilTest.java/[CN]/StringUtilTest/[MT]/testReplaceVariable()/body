{
  String sentence="$var1 var2";
  String word="var";
  String value="abc";
  Assert.assertEquals(sentence,StringUtil.replaceVariable(sentence,word,value));
  word="var1";
  Assert.assertEquals("abc var2",StringUtil.replaceVariable(sentence,word,value));
  String var2="var2";
  Assert.assertEquals(sentence,StringUtil.replaceVariable(sentence,var2,value));
}
