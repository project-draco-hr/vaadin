{
  List<String> attributes=new ArrayList<String>();
  for (  Method method : clazz.getMethods()) {
    if (method.getName().startsWith("set") && method.getParameterTypes().length == 1 && isSupported(method.getParameterTypes()[0]) && findGetterForSetter(clazz,method) != null) {
      attributes.add(toAttributeName(method.getName()));
    }
  }
  return attributes;
}
