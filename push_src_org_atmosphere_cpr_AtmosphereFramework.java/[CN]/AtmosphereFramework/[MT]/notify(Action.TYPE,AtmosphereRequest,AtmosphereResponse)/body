{
  for (  AsyncSupportListener l : asyncSupportListeners()) {
    try {
switch (type) {
case TIMEOUT:
        l.onTimeout(request,response);
      break;
case CANCELLED:
    l.onClose(request,response);
  break;
case SUSPEND:
l.onSuspend(request,response);
break;
case RESUME:
l.onSuspend(request,response);
break;
case DESTROYED:
l.onDestroyed(request,response);
break;
}
}
 catch (Throwable t) {
logger.warn("",t);
}
}
}
