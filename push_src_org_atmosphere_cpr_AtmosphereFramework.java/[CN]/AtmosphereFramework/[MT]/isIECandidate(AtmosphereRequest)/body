{
  String userAgent=request.getHeader("User-Agent");
  if (userAgent == null)   return false;
  if (userAgent.contains("MSIE") || userAgent.contains(".NET")) {
    String transport=request.getHeader(HeaderConfig.X_ATMOSPHERE_TRANSPORT);
    if (transport != null) {
      return false;
    }
 else {
      return true;
    }
  }
  return false;
}
