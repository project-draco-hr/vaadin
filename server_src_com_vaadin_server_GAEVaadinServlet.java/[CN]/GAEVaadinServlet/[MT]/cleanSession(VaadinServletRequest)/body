{
  WrappedSession wrappedSession=request.getWrappedSession(false);
  if (wrappedSession == null) {
    return;
  }
  VaadinServiceSession serviceSession=VaadinServiceSession.getForSession(getService(),wrappedSession);
  if (serviceSession == null) {
    return;
  }
  serviceSession.removeFromSession(getService());
}
