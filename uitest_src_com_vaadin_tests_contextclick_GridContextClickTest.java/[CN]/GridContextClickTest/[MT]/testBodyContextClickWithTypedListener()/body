{
  addOrRemoveTypedListener();
  contextClick($(GridElement.class).first().getCell(0,0));
  assertEquals("1. ContextClickEvent value: Lisa Schneider, propertyId: address, section: BODY",getLogRow(0));
  contextClick($(GridElement.class).first().getCell(0,3));
  assertEquals("2. ContextClickEvent value: Lisa Schneider, propertyId: lastName, section: BODY",getLogRow(0));
}
