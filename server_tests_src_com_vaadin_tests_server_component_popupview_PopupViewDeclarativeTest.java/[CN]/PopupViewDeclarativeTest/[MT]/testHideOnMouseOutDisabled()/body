{
  final Label label=new Label("Foo");
  PopupView component=new PopupView("Click Me!",label);
  component.setHideOnMouseOut(false);
  String design="<v-popup-view hide-on-mouse-out='false'>" + "Click Me!" + "<popup-content>" + new DesignContext().createElement(label) + "</popup-content>"+ "</v-popup-view>";
  testWrite(design,component);
  testRead(design,component);
}
