{
  if (fieldValue == JSONObject.NULL) {
    fieldValue=null;
  }
  if (referenceValue == JSONObject.NULL) {
    referenceValue=null;
  }
  if (fieldValue == referenceValue) {
    return true;
  }
 else   if (fieldValue == null || referenceValue == null) {
    return false;
  }
 else {
    return fieldValue.toString().equals(referenceValue.toString());
  }
}
