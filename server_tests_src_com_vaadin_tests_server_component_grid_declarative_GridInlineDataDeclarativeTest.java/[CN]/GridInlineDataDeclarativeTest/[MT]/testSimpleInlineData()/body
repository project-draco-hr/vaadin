{
  String design="<v-grid><table>" + "<colgroup>" + "   <col sortable='' property-id='Col1' />"+ "</colgroup>"+ "<thead />"+ "<tbody>"+ "<tr><td>Foo</tr>"+ "<tr><td>Bar</tr>"+ "<tr><td>Baz</tr>"+ "</table></v-grid>";
  Grid grid=new Grid();
  grid.addColumn("Col1",String.class);
  grid.addRow("Foo");
  grid.addRow("Bar");
  grid.addRow("Baz");
  grid.removeHeaderRow(grid.getDefaultHeaderRow());
  testWrite(design,grid,true);
  testRead(design,grid,true,true);
}
