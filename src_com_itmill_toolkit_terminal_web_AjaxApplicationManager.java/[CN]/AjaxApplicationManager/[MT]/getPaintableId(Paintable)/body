{
  String id=(String)paintableIdMap.get(paintable);
  if (id == null) {
    String UIID=null;
    if (paintable instanceof Identifiable)     UIID=((Identifiable)paintable).getUIID();
    if (UIID != null)     id="PID" + UIID;
 else {
      id="PID" + Integer.toString(idSequence++);
    }
  }
  paintableIdMap.put(paintable,id);
  return id;
}
