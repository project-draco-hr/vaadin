{
  for (int i=0; i < 4; i++) {
    Button b=new Button("B");
    b.setSizeFull();
    gl.addComponent(b);
  }
  String caption=gl.getClass().getSimpleName();
  caption+=" style: " + gl.getStyleName() + ", spacingFromServer:"+ ((SpacingHandler)gl).isSpacing();
  gl.setCaption(caption);
  getMainWindow().addComponent(gl);
}
