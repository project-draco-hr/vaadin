{
  super.setWidgetStyleNameWithPrefix(prefix,styleName,add);
  if (!styleName.startsWith("-")) {
    getWidget().popup.setStyleName(getWidget().getStylePrimaryName() + "-popup-" + styleName,add);
  }
 else {
    getWidget().popup.setStyleName(getWidget().getStylePrimaryName() + "-popup" + styleName,add);
  }
}
