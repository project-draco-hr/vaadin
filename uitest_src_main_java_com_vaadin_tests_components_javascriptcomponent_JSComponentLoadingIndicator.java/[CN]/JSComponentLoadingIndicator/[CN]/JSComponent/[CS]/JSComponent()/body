{
  addFunction("test",new JavaScriptFunction(){
    @Override public void call(    JsonArray arguments){
      try {
        Thread.sleep(1000);
        Label label=new Label("pong");
        label.addStyleName("pong");
        addComponent(label);
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
);
}
