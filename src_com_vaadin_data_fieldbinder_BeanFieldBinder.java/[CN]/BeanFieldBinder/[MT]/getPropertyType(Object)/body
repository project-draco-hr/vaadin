{
  if (getItemDataSource() != null) {
    return super.getPropertyType(propertyId);
  }
 else {
    java.lang.reflect.Field f;
    try {
      f=getField(beanType,propertyId.toString());
      return f.getType();
    }
 catch (    SecurityException e) {
      throw new BindException("Cannot determine type of propertyId '" + propertyId + "'.",e);
    }
catch (    NoSuchFieldException e) {
      throw new BindException("Cannot determine type of propertyId '" + propertyId + "'. The propertyId was not found in "+ beanType.getName(),e);
    }
  }
}
