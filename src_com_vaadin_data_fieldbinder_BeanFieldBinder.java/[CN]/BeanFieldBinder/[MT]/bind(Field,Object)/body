{
  try {
    super.bind(field,propertyId);
  }
 catch (  BindException e) {
    if (getItemDataSource() != null && field.getPropertyDataSource() == null) {
      Property p=new NestedMethodProperty(getItemDataSource().getBean(),(String)propertyId);
      getItemDataSource().addItemProperty(propertyId,p);
      super.bind(field,propertyId);
    }
  }
}
