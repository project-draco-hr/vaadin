{
  margin=DOM.createDiv();
  Element table=DOM.createTable();
  Element tBody=DOM.createTBody();
  childContainer=orientationMode == ORIENTATION_HORIZONTAL ? DOM.createTR() : tBody;
  DOM.appendChild(table,tBody);
  if (orientationMode == ORIENTATION_HORIZONTAL)   DOM.appendChild(tBody,childContainer);
  setElement(table);
  DOM.setElementAttribute(table,"cellSpacing","0");
  DOM.setElementAttribute(table,"cellPadding","0");
}
