{
  if (height != null && width != null) {
    return true;
  }
  int currentHeight=getElement().getOffsetHeight();
  int currentWidth=getElement().getOffsetWidth();
  if (currentHeight != renderedHeight || currentWidth != renderedWidth) {
    iLayout(-1,-1);
    return false;
  }
 else {
    return true;
  }
}
