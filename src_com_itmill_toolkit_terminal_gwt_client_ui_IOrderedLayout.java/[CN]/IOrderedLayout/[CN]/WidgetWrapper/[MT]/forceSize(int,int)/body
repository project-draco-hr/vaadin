{
  if (lastForcedPixelHeight == pixelHeight && lastForcedPixelWidth == pixelWidth) {
    return;
  }
  if (tableMode && (pixelHeight >= 0 || pixelWidth >= 0)) {
    if (clipperDiv == null) {
      createClipperDiv();
    }
  }
 else   if (clipperDiv != null) {
    removeClipperDiv();
  }
  Element e=clipperDiv != null ? clipperDiv : getElementWrappingAlignmentStructures();
  DOM.setStyleAttribute(e,"overflow",pixelWidth < 0 && pixelHeight < 0 ? "" : "hidden");
  DOM.setStyleAttribute(e,"width",pixelWidth < 0 ? "" : pixelWidth + "px");
  if (pixelHeight >= 0) {
    DOM.setStyleAttribute(e,"height",pixelHeight + "px");
  }
 else {
    if (e == clipperDiv && !BrowserInfo.get().isIE()) {
      DOM.setStyleAttribute(e,"height","100%");
    }
  }
  lastForcedPixelWidth=pixelWidth;
  lastForcedPixelHeight=pixelHeight;
}
