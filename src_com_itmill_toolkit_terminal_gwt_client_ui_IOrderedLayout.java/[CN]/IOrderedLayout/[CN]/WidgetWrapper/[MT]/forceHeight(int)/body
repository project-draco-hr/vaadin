{
  if (lastForcedPixelHeight == pixelHeight) {
    return;
  }
  if (tableMode) {
    if (pixelHeight >= 0) {
      if (clipperDiv == null) {
        createClipperDiv();
      }
    }
 else     if (clipperDiv != null && lastForcedPixelWidth < 0) {
      removeClipperDiv();
    }
  }
  Element e=clipperDiv != null ? clipperDiv : getWrappingElement();
  DOM.setStyleAttribute(e,"overflowY",pixelHeight < 0 ? "" : "hidden");
  DOM.setStyleAttribute(e,"height",pixelHeight < 0 ? (e == clipperDiv || !tableMode ? "100%" : "") : pixelHeight + "px");
  lastForcedPixelHeight=pixelHeight;
}
