{
  if (lastForcedPixelHeight == -1) {
    if (height == null) {
      return getElementWrappingClipperDiv().getOffsetHeight() - horizontalPadding;
    }
    return -1;
  }
  int available=lastForcedPixelHeight;
  if (caption != null) {
    available-=caption.getOffsetHeight();
  }
  return available;
}
