{
  String versions=System.getProperty("vaadin.version");
  if (versions == null || versions.equals("")) {
    usage();
  }
  String milestone="";
  for (  String version : versions.split(" ")) {
    if (!milestone.equals("")) {
      milestone+="&amp;";
    }
    milestone+="milestone=Vaadin+" + version;
  }
  printMilestone(milestone);
}
