{
  if (n.getNodeName().equals("window")) {
    console.log("Rendering main window");
    rw=new RootWindow(n,this);
    rw.setClient(this);
  }
 else {
    int pid=Component.getIdFromUidl(n);
    console.log("Updating node: " + n.getNodeName() + ", PID:"+ pid);
    Component c=getPaintable(pid);
    c.updateFromUidl(n);
  }
}
