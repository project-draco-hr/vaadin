{
  ComboBox combo=(ComboBox)field;
  ServerRpcMethodInvocation invocation=new ServerRpcMethodInvocation(combo.getConnectorId(),ComboBoxServerRpc.class,"setSelectedItem",1);
  invocation.setParameters(new Object[]{"myvalue"});
  try {
    ServerRpcManager.applyInvocation(combo,invocation);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
