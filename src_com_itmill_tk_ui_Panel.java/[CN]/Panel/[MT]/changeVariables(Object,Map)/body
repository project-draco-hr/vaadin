{
  super.changeVariables(source,variables);
  Integer newWidth=(Integer)variables.get("width");
  Integer newHeight=(Integer)variables.get("height");
  if (newWidth != null && newWidth.intValue() != getWidth())   setWidth(newWidth.intValue());
  if (newHeight != null && newHeight.intValue() != getHeight())   setHeight(newHeight.intValue());
  Integer newScrollX=(Integer)variables.get("scrollleft");
  Integer newScrollY=(Integer)variables.get("scrolldown");
  if (newScrollX != null && newScrollX.intValue() != getScrollOffsetX())   setScrollOffsetX(newScrollX.intValue());
  if (newScrollY != null && newScrollY.intValue() != getScrollOffsetY())   setScrollOffsetY(newScrollY.intValue());
}
