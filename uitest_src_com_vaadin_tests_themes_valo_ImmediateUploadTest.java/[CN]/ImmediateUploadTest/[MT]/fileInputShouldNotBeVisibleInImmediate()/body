{
  openTestURL();
  UploadElement normalUpload=$(UploadElement.class).id("upload");
  UploadElement immediateUpload=$(UploadElement.class).id("immediateupload");
  WebElement normalUploadInput=normalUpload.findElement(By.cssSelector("input[type='file']"));
  WebElement immediateUploadInput=immediateUpload.findElement(By.cssSelector("input[type='file']"));
  WebElement normalUploadButton=normalUpload.findElement(By.tagName("div"));
  WebElement immediateUploadButton=immediateUpload.findElement(By.tagName("div"));
  assertThat(normalUploadButton.getCssValue("display"),equalToIgnoringCase("block"));
  assertThat(immediateUploadButton.getCssValue("display"),equalToIgnoringCase("block"));
  assertThat(normalUploadInput.getCssValue("position"),equalToIgnoringCase("static"));
  assertThat(immediateUploadInput.getCssValue("position"),equalToIgnoringCase("absolute"));
}
