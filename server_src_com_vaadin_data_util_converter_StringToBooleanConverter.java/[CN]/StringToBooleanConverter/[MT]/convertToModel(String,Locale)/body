{
  if (value == null) {
    return null;
  }
  value=value.trim();
  if (getTrueString().equals(value)) {
    return true;
  }
 else   if (getFalseString().equals(value)) {
    return false;
  }
 else {
    throw new ConversionException("Cannot convert " + value + " to "+ getModelType().getName());
  }
}
