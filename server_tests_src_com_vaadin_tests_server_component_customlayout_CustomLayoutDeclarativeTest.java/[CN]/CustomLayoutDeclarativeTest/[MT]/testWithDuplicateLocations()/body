{
  String design="<v-custom-layout>" + "<v-button plain-text :location='foo'></v-button>" + "<v-label plain-text :location='foo'></v-label>"+ "</v-custom-layout>";
  CustomLayout expected=new CustomLayout();
  expected.addComponent(new Button(),"foo");
  expected.addComponent(new Label(),"foo");
  testRead(design,expected);
  String written="<v-custom-layout>" + "<v-label plain-text :location='foo'></v-label>" + "</v-custom-layout>";
  testWrite(written,expected);
}
