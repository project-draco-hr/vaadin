{
  String design="<vaadin-custom-layout>" + "<vaadin-button plain-text :location='b'></vaadin-button>" + "<vaadin-label plain-text :location='l'></vaadin-label>"+ "</vaadin-custom-layout>";
  CustomLayout expected=new CustomLayout();
  expected.addComponent(new Button(),"b");
  expected.addComponent(new Label(),"l");
  test(design,expected);
}
