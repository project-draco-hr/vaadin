{
  if (c == null) {
    throw new IllegalArgumentException();
  }
  componentList.add(c);
  if (componentToCoord.get(c) == null) {
    componentToCoord.put(c,new Coordinates(0,0,-1,-1,-1,-1));
  }
  super.addComponent(c);
  requestRepaint();
  ((Coordinates)componentToCoord.get(c)).addParentLayout(this);
}
