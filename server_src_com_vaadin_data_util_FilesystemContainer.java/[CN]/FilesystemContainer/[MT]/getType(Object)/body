{
  if (propertyId.equals(PROPERTY_NAME)) {
    return String.class;
  }
  if (propertyId.equals(PROPERTY_ICON)) {
    return Resource.class;
  }
  if (propertyId.equals(PROPERTY_SIZE)) {
    return Long.class;
  }
  if (propertyId.equals(PROPERTY_LASTMODIFIED)) {
    return Date.class;
  }
  return null;
}
