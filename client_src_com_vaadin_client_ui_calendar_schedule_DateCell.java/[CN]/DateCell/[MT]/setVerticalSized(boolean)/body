{
  if (isVerticalSized) {
    addStyleDependentName("Vsized");
    startingSlotHeight=slotElements[0].getOffsetHeight();
    updateEventCellsHeight();
    recalculateEventPositions();
    if (isToday()) {
      recalculateTimeBarPosition();
    }
  }
 else {
    removeStyleDependentName("Vsized");
  }
}
