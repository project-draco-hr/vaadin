{
  WrappedRequest request=context.getRequest();
  String widgetset=context.getApplication().getWidgetsetForUI(context.getRequest(),context.getUIClass());
  if (widgetset == null) {
    widgetset=request.getDeploymentConfiguration().getConfiguredWidgetset(request);
  }
  widgetset=AbstractApplicationServlet.stripSpecialChars(widgetset);
  return widgetset;
}
