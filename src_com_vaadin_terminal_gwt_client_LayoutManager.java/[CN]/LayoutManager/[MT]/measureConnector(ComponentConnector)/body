{
  MeasuredSize measuredSize=getMeasuredSize(connector);
  if (!isManagedLayout(connector) && !isManagedLayout(connector.getParent()) && elementResizeListeners.get(connector.getWidget().getElement()) == null && !measuredSize.hasDependents()) {
    return;
  }
  Element element=connector.getWidget().getElement();
  MeasureResult measureResult=measuredAndUpdate(element,measuredSize);
  if (measureResult.isChanged()) {
    onConnectorChange(connector,measureResult.isWidthChanged(),measureResult.isHeightChanged());
  }
}
