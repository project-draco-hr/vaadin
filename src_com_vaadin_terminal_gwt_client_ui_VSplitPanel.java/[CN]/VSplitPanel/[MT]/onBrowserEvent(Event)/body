{
switch (DOM.eventGetType(event)) {
case Event.ONMOUSEMOVE:
    if (resizing) {
      onMouseMove(event);
    }
  break;
case Event.ONMOUSEDOWN:
onMouseDown(event);
break;
case Event.ONMOUSEOUT:
if (resizing) {
showDraggingCurtain();
}
break;
case Event.ONMOUSEUP:
if (resizing) {
onMouseUp(event);
}
break;
case Event.ONCLICK:
resizing=false;
break;
}
if (!resized) {
super.onBrowserEvent(event);
}
}
