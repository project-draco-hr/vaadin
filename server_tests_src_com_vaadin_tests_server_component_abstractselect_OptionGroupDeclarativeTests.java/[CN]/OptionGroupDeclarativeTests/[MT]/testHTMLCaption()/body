{
  og.addItems("foo","bar","baz","bang");
  og.setHtmlContentAllowed(true);
  og.setItemCaption("foo","<b>True</b>");
  og.setItemCaption("bar","<font color='red'>False</font>");
  String expected="<v-option-group html-content-allowed='true'>" + "<option item-id=\"foo\"><b>True</b></option>" + "<option item-id=\"bar\"><font color='red'>False</font></option>"+ "<option>baz</option>"+ "<option>bang</option>"+ "</v-option-group>";
  testReadWrite(expected);
}
