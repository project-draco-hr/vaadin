{
  og.addItems("foo","bar","baz","bang");
  og.setItemCaption("foo","<b>True</b>");
  og.setItemCaption("bar","<font color=\"red\">False</font>");
  String expected="<v-option-group>" + "<option item-id=\"foo\">&lt;b&gt;True&lt;/b&gt;</option>" + "<option item-id=\"bar\">&lt;font color=\"red\"&gt;False&lt;/font&gt;</option>"+ "<option>baz</option>"+ "<option>bang</option>"+ "</v-option-group>";
  testReadWrite(expected);
}
