{
  if (startTagHandled && localName.equals("stylesheet"))   return;
  if (localName.equals("stylesheet")) {
    startTagHandled=true;
    this.xslNamespace=namespaceURI;
    handler.startElement(namespaceURI,localName,qName,new AttributeMapper(atts));
  }
 else   handler.startElement(namespaceURI,localName,qName,atts);
}
