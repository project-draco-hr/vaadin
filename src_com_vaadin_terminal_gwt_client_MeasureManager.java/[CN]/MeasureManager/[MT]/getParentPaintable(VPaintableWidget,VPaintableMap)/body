{
  Widget widget=paintable.getWidgetForPaintable();
  while (true) {
    widget=widget.getParent();
    if (widget == null) {
      return null;
    }
    VPaintableWidget parentPaintable=paintableMap.getPaintable(widget);
    if (parentPaintable != null) {
      return parentPaintable;
    }
  }
}
