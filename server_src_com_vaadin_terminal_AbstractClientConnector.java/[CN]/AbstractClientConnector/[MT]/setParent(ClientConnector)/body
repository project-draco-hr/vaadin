{
  if (parent == this.parent) {
    return;
  }
  if (parent != null && this.parent != null) {
    throw new IllegalStateException(getClass().getName() + " already has a parent.");
  }
  if (getApplication() != null) {
    detach();
  }
  this.parent=parent;
  if (getApplication() != null) {
    attach();
  }
}
