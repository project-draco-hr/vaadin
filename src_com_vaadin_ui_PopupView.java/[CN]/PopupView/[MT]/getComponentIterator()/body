{
  return new Iterator<Component>(){
    private boolean first=visibleComponent == null;
    public boolean hasNext(){
      return !first;
    }
    public Component next(){
      if (!first) {
        first=true;
        return visibleComponent;
      }
 else {
        return null;
      }
    }
    public void remove(){
      throw new UnsupportedOperationException();
    }
  }
;
}
