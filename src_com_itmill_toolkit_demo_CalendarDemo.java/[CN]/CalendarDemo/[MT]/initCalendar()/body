{
  try {
    QueryContainer qc=new QueryContainer("SELECT * FROM " + SampleCalendarDatabase.DB_TABLE_NAME,sampleDatabase.getConnection());
    calendar.setContainerDataSource(qc);
  }
 catch (  SQLException e) {
    e.printStackTrace();
  }
  calendar.setItemEndPropertyId(SampleCalendarDatabase.PROPERTY_ID_END);
  calendar.setItemTitlePropertyId(SampleCalendarDatabase.PROPERTY_ID_TITLE);
  calendar.setItemNotimePropertyId(SampleCalendarDatabase.PROPERTY_ID_NOTIME);
}
