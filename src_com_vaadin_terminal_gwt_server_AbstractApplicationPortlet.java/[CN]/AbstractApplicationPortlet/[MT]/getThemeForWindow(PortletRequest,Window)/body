{
  String themeName;
  if (request.getParameter(URL_PARAMETER_THEME) != null) {
    themeName=request.getParameter(URL_PARAMETER_THEME);
  }
 else {
    themeName=window.getTheme();
  }
  if (themeName == null) {
    if (request.getAttribute(AbstractApplicationServlet.REQUEST_DEFAULT_THEME) != null) {
      themeName=(String)request.getAttribute(AbstractApplicationServlet.REQUEST_DEFAULT_THEME);
    }
 else {
      themeName=DEFAULT_THEME_NAME;
    }
  }
  return themeName;
}
