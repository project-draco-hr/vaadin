{
  String userAgent=getHTTPHeader(request,"user-agent");
  browser.updateBrowserProperties(request.getLocale(),null,request.isSecure(),userAgent,getHTTPRequestParameter(request,"sw"),getHTTPRequestParameter(request,"sh"),getHTTPRequestParameter(request,"tzo"),getHTTPRequestParameter(request,"rtzo"),getHTTPRequestParameter(request,"dstd"),getHTTPRequestParameter(request,"dstActive"),getHTTPRequestParameter(request,"curdate"));
}
