{
  writer.write("vaadin.vaadinConfigurations[\"" + getApplicationDomId(request.getPortletRequest()) + "\"] = {");
  Iterator<String> keyIt=config.keySet().iterator();
  while (keyIt.hasNext()) {
    String key=keyIt.next();
    writer.write(key + ": " + config.get(key));
    if (keyIt.hasNext()) {
      writer.write(", ");
    }
  }
  writer.write("};\n");
}
