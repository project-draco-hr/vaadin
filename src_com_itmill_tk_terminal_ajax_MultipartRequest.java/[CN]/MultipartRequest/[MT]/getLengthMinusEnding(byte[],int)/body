{
  if (byteLine == null)   return 0;
  if (endOfArray >= 2 && byteLine[endOfArray - 2] == '\r' && byteLine[endOfArray - 1] == '\n')   return endOfArray - 2;
 else   if (endOfArray >= 1 && byteLine[endOfArray - 1] == '\n' || byteLine[endOfArray - 1] == '\r')   return endOfArray - 1;
 else   return endOfArray;
}
