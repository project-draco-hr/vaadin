{
  VerticalLayout vl=new VerticalLayout();
  Button b;
  b=new Button(CAPTION);
  b.setEnabled(enabled);
  vl.addComponent(b);
  b=new Button(CAPTION);
  b.setEnabled(enabled);
  b.setIcon(ICON);
  vl.addComponent(b);
  b=new Button();
  b.setEnabled(enabled);
  b.setIcon(ICON);
  vl.addComponent(b);
  return vl;
}
