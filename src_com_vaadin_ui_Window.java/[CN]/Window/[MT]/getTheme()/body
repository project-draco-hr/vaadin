{
  if (getParent() != null) {
    return ((Window)getParent()).getTheme();
  }
  if (theme != null) {
    return theme;
  }
  if ((application != null) && (application.getTheme() != null)) {
    return application.getTheme();
  }
  if (terminal != null) {
    return terminal.getDefaultTheme();
  }
  return null;
}
