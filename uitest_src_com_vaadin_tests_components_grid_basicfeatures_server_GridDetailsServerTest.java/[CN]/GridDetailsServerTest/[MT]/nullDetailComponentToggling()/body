{
  selectMenuPath(OPEN_FIRST_ITEM_DETAILS);
  selectMenuPath(DETAILS_GENERATOR_WATCHING);
  selectMenuPath(DETAILS_GENERATOR_NULL);
  try {
    assertTrue("Details should be empty with null component",getGridElement().getDetails(0).getText().isEmpty());
  }
 catch (  NoSuchElementException e) {
    fail("Expected to find a details row with empty content");
  }
  selectMenuPath(DETAILS_GENERATOR_WATCHING);
  assertFalse("Details should be not empty with details component",getGridElement().getDetails(0).getText().isEmpty());
}
