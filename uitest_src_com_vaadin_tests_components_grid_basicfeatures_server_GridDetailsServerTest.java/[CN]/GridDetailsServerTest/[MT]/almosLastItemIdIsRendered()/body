{
  selectMenuPath(CUSTOM_DETAILS_GENERATOR);
  selectMenuPath(ALMOST_LAST_ITEM_DETAILS);
  scrollGridVerticallyTo(100000);
  TestBenchElement details=getGridElement().getDetails(ALMOST_LAST_ITEM_INDEX);
  assertNotNull(details);
  assertTrue("Unexpected details content",details.getText().endsWith(ALMOST_LAST_ITEM_INDEX + " (0)"));
}
