{
  try {
    Runnable runCompiler=new Runnable(){
      public void run(){
        GWTCompiler.main(args);
      }
    }
;
    Thread runThread=new Thread(runCompiler);
    runThread.start();
    runThread.join();
    System.out.println("Widgetset compilation finished");
  }
 catch (  Throwable thr) {
    thr.printStackTrace();
  }
}
