{
  System.err.println((getMainWindow() == getWindow() ? "mainwin: " : "subwin: ") + context + ", "+ relativeUri);
  if (relativeUri != null && relativeUri.startsWith("msg/")) {
    addComponent(new Label(relativeUri));
    return null;
  }
 else {
    return super.handleURI(context,relativeUri);
  }
}
