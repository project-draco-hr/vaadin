{
  T instance=(T)instanceCache.get(instanceClass);
  if (instance == null) {
    try {
      instance=instanceClass.newInstance();
    }
 catch (    InstantiationException e) {
      e.printStackTrace();
    }
catch (    IllegalAccessException e) {
      e.printStackTrace();
    }
    instanceCache.put(instanceClass,instance);
  }
  return instance;
}
