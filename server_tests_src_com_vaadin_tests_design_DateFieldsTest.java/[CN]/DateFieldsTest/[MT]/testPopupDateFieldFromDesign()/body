{
  ByteArrayInputStream bis=new ByteArrayInputStream("<!DOCTYPE html><html><head></head><body><v-popup-date-field show-iso-week-numbers caption=\"Day is\" resolution=\"MINUTE\" range-end=\"2019-01-15\" input-prompt=\"Pick a day\" value=\"2003-02-27 07:15\"></v-popup-date-field></body></html>".getBytes());
  PopupDateField result=(PopupDateField)Design.read(bis);
  assertEquals(Resolution.MINUTE,result.getResolution());
  assertEquals("Day is",result.getCaption());
  assertTrue(result.isShowISOWeekNumbers());
  assertEquals("Pick a day",result.getInputPrompt());
  assertEquals(new SimpleDateFormat("yyyy-MM-dd HH:mm").parse("2003-02-27 07:15"),result.getValue());
  assertEquals(new SimpleDateFormat("yyyy-MM-dd").parse("2019-01-15"),result.getRangeEnd());
}
