{
  String resourceID=null;
  if (iconSize == 16) {
    resourceID="../runo/icons/16/user.png";
  }
 else   if (iconSize == 32) {
    resourceID="../runo/icons/32/user.png";
  }
 else   if (iconSize == 64) {
    resourceID="../runo/icons/64/user.png";
  }
  if (resourceID != null) {
    return new ThemeResource(resourceID + "?" + iconCacheIndex++);
  }
  return null;
}
