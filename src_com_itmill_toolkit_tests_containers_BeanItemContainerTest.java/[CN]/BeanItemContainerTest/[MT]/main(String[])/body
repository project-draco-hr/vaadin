{
  BeanItemContainer<Hello> c=new BeanItemContainer<Hello>(Hello.class);
  c.addItem(new Hello());
  Collection<Hello> col=new LinkedList<Hello>();
  for (int i=0; i < 100; i++) {
    col.add(new Hello());
  }
  c=new BeanItemContainer<Hello>(col);
  System.out.print(c + " contains " + c.size()+ "objects");
}
