{
  if (!strings.hasNext()) {
    return "";
  }
  String start=strings.next().toString();
  if (!strings.hasNext()) {
    return start;
  }
  StringBuilder sb=new StringBuilder(64).append(start);
  while (strings.hasNext()) {
    sb.append(sep);
    sb.append(strings.next());
  }
  return sb.toString();
}
