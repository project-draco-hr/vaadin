{
  if (readOnlyStatusChangeListeners != null) {
    Object[] l=readOnlyStatusChangeListeners.toArray();
    Property.ReadOnlyStatusChangeEvent event=new ObjectProperty.ReadOnlyStatusChangeEvent(this);
    for (int i=0; i < l.length; i++)     ((Property.ReadOnlyStatusChangeListener)l[i]).readOnlyStatusChange(event);
  }
}
