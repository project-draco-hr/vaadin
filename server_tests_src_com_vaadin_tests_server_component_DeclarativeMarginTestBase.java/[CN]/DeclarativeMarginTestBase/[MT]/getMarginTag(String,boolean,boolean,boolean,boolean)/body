{
  String s="<" + componentTag + " ";
  if (left && right && top&& bottom) {
    s+="margin=''";
  }
 else {
    if (left) {
      s+="margin-left='' ";
    }
    if (right) {
      s+="margin-right='' ";
    }
    if (top) {
      s+="margin-top='' ";
    }
    if (bottom) {
      s+="margin-bottom='' ";
    }
  }
  return s + " />";
}
