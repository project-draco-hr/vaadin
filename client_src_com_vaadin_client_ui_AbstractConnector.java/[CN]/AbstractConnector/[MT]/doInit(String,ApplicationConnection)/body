{
  Profiler.enter("AbstractConnector.doInit");
  this.connection=connection;
  id=connectorId;
  addStateChangeHandler(this);
  if (Profiler.isEnabled()) {
    Profiler.enter("AbstractConnector.init " + WidgetUtil.getSimpleName(this));
  }
  init();
  if (Profiler.isEnabled()) {
    Profiler.leave("AbstractConnector.init " + WidgetUtil.getSimpleName(this));
  }
  Profiler.leave("AbstractConnector.doInit");
}
