{
  int w=0;
  int i=getColIndexOf(child);
  HeaderCell headerCell=tHead.getHeaderCell(i);
  if (headerCell != null) {
    if (initializedAndAttached) {
      w=headerCell.getWidth();
    }
 else {
      w=headerCell.getOffsetWidth() - getCellExtraWidth();
    }
  }
  return new RenderSpace(w,getRowHeight());
}
