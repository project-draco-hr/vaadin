{
  int keycode=event.getNativeEvent().getKeyCode();
  if (calendar != null) {
    if (keycode == calendar.getSelectKey() || keycode == calendar.getCloseKey()) {
      if (keycode == calendar.getSelectKey()) {
        notifyServerOfChanges();
      }
      calendar.handleNavigation(keycode,event.getNativeEvent().getCtrlKey() || event.getNativeEvent().getMetaKey(),event.getNativeEvent().getShiftKey());
      return;
    }
  }
  event.stopPropagation();
}
