{
  StringBuffer s=new StringBuffer();
  boolean stop=false;
  Token tok;
  while (!stop) {
    tok=getToken(1);
    if (tok.kind == EOF) {
      return null;
    }
    for (    int sym : symbols) {
      if (tok.kind == sym) {
        stop=true;
        break;
      }
    }
    if (!stop) {
      if (tok.image != null) {
        s.append(tok.image);
      }
      getNextToken();
    }
  }
  return s.toString().trim();
}
