{
  StringBuffer s=new StringBuffer();
  boolean found=false;
  Token tok;
  while (!found) {
    tok=getToken(1);
    for (    int sym : symbols) {
      if (tok.kind == sym) {
        found=true;
        break;
      }
    }
    if (tok.kind == EOF) {
      break;
    }
    if (!found) {
      if (tok.image != null) {
        s.append(tok.image);
      }
      getNextToken();
    }
  }
  return found ? s.toString().trim() : null;
}
