{
  char connector=' ';
switch ((jj_ntk == -1) ? jj_ntk() : jj_ntk) {
case PLUS:
case PRECEDES:
case SIBLING:
    connector=combinatorChar();
  break;
case S:
jj_consume_token(S);
switch ((jj_ntk == -1) ? jj_ntk() : jj_ntk) {
case PLUS:
case PRECEDES:
case SIBLING:
connector=combinatorChar();
break;
default :
jj_la1[58]=jj_gen;
;
}
break;
default :
jj_la1[59]=jj_gen;
jj_consume_token(-1);
throw new ParseException();
}
{
if (true) return connector;
}
throw new Error("Missing return statement in function");
}
