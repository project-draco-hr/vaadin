{
  if (jj_scan_token(EACH_SYM)) {
    return true;
  }
  Token xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_scan_token(1)) {
      jj_scanpos=xsp;
      break;
    }
  }
  if (jj_scan_token(VARIABLE)) {
    return true;
  }
  while (true) {
    xsp=jj_scanpos;
    if (jj_scan_token(1)) {
      jj_scanpos=xsp;
      break;
    }
  }
  if (jj_scan_token(EACH_IN)) {
    return true;
  }
  return false;
}
