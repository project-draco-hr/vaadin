{
  if (jj_scan_token(MICROSOFT_RULE))   return true;
  Token xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_scan_token(1)) {
      jj_scanpos=xsp;
      break;
    }
  }
  if (jj_scan_token(COLON))   return true;
  if (jj_3R_170())   return true;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_170()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
