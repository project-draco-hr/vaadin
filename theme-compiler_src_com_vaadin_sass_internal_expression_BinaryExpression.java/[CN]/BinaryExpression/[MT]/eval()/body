{
  LexicalUnitImpl leftValue=(leftOperand instanceof BinaryExpression) ? ((BinaryExpression)leftOperand).eval() : (LexicalUnitImpl)leftOperand;
  LexicalUnitImpl rightValue=(rightOperand instanceof BinaryExpression) ? ((BinaryExpression)rightOperand).eval() : (LexicalUnitImpl)rightOperand;
  return operator.eval(leftValue,rightValue);
}
