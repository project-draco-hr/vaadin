{
  if (getContext() instanceof PortletApplicationContext) {
    try {
      return user.getFirstName() + " " + user.getLastName();
    }
 catch (    Exception e) {
      e.printStackTrace();
      return "Portlet Demouser";
    }
  }
 else {
    Object user=super.getUser();
    if (user == null) {
      return "Demo User";
    }
 else {
      return user;
    }
  }
}
