{
  MeasuredSize measured=measuredSizes.get(element);
  if (measured != null) {
    return measured;
  }
 else {
    MeasuredSize measuredSize=super.getMeasuredSize(element,null);
    if (measuredSize != null) {
      setMeasuredSize(element,measuredSize);
      return measuredSize;
    }
    return defaultSize;
  }
}
