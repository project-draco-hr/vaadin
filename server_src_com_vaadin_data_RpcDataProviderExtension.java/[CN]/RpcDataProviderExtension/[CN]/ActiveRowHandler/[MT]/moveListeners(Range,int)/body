{
  if (diff < 0) {
    for (Integer i=movedRange.getStart(); i < movedRange.getEnd(); ++i) {
      moveListener(i,i + diff);
    }
  }
 else   if (diff > 0) {
    for (Integer i=movedRange.getEnd() - 1; i >= movedRange.getStart(); --i) {
      moveListener(i,i + diff);
    }
  }
 else {
    return;
  }
}
