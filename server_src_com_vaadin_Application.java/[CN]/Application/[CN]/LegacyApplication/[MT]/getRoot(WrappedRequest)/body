{
  String pathInfo=request.getRequestPathInfo();
  String name=null;
  if (pathInfo != null && pathInfo.length() > 0) {
    Matcher matcher=WINDOW_NAME_PATTERN.matcher(pathInfo);
    if (matcher.matches()) {
      name=matcher.group(1);
    }
  }
  Root.LegacyWindow window=getWindow(name);
  if (window != null) {
    return window;
  }
  return mainWindow;
}
