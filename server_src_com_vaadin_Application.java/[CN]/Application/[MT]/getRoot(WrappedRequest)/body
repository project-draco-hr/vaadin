{
  for (int i=rootProviders.size() - 1; i >= 0; i--) {
    RootProvider provider=rootProviders.get(i);
    Class<? extends Root> rootClass=provider.getRootClass(this,request);
    if (rootClass != null) {
      return provider.instantiateRoot(this,rootClass,request);
    }
  }
  throw new RuntimeException("No root providers available or providers are not able to find root instance");
}
