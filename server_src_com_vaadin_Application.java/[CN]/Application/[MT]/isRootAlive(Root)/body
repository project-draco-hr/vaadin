{
  long now=System.currentTimeMillis();
  if (getHeartbeatTimeout() >= 0 && now - root.getLastHeartbeatTime() > 1000 * getHeartbeatTimeout()) {
    return false;
  }
  if (getUidlRequestTimeout() >= 0 && now - root.getLastUidlRequestTime() > 1000 * getUidlRequestTimeout()) {
    return false;
  }
  return true;
}
