{
  for (Iterator<UI> i=uIs.values().iterator(); i.hasNext(); ) {
    UI ui=i.next();
    if (!isUIAlive(ui)) {
      i.remove();
      retainOnRefreshUIs.values().remove(ui.getUIId());
      ui.fireCloseEvent();
      getLogger().info("Closed UI #" + ui.getUIId() + " due to inactivity");
    }
  }
}
