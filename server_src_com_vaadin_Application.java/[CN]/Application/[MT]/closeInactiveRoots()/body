{
  for (Iterator<Root> i=roots.values().iterator(); i.hasNext(); ) {
    Root root=i.next();
    if (!isRootAlive(root)) {
      i.remove();
      retainOnRefreshRoots.values().remove(root.getRootId());
      root.fireCloseEvent();
      getLogger().info("Closed root #" + root.getRootId() + " due to inactivity");
    }
  }
}
