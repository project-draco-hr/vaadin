{
  openTestURL();
  String theme="reindeer";
  assertMenubarTheme(theme);
  assertCombobBoxTheme(theme);
  assertTableTheme(theme);
  assertEmbeddedTheme(theme);
  theme="runo";
  changeTheme(theme);
  assertMenubarTheme(theme);
  assertCombobBoxTheme(theme);
  assertTableTheme(theme);
  assertEmbeddedTheme(theme);
  theme="reindeer";
  changeTheme(theme);
  assertMenubarTheme(theme);
  assertCombobBoxTheme(theme);
  assertTableTheme(theme);
  assertEmbeddedTheme(theme);
}
