{
  Collection<String> result=super.getCustomAttributes();
  result.add(DESIGN_ATTR_PLAIN_TEXT);
  result.add("caption");
  result.add("icon-alt");
  result.add("icon-alternate-text");
  result.add("click-shortcut");
  result.add("html-content-allowed");
  result.add("caption-as-html");
  return result;
}
