{
  Object val=null;
  System.out.println("converting " + strValue + " of type "+ variableType);
switch (variableType) {
case 'a':
    val=strValue.split(",");
  break;
case 's':
val=strValue;
break;
case 'i':
val=Integer.valueOf(strValue);
break;
case 'l':
val=Long.valueOf(strValue);
case 'f':
val=Float.valueOf(strValue);
break;
case 'd':
val=Double.valueOf(strValue);
break;
case 'b':
val=Boolean.valueOf(strValue);
break;
}
System.out.println("result: " + val + " of type "+ (val == null ? "-" : val.getClass().toString()));
return val;
}
