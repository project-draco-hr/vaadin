{
  String id=paintableIdMap.get(paintable);
  if (id == null) {
    id=paintable.getDebugId();
    if (id == null) {
      id="PID" + Integer.toString(idSequence++);
    }
 else {
      id="PID_S" + id;
    }
    paintableIdMap.put(paintable,id);
    idPaintableMap.put(id,paintable);
  }
  return id;
}
