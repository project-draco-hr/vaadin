{
  super.paintContent(target);
  target.addAttribute("dragStartMode",dragStartMode.ordinal());
  if (getDropHandler() != null) {
    getDropHandler().getAcceptCriterion().paint(target);
  }
  if (receivers != null && receivers.size() > 0) {
    for (Iterator<Entry<String,Html5File>> it=receivers.entrySet().iterator(); it.hasNext(); ) {
      Entry<String,com.vaadin.ui.Html5File> entry=it.next();
      String id=entry.getKey();
      Html5File html5File=entry.getValue();
      if (html5File.getStreamVariable() != null) {
        target.addVariable(this,"rec-" + id,new ProxyReceiver(html5File));
      }
 else {
        target.addVariable(this,"rec-" + id,(String)null);
        it.remove();
      }
    }
  }
}
