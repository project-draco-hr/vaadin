{
  super("Subwindow");
  setWidth("400px");
  Layout hl=(Layout)getContent();
  hl.addComponent(new Label("Current size: "));
  hl.addComponent(sizeLabel);
  addListener(new ResizeListener(){
    @Override public void windowResized(    ResizeEvent e){
      updateLabel();
    }
  }
);
  updateLabel();
}
