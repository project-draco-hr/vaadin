{
  checkDetached();
  if (!isEnabled()) {
    throw new IllegalStateException("This " + getClass().getSimpleName() + " is not enabled");
  }
  Item item=getContainer().getItem(itemId);
  if (item == null) {
    throw new IllegalArgumentException("Item with id " + itemId + " not found in current container");
  }
  fieldGroup.setItemDataSource(item);
  editedItemId=itemId;
}
