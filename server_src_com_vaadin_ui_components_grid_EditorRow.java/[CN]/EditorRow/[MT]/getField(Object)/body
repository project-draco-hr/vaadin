{
  checkDetached();
  final Field<?> field;
  if (fieldGroup.getUnboundPropertyIds().contains(propertyId)) {
    field=fieldGroup.buildAndBind(propertyId);
  }
 else {
    field=fieldGroup.getField(propertyId);
  }
  if (field != null) {
    boolean readonly=fieldGroup.isReadOnly() || field.getPropertyDataSource().isReadOnly() || !isPropertyEditable(propertyId);
    field.setReadOnly(readonly);
  }
  return field;
}
