{
  try {
    Class<?> c=Class.forName(fullyQualifiedClassName);
    if (!baseClass.isAssignableFrom(c)) {
      return;
    }
    if (!Modifier.isAbstract(c.getModifiers()) && !c.isAnonymousClass()) {
      result.add((Class<? extends T>)c);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
catch (  LinkageError e) {
  }
}
