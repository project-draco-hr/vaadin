{
  final List<FlyweightCell> cellsToTheRight=currentIterator.rawPeekNext(numberOfCells - 1);
  final int widthsOfColumnsToTheRight=cellsToTheRight.size() * Escalator.COLUMN_WIDTH_PX;
  final int selfWidth=Escalator.COLUMN_WIDTH_PX;
  getElement().getStyle().setWidth(selfWidth + widthsOfColumnsToTheRight,Unit.PX);
}
