{
  LinkedHashMap<String,Integer> options=new LinkedHashMap<String,Integer>();
  for (int i=1; i <= 10; i++) {
    options.put(String.valueOf(i),i);
  }
  options.put("20",20);
  options.put("50",50);
  options.put("100",100);
  createSelectAction("Number of root items",category,options,"3",rootItemIdsCommand);
}
