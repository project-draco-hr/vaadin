{
  String uri=callback.getRequestPathInfo(request);
  if (uri == null) {
    uri="";
  }
 else {
    while (uri.startsWith("/") && uri.length() > 0) {
      uri=uri.substring(1);
    }
  }
  try {
    throw new RuntimeException("Not ported to use roots");
  }
 catch (  final Throwable t) {
    application.getErrorHandler().terminalError(new URIHandlerErrorImpl(application,t));
    return null;
  }
}
