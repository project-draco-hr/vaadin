{
  if (errors.size() == 1) {
    (errors.iterator().next()).paint(target);
  }
 else {
    target.startTag("error");
    target.addAttribute("level",level.getText());
    for (final Iterator<ErrorMessage> i=errors.iterator(); i.hasNext(); ) {
      i.next().paint(target);
    }
    target.endTag("error");
  }
}
